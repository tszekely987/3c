<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_LOOKUPS" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of all lookups" languageCode="en" name="All lookups"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:b.id:id@,
          @SELECT:a.name:name@,
          @SELECT:a.description:description@,
          @SELECT:b.lookup_type:lookup_type@,
          @SELECT:e.name:lookup_source@,
          @SELECT:e.id:lookup_source_code@,
				  @SELECT:a.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
          @SELECT:l.ID:LANGUAGE_ID@,
          @SELECT:a.LANGUAGE_CODE:LANGUAGE_CODE@
          FROM    cmn_captions_nls a,
          cmn_lookup_types b,
          cmn_list_of_values c,
          cmn_lookups_v e,
          CMN_LANGUAGES l
          WHERE   a.table_name =  'CMN_LOOKUP_TYPES'
          AND     b.is_active = 1
          AND     @FILTER@
          AND     a.pk_id = b.id
          AND     a.language_code = @WHERE:PARAM:LANGUAGE@
          AND     b.lookup_type =        c.lookup_type_code
          AND     ( c.parent_id = 0 OR c.parent_id is null )
          AND     c.lookup_source_code = e.lookup_code
          AND     b.lookup_type not like 'ODF.OBJECT.%'
          AND     b.lookup_type not like 'ODF.NSQL.%'
          AND     b.lookup_type not like 'ODF.SYSTEM.%'
          AND     b.is_admin_visible = 1
          AND     b.is_system in ( 0, 1 )
          AND     a.LANGUAGE_CODE = l.LANGUAGE_CODE
          AND     a.LANGUAGE_CODE = e.LANGUAGE_CODE
          AND     e.language_code = @WHERE:PARAM:LANGUAGE@ and
           ( (@where:param:user_def:string:exclude_lookup_type@  IS NOT NULL
                         AND b.lookup_type != @where:param:user_def:string:exclude_lookup_type@)
                     OR (@where:param:user_def:string:exclude_lookup_type@ IS NULL
                          AND   b.lookup_type =   b.lookup_type))]]></nsql>
            <displayedSuggestionAttributes>
              <displayedSuggestionAttribute value="name"/>
              <displayedSuggestionAttribute value="lookup_type"/>
            </displayedSuggestionAttributes>
            <searchedSuggestionAttributes>
              <searchedSuggestionAttribute value="name"/>
              <searchedSuggestionAttribute value="lookup_type"/>
            </searchedSuggestionAttributes>
