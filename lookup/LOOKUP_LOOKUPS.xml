              </listColumn>
              <listColumn code="name" order="1" widgetType="text">
                
                
                
                
                <nls description="Name of the Lookup" languageCode="en" name="Lookup Name"/>
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
              </listColumn>
              <listColumn code="lookup_type" order="2" widgetType="text">
                
                
                
                
