            <nls description="User Groups Lookup" languageCode="en" name="Lookup for the groups associated with User"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:g.id:id@,
                @SELECT:g.group_name:name@,
                @SELECT:g.group_code:unique_code@
        FROM    cmn_sec_groups_v g, cmn_sec_user_groups ug
        WHERE   ug.group_id = g.id
        AND     g.language_code = @WHERE:PARAM:LANGUAGE@
        AND     g.group_role_type = 'GROUP'
        AND     g.principal_type is null
        AND     g.right_type is null
        AND     ug.user_id = @WHERE:PARAM:USER_DEF:INTEGER:USER_ID@
        AND     @FILTER@]]></nsql>
