            <nls description="Lookup Resource Class for Resource Financials" languageCode="en" name="Lookup Resource Class for Resource Financials"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT
                         @SELECT:w.resource_class:resource_class@,
                         @SELECT:w.description:description@,
						 @SELECT:w.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
                       FROM pac_fos_resource_class w
                       WHERE @FILTER@                      @BROWSE-ONLY:
                            and w.ACTIVE = 1
                            and w.RESOURCE_TYPE =  (case @WHERE:PARAM:USER_DEF:STRING:resource_type@
                            WHEN '0' THEN 'L'
                            WHEN '1' THEN 'Q'
                            WHEN '2' THEN 'M'
                            WHEN '3' THEN 'X'
                            ELSE NULL
                            END)
                            :BROWSE-ONLY@]]></nsql>
