<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="AUDIT_ENABLED_API_OBJECTS" displayAttributeName="name" hiddenAttributeName="code" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of all API Enabled Auditable Objects" languageCode="en" name="Audit Enabled API Objects"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:o.code:code@,
                                                                @SELECT:c.name:name@,
                                                                @SELECT:c.last_updated_date:last_updated_date@,
                                                                @SELECT:lang.id:language_id@,
                                                                @SELECT:lang.language_code:language_code@
            FROM     odf_objects o JOIN
                     (SELECT DISTINCT object_code FROM odf_audited_attributes WHERE attribute_code != 'odf_pk') a on a.object_code = o.code JOIN
                     cmn_captions_nls c ON c.pk_id = o.ID JOIN
                     cmn_languages lang ON lang.language_code = c.language_code LEFT JOIN
                     odf_object_capabilities ooc ON (ooc.object_code = o.code and ooc.capability_code = 'ADMIN_CONFIG_ALLOWED')
            WHERE    c.pk_id = o.ID
            AND      c.table_name = 'ODF_OBJECTS'
            AND      o.is_api_enabled = 1
            AND      o.code not in ('asset', 'service', 'product', 'application', 'other')
            AND      (o.is_customizable = 1 OR ooc.enabled = 1)
            AND      c.language_code = @WHERE:PARAM:LANGUAGE@
            AND      @FILTER@]]></nsql>
