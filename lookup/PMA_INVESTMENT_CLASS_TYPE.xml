            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:CODE:CODE@,
        @SELECT:NAME:NAME@,
        @SELECT:LANGUAGE_ID:LANGUAGE_ID@,
        @SELECT:LANGUAGE_CODE:LANGUAGE_CODE@,
        @SELECT:LAST_UPDATED_DATE:LAST_UPDATED_DATE@
        FROM
        (
          SELECT C.CODE CODE, NAME, LANG.ID LANGUAGE_ID, LANG.LANGUAGE_CODE, O.LAST_UPDATED_DATE
            FROM ODF_CLASS_V C, ODF_OBJECTS O, ODF_OBJECT_EXTENSIONS OE, CMN_LANGUAGES LANG
           WHERE C.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
             AND C.LANGUAGE_CODE = LANG.LANGUAGE_CODE
             AND O.CODE = C.OBJECT_CODE
             AND O.CODE = OE.OBJECT_CODE
             AND O.IS_CLASS_ENABLED = 1
             AND OE.EXTENSION_CODE = 'inv'
           UNION
          SELECT O.CODE, O.NAME, LANG.ID LANGUAGE_ID, LANG.LANGUAGE_CODE, O.LAST_UPDATED_DATE
            FROM ODF_OBJECTS_V O, ODF_OBJECT_EXTENSIONS OE, CMN_LANGUAGES LANG
           WHERE O.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
             AND O.LANGUAGE_CODE = LANG.LANGUAGE_CODE
             AND O.CODE = OE.OBJECT_CODE
             AND O.IS_CLASS_ENABLED = 0
             AND OE.EXTENSION_CODE = 'inv'
           UNION
          SELECT L.LOOKUP_CODE, L.NAME, LANG.ID LANGUAGE_ID, LANG.LANGUAGE_CODE, L.LAST_UPDATED_DATE
            FROM CMN_LOOKUPS_V L, CMN_LANGUAGES LANG
           WHERE L.LOOKUP_TYPE = 'PMA_INVESTMENT_TYPE'
             AND L.LOOKUP_CODE = 'all' and L.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
             AND L.LANGUAGE_CODE = LANG.LANGUAGE_CODE) Q
				WHERE 1=1
          AND @FILTER@]]></nsql>
