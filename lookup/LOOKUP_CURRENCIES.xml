		@SELECT:LANG.ID:LANGUAGE_ID@
FROM    cmn_countries b,
        cmn_captions_nls a,
        cmn_currencies c,
		CMN_LANGUAGES LANG
WHERE   a.table_name = 'CMN_COUNTRIES'
AND     @FILTER@
AND     a.pk_id = b.id
AND     a.language_code = @WHERE:PARAM:LANGUAGE@
AND     b.currency_id = c.id
AND   LANG.LANGUAGE_CODE=a.LANGUAGE_CODE
                ]]></nsql>
