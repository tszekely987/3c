          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="FIN_INVESTMENTS" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Investments for Post to WIP Job" languageCode="en" name="Financial Investments"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:INV.ID:ID@
       ,@SELECT:INV.CODE:CODE@
       ,@SELECT:INV.NAME:NAME@
       ,@SELECT:CLS.CODE:INVESTMENT_TYPE@
       ,@SELECT:CLS.NAME:INVESTMENT_TYPE_NAME@
FROM INV_INVESTMENTS INV LEFT OUTER JOIN INV_PROJECTS PRJ ON (INV.ID = PRJ.PRID),
      PAC_MNT_PROJECTS MP, DEPARTMENTS D, ODF_CLASS_V CLS, ODF_OBJECTS OBJ
WHERE @WHERE:SECURITY:INV:INV.ID@
      AND CLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
      AND (INV.PURGE_FLAG=0 OR INV.PURGE_FLAG IS NULL)
      AND INV.ID = MP.ID
      AND MP.DEPARTCODE=D.DEPARTCODE
      AND (MP.STATUS = 'O' OR EXISTS (SELECT 1 FROM PPA_TRANSCONTROL PPP WHERE  PPP.PROJECT_CODE = UPPER(INV.CODE)))
      AND @NVL@(PRJ.IS_TEMPLATE,0) = 0
      AND @NVL@(PRJ.IS_PROGRAM,0) = 0
      AND CLS.CODE = OBJ.CODE
      AND MP.ODF_OBJECT_CODE = CLS.CODE
      AND (MP.ODF_OBJECT_CODE IN ('application', 'asset', 'idea', 'other', 'product', 'project', 'service')
            OR (OBJ.TEMPLATE_EXTENSION = 'inv_extension' 
                AND OBJ.IS_CUSTOM = 1))
      AND @FILTER@
      @BROWSE-ONLY: AND INV.IS_ACTIVE = 1 :BROWSE-ONLY@]]></nsql>
