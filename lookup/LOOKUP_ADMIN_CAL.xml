            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[
                  SELECT  @SELECT:c.prid:id@,
	                    @SELECT:c.PRUID:UNIQUE_CODE@,
	                    @SELECT:c.prname:name@,
	                    @SELECT:b.prname:parentname@,
						@SELECT:c.prmodtime:LAST_UPDATED_DATE@
	            FROM    PRCalendar c
	            LEFT OUTER JOIN prcalendar b on c.prbasecalendarid = b.prid
	            WHERE   c.prresourceid IS NULL
	            AND @FILTER@
                ]]></nsql>
            <nsql databaseId="Niku" dbVendor="oracle"><![CDATA[SELECT  @SELECT:c.prid:id@,
	                    @SELECT:c.PRUID:UNIQUE_CODE@,
	                    @SELECT:c.prname:name@,
	                    @SELECT:b.prname:parentname@,
						@SELECT:c.prmodtime:LAST_UPDATED_DATE@
	            FROM    PRCalendar c,PRCalendar b
	            WHERE   c.prresourceid IS NULL
	                    AND c.prbasecalendarid=b.prid (+)
	            AND @FILTER@]]></nsql>
