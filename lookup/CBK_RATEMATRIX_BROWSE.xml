<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="CBK_RATEMATRIX_BROWSE" displayAttributeName="description" hiddenAttributeName="matrixkey" objectCode="" sortAttributeName="description" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Browse for Rate Matrices" languageCode="en" name="Browse for Rate Matrix"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:matrixkey:matrixkey@,
               @SELECT:description:description@,
			@SELECT:LAST_UPDATED_DATE:LAST_UPDATED_DATE@
        FROM ppa_matrix
        WHERE @FILTER@ AND matrixtype IN ('R', 'C')
               AND NOT EXISTS (
                 SELECT 1
                 FROM ppa_locmatrixassignments a, pac_mnt_resources r
                 WHERE a.matrixkey = ppa_matrix.matrixkey
                 AND a.locationid IS NOT NULL
                 AND @DBUSER@.pac_fos_is_valid_location_fct ((select unique_name from SRM_RESOURCES where user_id=@WHERE:PARAM:USER_ID@), a.locationid) = 0)]]></nsql>
