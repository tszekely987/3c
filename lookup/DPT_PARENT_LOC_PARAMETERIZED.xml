<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="DPT_PARENT_LOC_PARAMETERIZED" displayAttributeName="locationid" hiddenAttributeName="id" objectCode="" sortAttributeName="locationid" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Location browse" languageCode="en" name="Location browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:L.ID:ID@,
          @SELECT:L.LOCATIONID:LOCATIONID@,
          @SELECT:L.SHORTDESC:SHORTDESC@,
          @SELECT:L.LOCATIONDESCRIPTION:LOCATIONDESCRIPTION@
  FROM    LOCATIONS L
  WHERE   @FILTER@
  AND     (@WHERE:PARAM:USER_DEF:STRING:ENTITY_CONSTRAIN@ IS NULL OR
              @WHERE:PARAM:USER_DEF:STRING:ENTITY_CONSTRAIN@ =  L.ENTITY)
  AND     (@WHERE:PARAM:USER_DEF:STRING:LOCATION_ID_CONSTRAIN@ IS NULL OR
              @WHERE:PARAM:USER_DEF:STRING:LOCATION_ID_CONSTRAIN@ != L.LOCATIONID)
          @BROWSE-ONLY:
            AND
             (
                exists
                (
                 select 1 from cmn_sec_assgnd_obj_perm_r_v g, cmn_sec_objects so
                 where g.object_id = so.id
                 and so.object_code='DPT_LOCATION'
                 and g.permission_code = 'READ'
                 and g.user_id = @WHERE:PARAM:USER_ID@
                )
                or exists
                (
                 select 1 from CMN_SEC_CHK_USER_INST_V0 i, cmn_sec_objects so
                 where i.object_id = so.id
                 and so.object_code='DPT_LOCATION'
                 and i.permission_code = 'READ'
                 and i.user_id = @WHERE:PARAM:USER_ID@            and i.object_instance_id = L.id
                )
             )
          :BROWSE-ONLY@]]></nsql>
