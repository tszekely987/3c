            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[
                                    SELECT
                @SELECT:Q.PROCESS_CODE:PROCESS_CODE@,
                @SELECT:Q.PROCESS_CODE:UNIQUE_CODE@,
                @SELECT:Q.PROCESS_NAME:PROCESS_NAME@,@SELECT:Q.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
		,@SELECT:Q.ID:LANGUAGE_ID@,
		@SELECT:Q.lang:LANGUAGE_CODE@		
                FROM                   (SELECT process.lookup_code
                process_code, invtype.name + ' - ' +
                process.name process_name, process.language_code lang,l.id ID,process.LAST_UPDATED_DATE LAST_UPDATED_DATE
                FROM cmn_lookups_v process, cmn_lookups_v invtype,CMN_LANGUAGES l
                WHERE process.lookup_type = 'INV_PROCESS_TYPE'
                AND process.parent_lookup_code = invtype.lookup_code
				AND process.LANGUAGE_CODE = l.LANGUAGE_CODE
                AND process.language_code = invtype.language_code) Q
                WHERE                   LANG = @WHERE:PARAM:LANGUAGE@
                AND @FILTER@       
                ]]></nsql>
