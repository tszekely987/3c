        FROM INV_INVESTMENTS INV
        INNER JOIN ODF_CA_INV CA ON  CA.ID = INV.ID 
        INNER JOIN ODF_HIERARCHY_NODES NDS ON  NDS.INSTANCE_ID = INV.ID and NDS.object_code = CA.ODF_OBJECT_CODE
                WHERE
                 ca.ODF_OBJECT_CODE not in ('asset','application','product','service','other')
                   AND (INV.PURGE_FLAG=0 OR INV.PURGE_FLAG IS NULL)
                 
                   AND
                @FILTER@                                         
                @BROWSE-ONLY:AND INV.is_active=1:BROWSE-ONLY@
