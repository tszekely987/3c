      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="INTEGRATION_INVESTMENT_TYPES" displayAttributeName="clarity_inv_type_name" hiddenAttributeName="clarity_inv_type_code" objectCode="" sortAttributeName="clarity_inv_type_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of all Integration investment types" languageCode="en" name="Integration Investment Types"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT   @SELECT:P.code:clarity_inv_type_code@,
                @SELECT:P.name:clarity_inv_type_name@,
                @SELECT:P.last_updated_date:last_updated_date@,
                @SELECT:P.id:language_id@,
                @SELECT:P.language_code:language_code@
		FROM ( SELECT o.code AS code,
			c.NAME AS name,
			c.last_updated_date as last_updated_date,
			lang.id as id,
			lang.language_code as language_code
			FROM   odf_objects o,
			cmn_captions_nls c,
			odf_object_extensions oe,
			cmn_languages lang
			WHERE  c.pk_id = o.id
			AND c.table_name = 'ODF_OBJECTS'
			AND c.language_code = 'en'
			AND oe.extension_code = 'inv'
			AND lang.language_code = c.language_code
			AND lang.language_code = 'en'
			AND o.is_customizable = 1
			AND oe.object_code = o.code
			AND oe.object_code NOT IN ( 'asset', 'application', 'product', 'service',
							   'other' )
			UNION
			SELECT o.code as code,
			c.NAME as name,
			c.last_updated_date as last_updated_date,
			lang.id as id,
			lang.language_code as language_code
			FROM   odf_objects o,
			cmn_captions_nls c,
			cmn_languages lang
			WHERE  c.pk_id = o.id
			AND c.table_name = 'ODF_OBJECTS'
			AND c.language_code = 'en'
			AND o.template_extension is null
			AND lang.language_code = c.language_code
			AND lang.language_code = 'en'
			AND o.is_custom = 1
			AND o.is_system  = 0
			AND o.is_customizable = 1
			AND o.parent_object_code is null
			AND o.code NOT IN ( 'asset', 'application', 'product', 'service',
							   'other' )) P
                WHERE    @FILTER@]]></nsql>
