<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="INTEGRATION_INVESTMENT_TYPES" displayAttributeName="clarity_inv_type_name" hiddenAttributeName="clarity_inv_type_code" objectCode="" sortAttributeName="clarity_inv_type_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of all Integration investment types" languageCode="en" name="Integration Investment Types"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  
            @SELECT:P.code:clarity_inv_type_code@,
            @SELECT:P.name:clarity_inv_type_name@
            FROM (
                SELECT objs.code AS code, c.NAME AS name 
                FROM   ( 
                    /* Objects having INCLUDE_IN_INTEGRATIONS as capability */ 
                    SELECT o.id, o.code AS code 
                    FROM   odf_objects o 
                        INNER JOIN odf_object_capabilities cap 
                                ON cap.object_code = o.code 
                    WHERE  cap.capability_code = 'INCLUDE_IN_INTEGRATIONS' 
                        AND cap.enabled = 1 
                    UNION 
                    /* Custom Master Objects */ 
                    SELECT o.id, o.code AS code 
                    FROM   odf_objects o 
                    WHERE  o.is_customizable = 1 
                        AND o.template_extension IS NULL 
                        AND o.is_custom = 1 
                        AND o.is_system = 0 
                        AND o.parent_object_code IS NULL 
                    UNION 
                    /* Project, Idea and Custom Investment Types */
                    SELECT o.id, o.code AS code 
                    FROM   odf_objects o 
                            INNER JOIN odf_object_extensions oe 
                                    ON oe.object_code = o.code 
                    WHERE  o.is_customizable = 1 
                            AND oe.extension_code = 'inv' 
                            AND o.code NOT IN ( 'asset', 'application', 'product', 'service', 'other' )
                ) objs
                INNER JOIN cmn_captions_nls c 
                    ON c.table_name = 'ODF_OBJECTS' 
                        AND c.pk_id = objs.id 
                        AND c.language_code = @WHERE:PARAM:LANGUAGE@
            ) P
            WHERE    @FILTER@]]></nsql>
