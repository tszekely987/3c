          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUPS_STATIC_LIST" displayAttributeName="name" hiddenAttributeName="lookup_type" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of static lookups available." languageCode="en" name="Active static lookups"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:a.name:name@,
        @SELECT:a.description:description@,
        @SELECT:b.lookup_type:lookup_type@,
        @SELECT:d.object_internal_name:internal_name@,
        @SELECT:e.name:lookup_source@,
        @SELECT:c.lookup_source_code:lookup_source_code@,
		@SELECT:e.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
 		@SELECT:l.ID:LANGUAGE_ID@,
		@SELECT:e.LANGUAGE_CODE:LANGUAGE_CODE@		
FROM    cmn_captions_nls a,             
        cmn_lookup_types b,
        cmn_list_of_values c,             
        cmn_lov_objects d,
        cmn_lookups_v e,
		CMN_LANGUAGES l		
WHERE   a.table_name = 'CMN_LOOKUP_TYPES'    
AND     @FILTER@    
AND     a.pk_id = b.id    
AND     b.is_active = 1
AND     a.language_code = @WHERE:PARAM:LANGUAGE@    
AND     b.lookup_type = c.lookup_type_code    
AND     c.lookup_type_code = d.lookup_type_code    
AND     c.lookup_source_code = e.lookup_code    
AND     c.lookup_source_code = 'LOOKUP_SOURCE_STATIC'
AND     a.LANGUAGE_CODE = e.LANGUAGE_CODE
AND     e.LANGUAGE_CODE = l.LANGUAGE_CODE	  		    
AND     e.language_code = @WHERE:PARAM:LANGUAGE@]]></nsql>
