<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_ODF_OBJECT_MAPPING" displayAttributeName="mapping_code" hiddenAttributeName="mapping_code" objectCode="" sortAttributeName="mapping_code" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List mappings available for a source object type" languageCode="en" name="Conversion Mappings"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:om.id:object_mapping_id@,
	             @SELECT:om.mapping_code:mapping_code@,
	             @select:nls.name:name@,
	             @select:nls.description:description@,
	             @SELECT:om.src_object_code:src_object_code@,
	             @select:nls1.name:src_object_name@,
	             @SELECT:om.dest_object_code:dest_object_code@,
	             @select:nls2.name:dest_object_name@,@SELECT:om.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
               @SELECT:l.ID:LANGUAGE_ID@,
               @SELECT:nls1.LANGUAGE_CODE:LANGUAGE_CODE@
               FROM   odf_object_mapping om, cmn_captions_nls nls, cmn_captions_nls nls1, cmn_captions_nls nls2, odf_objects o1, odf_objects o2,CMN_LANGUAGES l
               WHERE om.parent_mapping_code is null and
               upper(om.src_object_code) =  upper(@where:param:xml:string:/data/object_type_code/@value@)
                and    nls.language_code = @where:param:language@
                and    nls.table_name = 'ODF_OBJECT_MAPPING'
                and    nls.pk_id  = om.id
                and    nls1.language_code = @where:param:language@
                AND    nls1.LANGUAGE_CODE = l.LANGUAGE_CODE
                and    nls1.table_name = 'ODF_OBJECTS'
                and    nls1.pk_id  = o1.id
                and    upper(o1.code) = upper(om.src_object_code)
                and    nls2.language_code = @where:param:language@
                and    nls2.table_name = 'ODF_OBJECTS'
                and    nls2.pk_id  = o2.id
                and    upper(o2.code) = upper(om.dest_object_code)
                AND  @FILTER@]]></nsql>
