      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="BROWSE_ACTIVE_INVESTMENTS" displayAttributeName="code" hiddenAttributeName="id" objectCode="" sortAttributeName="code" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Displays active investments that the user has view rights on." languageCode="en" name="Browse Active Investments"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT
       @SELECT:p.id:id@,
       @SELECT:p.code:code@,
       @SELECT:p.name:name@,
       @SELECT:cainv.odf_object_code:object@,
       @SELECT:p.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
       FROM inv_investments p, odf_ca_inv cainv 
       WHERE @FILTER@
         AND   p.id = cainv.id
         AND p.is_custom = 0
         @BROWSE-ONLY:
           AND p.is_active = 1
           AND @NVL@(p.PURGE_FLAG, 0) = 0
           AND @WHERE:SECURITY:INV:P.ID@
         :BROWSE-ONLY@]]></nsql>
