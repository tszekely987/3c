            <nsql databaseId="Niku" dbVendor="all"><![CDATA[select @SELECT:pr.prid:ID@,@SELECT:pr.prname:PRNAME@,
		@SELECT:pr.prexternalid:PREXTERNALID@, @SELECT:pr.LAST_UPDATED_DATE:LAST_UPDATED_DATE@ from prtask pr,
                pac_mnt_projects inv
                WHERE @FILTER@
                AND pr.PRPROJECTID=inv.ID
                AND     (@WHERE:PARAM:USER_DEF:INTEGER:INVESTMENT_ID_CONSTRAIN@ IS NULL OR
                @WHERE:PARAM:USER_DEF:INTEGER:INVESTMENT_ID_CONSTRAIN@ = pr.PRPROJECTID)
                @BROWSE-ONLY:
				AND @WHERE:SECURITY:INV:INV.ID@
                AND inv.status='O'
			          AND NOT EXISTS (
                           SELECT 1 from prtask a 
                           WHERE a.PRPROJECTID = pr.PRPROJECTID                         
                           AND a.WBS_PARSEQ = pr.PRWBSSEQUENCE )
  :BROWSE-ONLY@]]></nsql>
