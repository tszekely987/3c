      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="AGL_PRODUCT_BROWSE" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of products synchronized with Agile" languageCode="en" name="Product"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT @SELECT:i.id:id@,
       @SELECT:i.code:code@,
       @SELECT:i.name:name@,
       @SELECT:i.description:description@
FROM   inv_investments i
       INNER JOIN inv_flat_hierarchies h ON h.parent_id = i.id
       INNER JOIN odf_ca_project p ON h.child_id = p.id
WHERE  i.odf_object_code = 'product'
AND    p.agl_id IS NOT NULL
AND    (p.agl_integration_type IS NOT NULL OR p.agl_integration_type <> 'investment')
AND    @FILTER@ 
@BROWSE-ONLY: AND  @WHERE:SECURITY:INV:i.id@:BROWSE-ONLY@]]></nsql>
