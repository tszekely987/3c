          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="SCH_BROWSE_GL_ACCTS" displayAttributeName="display_name" hiddenAttributeName="id" objectCode="" sortAttributeName="main_account_id" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Browse for GL Accounts" languageCode="en" name="Browse for GL Accounts"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[
                  
        SELECT  @SELECT:D.ID:ID@,
            @SELECT:D.MAIN_ACCOUNT_ID:MAIN_ACCOUNT_ID@,
            @SELECT:D.SUB_ACCOUNT_ID:SUB_ACCOUNT_ID@,
            @SELECT:D.MAIN_ACCOUNT_ID + '-' + D.SUB_ACCOUNT_ID:DISPLAY_NAME@,
            @SELECT:D.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
        FROM    CBK_GL_ACCOUNT D
        WHERE @FILTER@
          AND  (@WHERE:PARAM:USER_DEF:INTEGER:OVERHEAD_CONSTRAIN@ IS NULL OR
            @WHERE:PARAM:USER_DEF:INTEGER:OVERHEAD_CONSTRAIN@ = D.OVERHEAD)
          AND (@WHERE:PARAM:USER_DEF:INTEGER:ENTITYID_CONSTRAIN@ IS NULL OR
             @WHERE:PARAM:USER_DEF:INTEGER:ENTITYID_CONSTRAIN@ = D.ENTITY_ID OR
             D.ENTITY_ID IS NULL)
        @BROWSE-ONLY:
        AND  D.IS_ACTIVE = 1
        :BROWSE-ONLY@
                ]]></nsql>
            <nsql databaseId="Niku" dbVendor="oracle"><![CDATA[SELECT  @SELECT:D.ID:ID@,
            @SELECT:D.MAIN_ACCOUNT_ID:MAIN_ACCOUNT_ID@,
            @SELECT:D.SUB_ACCOUNT_ID:SUB_ACCOUNT_ID@,
            @SELECT:D.MAIN_ACCOUNT_ID || '-' || D.SUB_ACCOUNT_ID:DISPLAY_NAME@,
            @SELECT:D.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
        FROM    CBK_GL_ACCOUNT D      
        WHERE @FILTER@
        AND  (@WHERE:PARAM:USER_DEF:INTEGER:OVERHEAD_CONSTRAIN@ IS NULL OR 
            @WHERE:PARAM:USER_DEF:INTEGER:OVERHEAD_CONSTRAIN@ = D.OVERHEAD)
        AND (@WHERE:PARAM:USER_DEF:INTEGER:ENTITYID_CONSTRAIN@ IS NULL OR
             @WHERE:PARAM:USER_DEF:INTEGER:ENTITYID_CONSTRAIN@ = D.ENTITY_ID OR
             D.ENTITY_ID IS NULL)
        @BROWSE-ONLY:
        AND  D.IS_ACTIVE = 1
        :BROWSE-ONLY@]]></nsql>
            <nsql databaseId="Niku" dbVendor="postgres"><![CDATA[SELECT  @SELECT:D.ID:ID@,
