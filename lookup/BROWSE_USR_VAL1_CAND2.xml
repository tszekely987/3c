      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="BROWSE_USR_VAL1_CAND2" displayAttributeName="user_value1_lookup_name" hiddenAttributeName="user_value1_lookup" objectCode="" sortAttributeName="user_value1_lookup_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Browse for candidates to provide data for User Value 1 field." languageCode="en" name="User Value Lookup Candidates"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT
              @SELECT:LOOKUP_TYPE_CODE:user_value1_lookup@,
              @SELECT:NAME:user_value1_lookup_name@,
              @SELECT:LOOKUP_CANDIDATES.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
              @SELECT:l.ID:LANGUAGE_ID@,
              @SELECT:l.LANGUAGE_CODE:LANGUAGE_CODE@

              FROM
              (
                SELECT LOOKUP_TYPE_CODE, LAST_UPDATED_DATE FROM CMN_LIST_OF_VALUES WHERE ID IN
                (
                  SELECT PK_ID FROM CMN_ATTRIBUTES
                  where COLUMN_NAME = 'userdeftsid_param' OR COLUMN_NAME = 'param_userdeftsid_param'
                )
                AND ID IN
                (
                  SELECT PK_ID FROM CMN_ATTRIBUTES
                  WHERE COLUMN_NAME = 'prtimeentryid'
                )
                AND ID IN
                (
                  SELECT PK_ID FROM CMN_ATTRIBUTES
                  WHERE COLUMN_NAME = 'uservalue'
                )UNION
                SELECT LOOKUP_TYPE_CODE , LAST_UPDATED_DATE FROM cmn_list_of_values
                where LOOKUP_TYPE_CODE = 'PRTIMEENTRY_USER_LOV1'
              ) LOOKUP_CANDIDATES, CMN_LOOKUP_TYPES_V, CMN_LANGUAGES l
              WHERE LOOKUP_TYPE_CODE = LOOKUP_TYPE
              AND CMN_LOOKUP_TYPES_V.LANGUAGE_CODE = l.LANGUAGE_CODE
              AND CMN_LOOKUP_TYPES_V.LANGUAGE_CODE=@WHERE:PARAM:LANGUAGE@
              AND @FILTER@]]></nsql>
