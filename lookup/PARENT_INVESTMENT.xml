      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="PARENT_INVESTMENT" displayAttributeName="inv_name" hiddenAttributeName="id" objectCode="" sortAttributeName="inv_type" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Lookup for Parent Investment" languageCode="en" name="Parent Investment"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:inv.id:id@, @SELECT:inv.name:inv_name@, @SELECT:inv.code:inv_code@
        , @SELECT:cap.name:inv_type@, @SELECT:inv.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
FROM inv_investments inv
     join odf_objects obj on obj.code = inv.odf_object_code
     join odf_object_capabilities ooc on inv.odf_object_code = ooc.object_code and ooc.enabled = 1 and ooc.capability_code = 'HIERARCHY_ENABLED'
     join cmn_captions_nls cap on cap.table_name = 'ODF_OBJECTS' and cap.language_code = @WHERE:PARAM:LANGUAGE@ and cap.pk_id = obj.ID
     left outer join inv_projects ip on ip.prid = inv.id
WHERE @FILTER@
   AND ((obj.template_extension is not null) or inv.odf_object_code in ('project', 'idea'))
   AND inv.is_active = 1
   AND @NVL@(inv.odf_template, 0) != 1
   AND @NVL@(ip.is_program, 0) != 1
   AND (inv.id != @WHERE:PARAM:USER_DEF:INTEGER:INV_ID@ OR @WHERE:PARAM:USER_DEF:INTEGER:INV_ID@ is null)
   @BROWSE-ONLY:
   AND exists (select 1 from (select object_instance_id from odfsec_inv_v2 where user_id = @WHERE:PARAM:USER_ID@) v2 where v2.object_instance_id = inv.ID)
   :BROWSE-ONLY@]]></nsql>
