            <nls description="Process Action Item Status" languageCode="en" name="Process Action Item Status"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[select
 @SELECT:aiStatus.CODE:CODE@,
@SELECT:aiStatus.name:name@,
@SELECT:aiStatus.lookup_code:lookup_code@,
@SELECT:aiStatus.lookup_type:lookup_type@,
@SELECT:aiStatus.sort_order:sort_order@
 from (select id code, name, lookup_code, lookup_type, 0 sort_order from cmn_lookups_v where is_active = 1 and language_code = @WHERE:PARAM:LANGUAGE@ and lookup_type = 'CAL_ACTIONITEM_STATUS'  and lookup_code='CAL_OPEN'
 union
 select clv.id code, clv.name, clv.lookup_code lookup_code, clv.lookup_type lookup_type, clv.sort_order from cmn_lookups_v clv
    INNER JOIN 	(select a.status_type_code stype, a.status_code code from BPM_RUN_STEP_ACTION_RESULTS ar, BPM_DEF_STEP_AI_ACTIONS a, 	CAL_ACTION_ITEMS cai where ar.id = cai.process_handler_id and ( @WHERE:PARAM:USER_DEF:INTEGER:CAL_ACTION_ITEM_ID@ IS NULL or @WHERE:PARAM:USER_DEF:INTEGER:CAL_ACTION_ITEM_ID@ = cai.id) and ar.step_action_id = a.step_action_id) selected_status
        ON (clv.lookup_type = selected_status.stype AND  clv.lookup_code = selected_status.code) where clv.is_active = 1 and clv.language_code = @WHERE:PARAM:LANGUAGE@
        union
  select clv.id code, clv.name, clv.lookup_code, clv.lookup_type, clv.sort_order from cal_action_items cai
 join odf_multi_valued_lookups mvl on cai.ai_def_id = mvl.pk_id and mvl.object = 'actionitemdef' and mvl.attribute = 'status_list'
 join cmn_lookups_v clv on clv.lookup_code = mvl.value and clv.lookup_type = 'PROCESS_DEFAULT_AI_STATUS'
 where clv.is_active = 1 and cai.ai_def_type = 'actionitemdef' and clv.language_code = @WHERE:PARAM:LANGUAGE@ and @WHERE:PARAM:USER_DEF:INTEGER:CAL_ACTION_ITEM_ID@ = cai.id
        ) aiStatus]]></nsql>
