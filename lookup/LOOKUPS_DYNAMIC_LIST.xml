<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUPS_DYNAMIC_LIST" displayAttributeName="name" hiddenAttributeName="lookup_type" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of dynamic lookups (browse pages) available." languageCode="en" name="Active dynamic lookups"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:a.name:name@,
        @SELECT:a.description:description@,
        @SELECT:b.lookup_type:lookup_type@,
        @SELECT:e.name:lookup_source@,
        @SELECT:c.lookup_source_code:lookup_source_code@,
		@SELECT:LANG.ID:LANGUAGE_ID@,
		@SELECT:LANG.LANGUAGE_CODE:LANGUAGE_CODE@,
		@SELECT:e.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
FROM    cmn_captions_nls a,
        cmn_lookup_types b,
        cmn_list_of_values c,
        cmn_lookups_v e,
		CMN_LANGUAGES LANG
WHERE   a.table_name =  'CMN_LOOKUP_TYPES'      
AND     b.is_active = 1               
AND     @FILTER@                     
AND     a.pk_id = b.id
AND     a.language_code = @WHERE:PARAM:LANGUAGE@
AND     b.lookup_type =            c.lookup_type_code
AND     ( c.parent_id = 0 OR c.parent_id is null )
AND     c.lookup_source_code =   e.lookup_code
AND     c.lookup_source_code = 'LOOKUP_SOURCE_DYNAMIC'
AND   a.LANGUAGE_CODE = e.LANGUAGE_CODE
AND 	e.LANGUAGE_CODE = LANG.LANGUAGE_CODE
AND     e.language_code = @WHERE:PARAM:LANGUAGE@]]></nsql>
            <displayedSuggestionAttributes>
              <displayedSuggestionAttribute value="name"/>
              <displayedSuggestionAttribute value="lookup_type"/>
              <displayedSuggestionAttribute value="description"/>
            </displayedSuggestionAttributes>
            <searchedSuggestionAttributes>
