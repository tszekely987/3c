<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="BROWSE_WBS_PARENT_TASKS" displayAttributeName="prname" hiddenAttributeName="prid" objectCode="" sortAttributeName="prname" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List tasks for a project hierarchy" languageCode="en" name="Browse Tasks for a Project Hierarchy"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:q.prid:prid@,
         @SELECT:q.PRUID:UNIQUE_CODE@,
         @SELECT:q.prexternalid:prexternalid@,
         @SELECT:q.prname:prname@,
         @SELECT:q.investment_name:investment_name@,
         @SELECT:q.subproject:subproject@,
		 @SELECT:q.last_updated_date:last_updated_date@
         FROM (
  SELECT task.prid, task.PRUID, task.prexternalid, task.prname, inv.name investment_name, task.last_updated_date
         , ( CASE WHEN task.prprojectid = @where:param:xml:integer:/data/id/@value@ THEN 0 ELSE 1 END ) subproject          
  FROM   prtask task, inv_investments inv
  WHERE  task.prprojectid in
          (
           select id
             from   inv_investments
           where  id = @where:param:xml:integer:/data/id/@value@
            union
           select child_id
             from inv_flat_hierarchies
            where parent_id = @where:param:xml:integer:/data/id/@value@
            )
  AND    task.pristask = 0
  AND    inv.id = task.prprojectid
  @BROWSE-ONLY: AND @WHERE:SECURITY:INV:INV.ID@:BROWSE-ONLY@ 
        ) q
          WHERE  1=1 AND @FILTER@]]></nsql>
