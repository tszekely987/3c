      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_INVS_COSTPLAN_CAPEX" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Investment browse shows existing investments that have been already enabled for Capitalization but their plan of record and budget is not yet processed to use Cost Type." languageCode="en" name="Investments Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT 
                  @SELECT:INV.INVESTMENT_ID:ID@,
                  @SELECT:INV.INVESTMENT_CODE:CODE@,
                  @SELECT:INV.INVESTMENT_CODE:UNIQUE_CODE@,             @SELECT:INV.INVESTMENT_NAME:NAME@,
                  @SELECT:CC.PRID:chargeCodeID@,
                  @SELECT:CC.PREXTERNALID:chargeCode@
                FROM  INV_INVESTMENTS I
                  JOIN TEMP_CAPEX_ENABLED_INVESTMENTS INV ON I.ID = INV.INVESTMENT_ID
                  LEFT OUTER JOIN PRCHARGECODE CC ON I.CHARGECODEID=CC.PRID
                WHERE INV.STATUS = 'S'
                   AND (INV.CP_MIGRATION_STATUS != 'S' OR INV.BP_MIGRATION_STATUS != 'S')
                   AND @FILTER@ @BROWSE-ONLY: AND @WHERE:SECURITY:INV:I.ID@:BROWSE-ONLY@]]></nsql>
