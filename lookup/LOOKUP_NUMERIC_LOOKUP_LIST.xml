      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_NUMERIC_LOOKUP_LIST" displayAttributeName="name" hiddenAttributeName="lookup_type" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of Active Numeric Lookups" languageCode="en" name="Active Numeric Lookups"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[select  @select:a.name:name@,
	            @select:a.description:description@,
	            @select:b.lookup_type:lookup_type@,
	            @select:e.name:source_code_caption@,
	            @select:key_attributes.column_name:key_column@,
				@SELECT:e.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
				@SELECT:l.ID:LANGUAGE_ID@,
				@SELECT:e.LANGUAGE_CODE:LANGUAGE_CODE@
	    from    cmn_captions_nls a,
	            cmn_lookup_types b,
	            cmn_lov_objects c,
	            cmn_list_of_values d,
	            cmn_lookups_v e,
	            cmn_attributes key_attributes,
				CMN_LANGUAGES l
	    where   a.table_name =  'CMN_LOOKUP_TYPES'
	    and     b.is_active = 1
	    and     a.pk_id = b.id
	    and     a.language_code = @where:param:language@
	    and     b.lookup_type = d.lookup_type_code
	    and     b.lookup_type = c.lookup_type_code
	    and     (d.parent_id = 0 OR d.parent_id is null)
	    and     b.lookup_type not like 'ODF.OBJECT.%'
	    and     b.lookup_type not like 'ODF.NSQL.%'
	    and     b.lookup_type not like 'ODF.SYSTEM.%'
	    and     c.ui_type = 'LOV_SELECT'
	    and     d.lookup_source_code = e.lookup_code
	    and     e.language_code = @where:param:language@
	    AND     a.LANGUAGE_CODE = e.LANGUAGE_CODE
		  AND     e.LANGUAGE_CODE = l.LANGUAGE_CODE
	    and     e.lookup_type = 'CMN_LOOKUPS_SOURCE'
	    and     key_attributes.id = c.key_attribute_id
	    and     key_attributes.data_type = 'ATTR_DATA_TYPE_NUMBER'
	    and     d.lookup_source_code != 'LOOKUP_SOURCE_DEPENDENT'
	    and     @filter@]]></nsql>
