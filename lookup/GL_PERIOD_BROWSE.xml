          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="GL_PERIOD_BROWSE" displayAttributeName="pname" hiddenAttributeName="pid" objectCode="" sortAttributeName="pname" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of GL Periods" languageCode="en" name="GL Period Browse."/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:p.ID:pid@,
           @SELECT:NLS.name:pname@,
           @SELECT:l.NAME:lname@, 
           @SELECT:p.start_date:stdate@, 
           @SELECT:p.end_date:edate@
           , @SELECT:p.entity_id:entity_id@
           , @SELECT:e.entity:entity@
           , @SELECT:LANG.ID:LANGUAGE_ID@
           , @SELECT:LANG.LANGUAGE_CODE:LANGUAGE_CODE@
           , @SELECT:P.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
      FROM cmn_lookups_v l, CMN_CAPTIONS_NLS NLS, cmn_languages lang
                    , biz_com_periods p
                         LEFT OUTER JOIN entity E
                            ON p.ENTITY_ID = E.ID
     WHERE l.language_code = @WHERE:PARAM:LANGUAGE@
	   AND l.language_code = lang.language_code
       AND l.lookup_type = 'PERIOD_TYPE'
       AND l.lookup_code = p.period_type
       AND p.ID = NLS.PK_ID AND NLS.TABLE_NAME='BIZ_COM_PERIODS' AND NLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
       AND @FILTER@]]></nsql>
