<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_PARAM_RES_LOC_CODE" displayAttributeName="shortdesc" hiddenAttributeName="locationid" objectCode="" sortAttributeName="locationid" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Location browse for resource financials" languageCode="en" name="Location browse for resource financials"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT
		       @SELECT:locations.ID:ID@,
		       @SELECT:locations.locationid:LOCATIONID@,
		       @SELECT:locations.locationid:unique_code@,
		       @SELECT:locations.shortdesc:SHORTDESC@,
		       @SELECT:locations.entity:ENTITY@,
               @SELECT:locations.last_updated_date:LAST_UPDATED_DATE@
           FROM locations, locationdept
             WHERE @FILTER@
             @BROWSE-ONLY:
			 AND @WHERE:SECURITY:LOCATION:LOCATIONS.ID@
               and exists (select 1 from  srm_resources r
                         where @WHERE:PARAM:USER_ID@ = r.user_ID
                         AND r.IS_HIDDEN = 0
                         and @DBUSER@.PAC_FOS_IS_VALID_LOCATION_FCT(r.unique_name, locations.locationid) = 1)
             :BROWSE-ONLY@
             AND locations.locationid = locationdept.locationid
             AND        (
                locationdept.departcode = (@WHERE:PARAM:USER_DEF:STRING:DEPARTMENT_CODE_CONSTRAIN@)
                OR
                (@WHERE:PARAM:USER_DEF:STRING:DEPARTMENT_CODE_CONSTRAIN@) is NULL
             )]]></nsql>
