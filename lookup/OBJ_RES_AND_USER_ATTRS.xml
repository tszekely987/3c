<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="OBJ_RES_AND_USER_ATTRS" displayAttributeName="name" hiddenAttributeName="internal_name" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Resource and User Attributes for Object" languageCode="en" name="Resource and User Attributes for Object"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT
                @SELECT:internal_name:internal_name@,
                @SELECT:name:name@
                from (
                    select oca.internal_name, ccn.name
                        from odf_custom_attributes oca
                        join cmn_list_of_values lov on oca.lookup_type = lov.lookup_type_code
                        join cmn_captions_nls ccn on oca.id = ccn.pk_id
                        WHERE lov.object_code in ('resource', 'nmc_user') and data_type = 'number' and ccn.table_name = 'ODF_CUSTOM_ATTRIBUTES' and ccn.language_code = @WHERE:PARAM:LANGUAGE@
                        and object_name in
                            (
                            select code from odf_objects where code = @WHERE:PARAM:USER_DEF:string:OBJECT_CODE@
                                union
                            select
                                case when template_extension = 'inv_extension' then 'inv'
                                when ooe.extension_code = 'inv' then 'inv'
                                else null
                                end
                            from odf_objects oo
                            join odf_object_extensions ooe on oo.code = ooe.object_code where code = @WHERE:PARAM:USER_DEF:string:OBJECT_CODE@
                            )
                    union
                    select oam.attribute_code internal_name, ccn.name from odf_attr_metadata oam
                        join cmn_list_of_values lov on oam.lookup_type = lov.lookup_type_code and oam.is_custom = 0
                        join cmn_captions_nls ccn on oam.id = ccn.pk_id
                        WHERE
                        lov.object_code in ('resource', 'nmc_user') and ccn.table_name = 'ODF_ATTR_METADATA' and ccn.language_code = @WHERE:PARAM:LANGUAGE@
                        and oam.object_code = @WHERE:PARAM:USER_DEF:string:OBJECT_CODE@
                ) attr_list WHERE @FILTER@]]></nsql>
