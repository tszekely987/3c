          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="OBJ_PROJECT_TEMPLATE" displayAttributeName="investment_name" hiddenAttributeName="investment_int_id" objectCode="" sortAttributeName="investment_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="This lookup displays all project templates and is used to link projects to templates for methodology portlets." languageCode="en" name="PMO Project Templates"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @select:i.id:investment_int_id@,
                @select:i.code:investment_code@,
                @select:i.code:UNIQUE_CODE@,
@select:i.name:investment_name@
FROM   inv_investments i, inv_projects p
WHERE  i.id = p.prid
AND    p.is_template = 1
AND   (i.purge_flag = 0 OR i.purge_flag IS NULL)
AND    @WHERE:SECURITY:PROJECT:p.prid@
AND    @FILTER@
@BROWSE-ONLY:AND i.is_active = 1:BROWSE-ONLY@]]></nsql>
