            <nls description="Processes attached to a project template" languageCode="en" name="BPM Project Template Processes"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT
 @SELECT:inv.id:project_id@,
 @SELECT:inv.name:project_name@,
 @SELECT:inv.code:project_code@,
 @SELECT:inv.code:UNIQUE_CODE@,
 @SELECT:inv.description:project_description@,
 @SELECT:p.id:process_id@,
 @SELECT:v.id:process_version_id@,
 @SELECT:p.process_code:process_code@,
 @SELECT:p.name:process_name@,
 @SELECT:p.description:process_description@,@SELECT:p.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
 		@SELECT:l.ID:LANGUAGE_ID@,
		@SELECT:p.LANGUAGE_CODE:LANGUAGE_CODE@
FROM
 bpm_def_processes_v p,
 bpm_def_process_versions v,
 bpm_def_objects o,
 inv_investments inv,
 inv_projects prj,CMN_LANGUAGES l
WHERE
 p.id = v.process_id AND
 p.language_code = @WHERE:PARAM:LANGUAGE@ AND
 o.table_name = 'BPM_DEF_PROCESS_VERSIONS' AND
 o.pk_id = v.id AND
 o.object_type_code = 'project' AND
 o.object_id = inv.id AND
 prj.prid = inv.id AND
 prj.is_template = 1 AND
 p.LANGUAGE_CODE = l.LANGUAGE_CODE AND     
 @FILTER@]]></nsql>
