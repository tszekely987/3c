        @SELECT:page.space_code:space_code@,
        @SELECT:c.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
        @SELECT:l.ID:LANGUAGE_ID@,
        @SELECT:c.LANGUAGE_CODE:LANGUAGE_CODE@
        FROM    cmn_pages page,
        cmn_captions_nls c ,CMN_LANGUAGES l
        WHERE   @FILTER@
        AND     page.principal_type = 'SYSTEM'
        AND     page.instance_type = 'PFM_PLAN'
        AND     (page.page_type_code = 'page' or page.page_type_code = 'tabPage')
        AND     c.pk_id = page.id
        AND     c.language_code = @WHERE:PARAM:LANGUAGE@
        AND     c.LANGUAGE_CODE = l.LANGUAGE_CODE
        AND     c.table_name = 'CMN_PAGES']]></nsql>
