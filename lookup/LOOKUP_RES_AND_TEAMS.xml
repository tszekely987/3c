          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_RES_AND_TEAMS" displayAttributeName="full_name" hiddenAttributeName="id" objectCode="resource" sortAttributeName="last_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Resources and Teams Browse" languageCode="en" name="Resources and Teams Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:RESOURCES.ID:ID@,
                @SELECT:RESOURCES.LAST_NAME:LAST_NAME@,
                @SELECT:RESOURCES.FIRST_NAME:FIRST_NAME@,
                @SELECT:RESOURCES.FULL_NAME:FULL_NAME@,
                @SELECT:RESOURCES.UNIQUE_NAME:UNIQUE_NAME@,
                @SELECT:RESOURCES.UNIQUE_NAME:UNIQUE_CODE@,
                @SELECT:RESOURCES.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
                FROM (
                      SELECT ID,
                        LAST_NAME,
                        FIRST_NAME,
                        FULL_NAME,
                        UNIQUE_NAME,
                        UNIQUE_NAME UNIQUE_CODE,
                        LAST_UPDATED_DATE,
                        USER_ID,
                        IS_HIDDEN,
                        IS_ACTIVE
                        FROM SRM_RESOURCES
                        WHERE PERSON_TYPE != 0
                      UNION
                        SELECT ID,
                        LAST_NAME,
                        FIRST_NAME,
                        FULL_NAME,
                        UNIQUE_NAME,
                        UNIQUE_NAME UNIQUE_CODE,
                        LAST_UPDATED_DATE,
                        USER_ID,
                        IS_HIDDEN,
                        IS_ACTIVE
                        FROM SRM_RESOURCES
                        WHERE DEFINED_TEAM_ID IS NOT NULL
                 ) RESOURCES
                WHERE @WHERE:SECURITY:RESOURCE:RESOURCES.ID@               
                AND @FILTER@
                @BROWSE-ONLY:
                 AND     RESOURCES.IS_HIDDEN = 0
                 AND     RESOURCES.IS_ACTIVE = 1
               :BROWSE-ONLY@]]></nsql>
