          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="CSK_RPT_PORTFOLIO_PLAN" displayAttributeName="plan_name" hiddenAttributeName="plan_key" objectCode="" sortAttributeName="portfolio_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Report (Portfolio Plans)" languageCode="en" name="Report (Portfolio Plans)"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:pfm.id:plan_key@,
       @SELECT:pfm.code:plan_id@,
       @SELECT:pfm.name:plan_name@,
       @SELECT:p.name:portfolio_name@	   
FROM   pfm_plans pfm
       INNER JOIN pfm_portfolios p ON  pfm.portfolio_id = p.id
WHERE  1=1
AND   (EXISTS (SELECT user_id
               FROM   cmn_sec_assgnd_obj_perm_r_v
               WHERE  object_type = 'RECORD'
               AND    object_code = 'PFM_PORTFOLIO'
               AND    permission_code = 'READ'
               AND    component_code = 'PFM'
               AND    user_id = @WHERE:PARAM:USER_ID@
               UNION
               SELECT object_instance_id
               FROM   cmn_sec_assgnd_obj_perm_v
               WHERE  object_instance_id = p.id
               AND    object_type = 'RECORD'
               AND    object_code = 'PFM_PORTFOLIO'
               AND    permission_code = 'READ'
               AND    component_code = 'PFM'
               AND    user_id = @WHERE:PARAM:USER_ID@))
AND    @FILTER@]]></nsql>
