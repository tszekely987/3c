            <nls description="List resources who are participants of a project" languageCode="en" name="DMS Owners Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:r.user_id:user_id@,    @SELECT:r.id:resource_id@,
                @SELECT:r.unique_name:unique_name@,
                @SELECT:r.unique_name:UNIQUE_CODE@,
                @SELECT:r.first_name:first_name@,
                @SELECT:r.last_name:last_name@,
                @SELECT:r.full_name:full_name@,
				@SELECT:r.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
                FROM
                srm_resources r,
                cmn_sec_assgnd_obj_perm  a
                WHERE
                r.user_id = a.principal_id
                AND r.IS_HIDDEN = 0
                AND a.object_id = 200
                AND a.principal_type = 'USER'
                AND a.permission_id IN ( 5, 4, 1 )
                AND a.object_instance_id = @where:param:user_def:integer:folder_id@
                AND @FILTER@]]></nsql>
