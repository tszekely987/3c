            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[SELECT  @SELECT:p.id:id@,
                      @SELECT:p.vendor_code:code@,
                      @SELECT:p.address_name:name@,
                      @SELECT:p.status_type:status@,
                      @SELECT:l.name:statusname@,
					  @SELECT:p.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
					            @SELECT:lang.language_code:language_code@,
					            @SELECT:lang.id:language_id@
                      FROM apmaster p, cmn_lookups_v l, cmn_languages lang
                      Where p.status_type = l.lookup_code
                      AND @FILTER@
                      AND     l.language_code=@WHERE:PARAM:LANGUAGE@
                      AND     l.language_code=lang.language_code
                      AND     l.lookup_type='LOOKUP_FIN_VENDOR_STATUS'
                      @BROWSE-ONLY:
                        AND     l.lookup_code IN ('1','2','3')
                      :BROWSE-ONLY@]]></nsql>
