               WHERE  object_instance_id = pfm.id
               AND    object_type = 'RECORD'
               AND    object_code = 'PFM_PORTFOLIO'
               AND    permission_code = 'READ'
               AND    component_code = 'PFM'
               AND    user_id = @WHERE:PARAM:USER_ID@))
AND    @FILTER@]]></nsql>
