      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="COP_PORTLET_TIMESHEET_PERIOD" displayAttributeName="timeperiod" hiddenAttributeName="prid" objectCode="" sortAttributeName="timeperiod" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Portlet Timesheet Period Browse" languageCode="en" name="Portlet Timesheet Period Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[SELECT @SELECT:TP.PRID:PRID@,
       @SELECT:TP.PRUID:UNIQUE_CODE@,
       @SELECT:TP.PRSTART:PRSTART@,
       @SELECT:TP.PRFINISH-1:PRFINISH@,
       @SELECT:CONVERT(CHAR(10),TP.PRSTART,120) + ' - ' + CONVERT(CHAR(10),TP.PRFINISH-1,120):TIMEPERIOD@
FROM   PRTIMEPERIOD TP
WHERE (@WHERE:PARAM:USER_DEF:DATE:START_DATE@ IS NULL OR TP.PRSTART >= @WHERE:PARAM:USER_DEF:DATE:START_DATE@)
AND   (@WHERE:PARAM:USER_DEF:DATE:END_DATE@ IS NULL OR TP.PRFINISH <= @WHERE:PARAM:USER_DEF:DATE:END_DATE@)
AND    @FILTER@]]></nsql>
            <nsql databaseId="Niku" dbVendor="oracle"><![CDATA[SELECT @SELECT:TP.PRID:PRID@,
       @SELECT:TP.PRUID:UNIQUE_CODE@,
       @SELECT:TP.PRSTART:PRSTART@,
       @SELECT:TP.PRFINISH-1:PRFINISH@,
       @SELECT:TO_CHAR(TP.PRSTART, 'YYYY-MM-DD') @+@ ' - ' @+@ TO_CHAR((TP.PRFINISH-1), 'YYYY-MM-DD'):TIMEPERIOD@
FROM   PRTIMEPERIOD TP
WHERE (@WHERE:PARAM:USER_DEF:DATE:START_DATE@ IS NULL OR TP.PRSTART >= @WHERE:PARAM:USER_DEF:DATE:START_DATE@)
AND   (@WHERE:PARAM:USER_DEF:DATE:END_DATE@ IS NULL OR TP.PRFINISH <= @WHERE:PARAM:USER_DEF:DATE:END_DATE@)
AND    @FILTER@]]></nsql>
            <nsql databaseId="Niku" dbVendor="postgres"><![CDATA[SELECT @SELECT:TP.PRID:PRID@,
       @SELECT:TP.PRUID:UNIQUE_CODE@,
       @SELECT:TP.PRSTART:PRSTART@,
       @SELECT:TP.PRFINISH - cast('1 days' as interval):PRFINISH@,
       @SELECT:TO_CHAR(TP.PRSTART, 'YYYY-MM-DD') @+@ ' - ' @+@ TO_CHAR((TP.PRFINISH - cast('1 days' as interval)), 'YYYY-MM-DD'):TIMEPERIOD@
FROM   PRTIMEPERIOD TP
WHERE (TP.PRSTART >= @WHERE:PARAM:USER_DEF:DATE:START_DATE@ OR @WHERE:PARAM:USER_DEF:DATE:START_DATE@@CASTAS@date IS  NULL)
AND   (TP.PRFINISH <= @WHERE:PARAM:USER_DEF:DATE:END_DATE@ OR @WHERE:PARAM:USER_DEF:DATE:END_DATE@@CASTAS@date IS  NULL)
AND    @FILTER@]]></nsql>
