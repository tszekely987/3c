            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:S.UNIQUE_NAME:UNIQUE_NAME@,
                 @SELECT:S.UNIQUE_NAME:UNIQUE_CODE@,
                 @SELECT:S.FULL_NAME:FULL_NAME@, @SELECT:S.ID:ID@,
        @SELECT:(case
		           when S.RESOURCE_TYPE=0 then 'L'
		           when S.RESOURCE_TYPE=1 then 'Q'
		           when S.RESOURCE_TYPE=2 then 'M'
		           when S.RESOURCE_TYPE=3 then 'X'
        END):resource_type@,
        @SELECT:(case
		           when S.RESOURCE_TYPE=0 then 'Labor'
		           when S.RESOURCE_TYPE=1 then 'Equipment'
		           when S.RESOURCE_TYPE=2 then 'Material'
		           when S.RESOURCE_TYPE=3 then 'Expense'
        END):resource_type_caption@,
                 @SELECT:S.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
                 FROM SRM_RESOURCES S,PRJ_RESOURCES PR
                 WHERE @FILTER@ @BROWSE-ONLY: AND @WHERE:SECURITY:RESOURCE:S.ID@:BROWSE-ONLY@
				 AND PR.PRISROLE = 1
                 AND S.ID = PR.PRID
                 @BROWSE-ONLY:
                   AND S.IS_ACTIVE=1
              :BROWSE-ONLY@]]></nsql>
