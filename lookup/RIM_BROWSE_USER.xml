            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:USERS.ID:ID@,
                @SELECT:USERS.USER_NAME:UNIQUE_CODE@,
                @SELECT:RESOURCES.LAST_NAME:LAST_NAME@,
				@SELECT:RESOURCES.FIRST_NAME:FIRST_NAME@,
                @SELECT:RESOURCES.FULL_NAME:FULL_NAME@,
                @SELECT:RESOURCES.UNIQUE_NAME:UNIQUE_NAME@,
				@SELECT:RESOURCES.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
                FROM SRM_RESOURCES RESOURCES, CMN_SEC_USERS USERS WHERE USERS.ID = RESOURCES.USER_ID
                AND RESOURCES.IS_HIDDEN = 0 AND
                @WHERE:SECURITY:RESOURCE:RESOURCES.ID@ AND
                @FILTER@
                 AND RESOURCES.PERSON_TYPE != 0
				@BROWSE-ONLY:
				AND RESOURCES.IS_ACTIVE = 1
				:BROWSE-ONLY@]]></nsql>
