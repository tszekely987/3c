<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="USER_STATUS_AL" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="lookup_code" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Custom User Status lookup that searches only for the active and lock status values." languageCode="en" name="User Status - Active Lock"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:LOOKUP.id:id@,
              @SELECT:LOOKUP.lookup_code:lookup_code@,
              @SELECT:LOOKUP.name:name@,
			  @SELECT:LOOKUP.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
 			  @SELECT:LANG.ID:LANGUAGE_ID@,
			  @SELECT:LOOKUP.LANGUAGE_CODE:LANGUAGE_CODE@
            FROM  cmn_lookups_v LOOKUP, CMN_LANGUAGES LANG
            WHERE   LOOKUP.lookup_type='SEC_USER_STATUS'
              AND     LOOKUP.lookup_code in ('ACTIVE', 'LOCK' )
              AND     LOOKUP.language_code=@WHERE:PARAM:LANGUAGE@
			  AND     LOOKUP.LANGUAGE_CODE = LANG.LANGUAGE_CODE	  		
              AND     @FILTER@]]></nsql>
            <displayedSuggestionAttributes>
              <displayedSuggestionAttribute value="name"/>
            </displayedSuggestionAttributes>
            <searchedSuggestionAttributes>
              <searchedSuggestionAttribute value="name"/>
            </searchedSuggestionAttributes>
