            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[SELECT @SELECT:P.ID:PID@,
       @SELECT:NLS.NAME:PNAME@,
       @SELECT:P.PERIOD_TYPE:PTYPE@,
       @SELECT:NLS.DESCRIPTION:DESCRIPTION@,
       @SELECT:L.NAME:LNAME@,
       @SELECT:P.START_DATE:STDATE@,
       @SELECT:P.END_DATE-1:EDATE@,
	     @SELECT:P.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
	     @SELECT:LANG.LANGUAGE_CODE:LANGUAGE_CODE@,
	     @SELECT:LANG.ID:LANGUAGE_ID@
    FROM BIZ_COM_PERIODS P, CMN_LOOKUPS_V L, CMN_CAPTIONS_NLS NLS, CMN_LANGUAGES LANG
   WHERE P.ID = NLS.PK_ID
    AND NLS.TABLE_NAME='BIZ_COM_PERIODS'
    AND NLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
    AND L.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
    AND L.LOOKUP_TYPE = 'PERIOD_TYPE'
    AND L.LOOKUP_CODE = P.PERIOD_TYPE
    AND NLS.LANGUAGE_CODE =LANG.LANGUAGE_CODE
	AND L.LANGUAGE_CODE = NLS.LANGUAGE_CODE
    AND P.ENTITY_ID = (SELECT ENTITY.ID EID FROM ENTITY WHERE ENTITY.ENTITY = (SELECT NAMEOPTIONS.DEFAULTENTITY NDEF FROM NAMEOPTIONS))
    AND P.PERIOD_TYPE = 'ANNUALLY'
    AND @FILTER@]]></nsql>
