<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="AGL_PRJ_BROWSE_INV_INTEGRATION" displayAttributeName="name" hiddenAttributeName="id" objectCode="project" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Active, non-template projects that are not marked for deletion and have integration type as Investment" languageCode="en" name="Agile Project Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:inv.id:id@,
       @SELECT:inv.code:code@,
       @SELECT:inv.code:unique_code@,
       @SELECT:inv.name:name@,
       @SELECT:inv.last_updated_date:last_updated_date@
FROM   inv_investments inv
       INNER JOIN inv_projects prj ON inv.id = prj.prid
       INNER JOIN odf_ca_project odf ON inv.id = odf.id
WHERE  @WHERE:SECURITY:PROJECT:inv.id@
AND    @FILTER@
@BROWSE-ONLY:
AND    inv.is_active = 1
AND    prj.is_template = 0
AND    prj.is_program = 0
AND    odf.agl_integration_type = 'investment'
AND    odf.agl_sync = 1
AND    @NVL@(inv.purge_flag,0) = 0
:BROWSE-ONLY@]]></nsql>
