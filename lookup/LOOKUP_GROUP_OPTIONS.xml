            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT
 @SELECT:A.uc:unique_code@,
 @SELECT:A.uc:id@,
 @SELECT:A.oc:object_code@,
 @SELECT:A.ac:attribute_code@,
 @SELECT:A.an:attribute_name@,
 @SELECT:A.aap:attribute_alias@
FROM
(
select (CASE WHEN cap.custom_label IS NULL THEN cap.name ELSE cap.custom_label END) an,
        oam.attribute_api_alias  uc ,
        oam.object_code oc,
        oam.attribute_code ac,
        oam.attribute_api_alias aap
        from odf_attr_metadata oam
             join cmn_captions_nls cap on cap.table_name = 'ODF_ATTR_METADATA' and cap.language_code = @WHERE:PARAM:LANGUAGE@ and cap.pk_id = oam.ID
where oam.object_code='oba_todo' and oam.attribute_code in ('due_date','owner_name','name')
)A
WHERE 1=1
 and @FILTER@]]></nsql>
