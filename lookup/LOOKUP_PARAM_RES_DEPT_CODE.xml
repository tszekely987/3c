          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_PARAM_RES_DEPT_CODE" displayAttributeName="shortdesc" hiddenAttributeName="departcode" objectCode="" sortAttributeName="departcode" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Department browse for resource financials page" languageCode="en" name="Department browse for resource financials"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT
                 @SELECT:DEPARTMENTS.DEPARTCODE:DEPARTCODE@,
                 @SELECT:DEPARTMENTS.SHORTDESC:SHORTDESC@,
                 @SELECT:DEPARTMENTS.DESCRIPTION:DESCRIPTION@,
                 @SELECT:(SELECT entity
                            FROM entity    WHERE id = DEPARTMENTS.entity_id):ENTITY@,
                 @SELECT:DEPARTMENTS.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
            FROM DEPARTMENTS
           WHERE @FILTER@
           @BROWSE-ONLY:
		   AND IS_ACTIVE = 1 AND @WHERE:SECURITY:DEPARTMENT:DEPARTMENTS.ID@
            and exists (select 1 from srm_resources r, locations l
	                          where l.entity_id=DEPARTMENTS.entity_id
	                          and @WHERE:PARAM:USER_ID@ = r.user_ID
                               AND r.IS_HIDDEN = 0                   and @DBUSER@.PAC_FOS_IS_VALID_LOCATION_FCT(r.unique_name,l.locationid) = 1)
            AND exists (select 1 from locationdept where locationdept.dept_id = departments.id)
           :BROWSE-ONLY@]]></nsql>
