            <nls description="List of charge codes when Investment specific charge codes are enabled" languageCode="en" name="Active Charge Codes"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[
                  
         select @SELECT:cc.prID:prID@,
        @SELECT:cc.PRUID:UNIQUE_CODE@,
        @SELECT:cc.prName:prName@,
        @SELECT:cc.prExternalID:prExternalID@,
        @SELECT:inv.name:name@,
		@SELECT:cc.prModTime:LAST_UPDATED_DATE@
 from prchargecode cc left outer join inv_investments inv on cc.prprojectid=inv.id
where @FILTER@
@BROWSE-ONLY:  
and cc.prisopen<>0
and (cc.prprojectid is null or cc.prprojectid=ISNULL(@WHERE:PARAM:USER_DEF:INTEGER:INV_ID@,@WHERE:PARAM:USER_DEF:INTEGER:PROJECT_ID@))
  :BROWSE-ONLY@
                ]]></nsql>
            <nsql databaseId="Niku" dbVendor="oracle"><![CDATA[select @SELECT:cc.prID:prID@,
        @SELECT:cc.PRUID:UNIQUE_CODE@,
        @SELECT:cc.prName:prName@,
        @SELECT:cc.prExternalID:prExternalID@,
        @SELECT:inv.name:name@,
		@SELECT:cc.prModTime:LAST_UPDATED_DATE@
 from prchargecode cc, inv_investments inv
where cc.prprojectid=inv.id(+)
@BROWSE-ONLY:  
and cc.prisopen<>0
and (cc.prprojectid is null or cc.prprojectid=NVL(@WHERE:PARAM:USER_DEF:INTEGER:INV_ID@,@WHERE:PARAM:USER_DEF:INTEGER:PROJECT_ID@))
  :BROWSE-ONLY@
and @FILTER@]]></nsql>
            <nsql databaseId="Niku" dbVendor="postgres"><![CDATA[select @SELECT:cc.prID:prID@,
 @SELECT:cc.PRUID:UNIQUE_CODE@,
 @SELECT:cc.prName:prName@,
