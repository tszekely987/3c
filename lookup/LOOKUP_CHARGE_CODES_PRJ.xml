 @SELECT:inv.name:name@,
 @SELECT:cc.prModTime:LAST_UPDATED_DATE@
 from prchargecode cc left outer join inv_investments inv on cc.prprojectid=inv.id
where @FILTER@
@BROWSE-ONLY:
and cc.prisopen<>0
and (cc.prprojectid is null or cc.prprojectid=coalesce(@WHERE:PARAM:USER_DEF:INTEGER:INV_ID@,@WHERE:PARAM:USER_DEF:INTEGER:PROJECT_ID@))
  :BROWSE-ONLY@
]]></nsql>
