            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[SELECT  @SELECT:EA.name + ' ('+ EO.name + ')' :external_attribute_name@,
        @SELECT:EA.code:external_attribute_code@
        FROM INT_EXTERNAL_ATTRIBUTES  EA, INT_EXTERNAL_OBJECTS  EO
        WHERE   @FILTER@
		AND     EA.external_object_id = EO.id
        @BROWSE-ONLY:
        AND     ((EA.name in ('Milestones','State','Investment Category','Project','Projects','Preliminary Estimate')
        AND     EA.external_data_type in ('COLLECTION','RATING','OBJECT')) OR (EA.is_custom = 1  and real_attr_type = 'DROP_DOWN'))
		AND 	((EA.name = 'State' and EO.type_path not in ('Objective')) or (EA.name != 'State' and EO.type_path not like 'Objective/%'))
        AND     EA.is_active = 1
        AND     EO.is_active = 1
		AND     (@WHERE:PARAM:USER_DEF:INTEGER:TYPE_CONSTRAIN@ = EO.IM_ID)
        :BROWSE-ONLY@]]></nsql>
