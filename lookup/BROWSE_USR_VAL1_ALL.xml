<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="BROWSE_USR_VAL1_ALL" displayAttributeName="name" hiddenAttributeName="lookup_code" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Browse for all User LOV1 candidates when context is not present." languageCode="en" name="User Value 1 Lookup All"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT
           @SELECT:LOOKUP_CODE:LOOKUP_CODE@,
           @SELECT:NAME:NAME@,
           @SELECT:LANGUAGE_ID:LANGUAGE_ID@,
           @SELECT:LANGUAGE_CODE:LANGUAGE_CODE@

            FROM ( SELECT lkp.LOOKUP_CODE, lkp.NAME AS NAME, lkp.SORT_ORDER
                  , lkp.LANGUAGE_CODE, l.ID LANGUAGE_ID
                  FROM   CMN_LOOKUPS_V lkp, CMN_LANGUAGES l
                    WHERE  LOOKUP_TYPE = 'PRTIMEENTRY_USER_LOV1'
                    AND 	 lkp.LANGUAGE_CODE = l.LANGUAGE_CODE
                    AND    lkp.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
                    AND    lkp.IS_ACTIVE = 1
                    AND EXISTS (
                    SELECT 'A' FROM CMN_OPTION_VALUES WHERE OPTION_ID IN (
                    SELECT ID FROM CMN_OPTIONS WHERE OPTION_CODE = 'NKT.GEN.LOOKUP_4_USER_VALUE1')
                    AND USER_ID IS NULL
                    AND VALUE = 'PRTIMEENTRY_USER_LOV1')

                  UNION

                  SELECT LOOKUP_CODE, LOOKUP_CODE AS NAME, 1 SORT_ORDER,
                  'en' LANGUAGE_CODE,	1 LANGUAGE_ID
                  FROM USER_LOV1_ALL_V
                    WHERE NOT EXISTS (
                    SELECT 'A' FROM CMN_OPTION_VALUES WHERE OPTION_ID IN (
                    SELECT ID FROM CMN_OPTIONS WHERE OPTION_CODE = 'NKT.GEN.LOOKUP_4_USER_VALUE1')
                    AND USER_ID IS NULL
                    AND VALUE = 'PRTIMEENTRY_USER_LOV1')
                  ) LOOKUP_DATA
            WHERE @FILTER@]]></nsql>
