      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="FIN_TXN_TRANSCLASS" displayAttributeName="shortdesc" hiddenAttributeName="transclass" objectCode="" sortAttributeName="shortdesc" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Browse for Transaction Class" languageCode="en" name="Transaction Class Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT	@SELECT:T.TRANSCLASS:TRANSCLASS@,
	@SELECT:T.SHORTDESC:SHORTDESC@,
	@SELECT:L.NAME:TYPE@
FROM TRANSCLASS T, CMN_LOOKUPS_V L, SRM_RESOURCES S
WHERE @FILTER@
	AND L.LOOKUP_TYPE = 'PAC_TRANSACTION_TYPE'         
	AND L.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
	AND L.LOOKUP_CODE = T.TRANSTYPE 
	AND @NVL@(T.BUILT_IN,0) != 1
	@BROWSE-ONLY:
	AND S.UNIQUE_NAME = @NVL@(@WHERE:PARAM:USER_DEF:string:RESOURCE_CODE@, S.UNIQUE_NAME)
	AND T.TRANSTYPE = (CASE WHEN (4798 = @WHERE:PARAM:USER_DEF:integer:VOUCHER_ENTRY_TYPE@ OR S.RESOURCE_TYPE = 3) THEN 'X' ELSE
	  (CASE WHEN S.RESOURCE_TYPE = 0 THEN 'L'
			WHEN S.RESOURCE_TYPE = 1 THEN 'Q'
			WHEN S.RESOURCE_TYPE = 2 THEN 'M'
      END) END)
	:BROWSE-ONLY@]]></nsql>
