      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="FIN_TXN_INVESTMENTS" displayAttributeName="name" hiddenAttributeName="code" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Financially enabled Investments for Transactions" languageCode="en" name="Investments for Transactions"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:MP.PROJECT_CODE:CODE@
       ,@SELECT:INV.NAME:NAME@
       ,@SELECT:OBJ.CODE:INVESTMENT_TYPE@
       ,@SELECT:OBJ.NAME:INVESTMENT_TYPE_NAME@
FROM INV_INVESTMENTS INV LEFT OUTER JOIN INV_PROJECTS PRJ ON INV.ID = PRJ.PRID,
      PAC_MNT_PROJECTS MP, DEPARTMENTS D, ODF_OBJECTS_V OBJ
WHERE @WHERE:SECURITY:INV:INV.ID@
      AND OBJ.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
      AND OBJ.CODE = INV.ODF_OBJECT_CODE
      AND (INV.PURGE_FLAG=0 OR INV.PURGE_FLAG IS NULL)
      AND INV.ID = MP.ID
      AND MP.DEPARTCODE=D.DEPARTCODE
      AND (MP.STATUS = 'O' OR EXISTS (SELECT 1 FROM PPA_TRANSCONTROL PPP WHERE  PPP.PROJECT_CODE = UPPER(INV.CODE)))
      AND (MP.ODF_OBJECT_CODE IN ('idea', 'project') OR (OBJ.TEMPLATE_EXTENSION = 'inv_extension' AND OBJ.IS_CUSTOM = 1))
      AND @FILTER@
      @BROWSE-ONLY: AND INV.IS_ACTIVE = 1 AND @NVL@(PRJ.IS_PROGRAM,0) = 0 AND @NVL@(PRJ.IS_TEMPLATE,0) = 0 :BROWSE-ONLY@]]></nsql>
