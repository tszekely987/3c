      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="FIN_TXN_INVESTMENTS" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Financially enabled Investments for Transactions" languageCode="en" name="Investments for Transactions"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:MP.ID:ID@
       ,@SELECT:INV.NAME:NAME@
       ,@SELECT:MP.PROJECT_CODE:CODE@
       ,@SELECT:OBJ.CODE:INVESTMENT_TYPE@
       ,@SELECT:OBJ.NAME:INVESTMENT_TYPE_NAME@
FROM INV_INVESTMENTS INV
JOIN PAC_MNT_PROJECTS MP ON INV.ID = MP.ID AND COALESCE(INV.PURGE_FLAG,0) = 0 AND MP.DEPARTCODE IS NOT NULL
JOIN ( SELECT O.CODE,(SELECT NAME FROM CMN_CAPTIONS_NLS NLS
                                           WHERE TABLE_NAME = 'ODF_OBJECTS' AND LANGUAGE_CODE =@WHERE:PARAM:LANGUAGE@ AND NLS.PK_ID = O.ID) NAME
        FROM ODF_OBJECTS O WHERE (O.CODE IN ('idea', 'project') OR (O.TEMPLATE_EXTENSION = 'inv_extension' AND O.IS_CUSTOM = 1))
     ) OBJ ON OBJ.CODE = INV.ODF_OBJECT_CODE
WHERE @WHERE:SECURITY:INV:INV.ID@
  AND @FILTER@
  @BROWSE-ONLY: AND INV.IS_ACTIVE = 1
  AND (MP.STATUS = 'O' OR INV.ID IN (SELECT INV.ID
                                         FROM INV_INVESTMENTS INV
                                         JOIN PPA_TRANSCONTROL PPP ON PPP.PROJECT_CODE = UPPER(INV.CODE)
                                       GROUP BY INV.ID
                                    )
       )
  AND MP.DEPARTCODE IN (SELECT D.DEPARTCODE FROM DEPARTMENTS D)
  AND INV.ID IN (SELECT INV.ID FROM INV_INVESTMENTS INV
                                 LEFT JOIN INV_PROJECTS PRJ ON INV.ID = PRJ.PRID
                                 WHERE @NVL@(PRJ.IS_PROGRAM,0) = 0 AND @NVL@(PRJ.IS_TEMPLATE,0) = 0) :BROWSE-ONLY@]]></nsql>
