<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="FIN_BROWSE_RESOURCE_TYPE" displayAttributeName="name" hiddenAttributeName="code" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Resource Type" languageCode="en" name="Resource Type"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DATA.LOOKUP_CODE:CODE@
  		 ,@SELECT:DATA.NAME:NAME@
  		 ,@SELECT:DATA.DESCRIPTION:DESCRIPTION@
  		 ,@SELECT:DATA.LANGUAGE_ID:LANGUAGE_ID@
  		 ,@SELECT:DATA.LANGUAGE_CODE:LANGUAGE_CODE@
  		 ,@SELECT:DATA.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
  		        FROM
(select V.name,
case V.lookup_code when '0' then 'L'
when '1' then 'Q'
when '2' then 'M'
when '3' then 'X'
end lookup_code, V.description, LANG.id language_id, LANG.language_code, V.last_updated_date
from CMN_LOOKUPS_V V, CMN_LANGUAGES LANG
                         WHERE  V.LOOKUP_TYPE = 'RESOURCE_TYPE'
                         AND V.LANGUAGE_CODE = LANG.LANGUAGE_CODE
                     AND    V.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
                     AND    V.IS_ACTIVE = 1) DATA
                     WHERE @FILTER@]]></nsql>
