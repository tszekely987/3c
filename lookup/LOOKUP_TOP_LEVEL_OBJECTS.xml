      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_TOP_LEVEL_OBJECTS" displayAttributeName="name" hiddenAttributeName="code" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of Top Level Objects" languageCode="en" name="Top Level Objects"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[select @select:o.code:code@,
	           @select:nls.name:name@,
	           @select:nls.description:description@,@SELECT:o.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
		,@SELECT:l.ID:LANGUAGE_ID@,
		@SELECT:nls.LANGUAGE_CODE:LANGUAGE_CODE@	
	    from   odf_objects o, cmn_captions_nls nls,CMN_LANGUAGES l
	    where  o.id = nls.pk_id
	    and    nls.table_name = 'ODF_OBJECTS'
	    and    o.is_customizable = 1
	    and    nls.language_code = @where:param:language@
	    and    o.parent_object_code is null
		AND    nls.LANGUAGE_CODE = l.LANGUAGE_CODE
	    and    (exists( select  user_id
	                    from    cmn_sec_assgnd_obj_perm_r_v
	                    where   user_id = @where:param:user_id@
	                    and     permission_code = 'WRITE'
	                    and     object_code = 'ODF_OBJECT'
	                    and     object_type = 'RECORD'
	                    and     component_code = 'CMN')
	            or
	            exists( select  user_id
	                    from    cmn_sec_assgnd_obj_perm_v
	                    where   user_id = @where:param:user_id@
	                    and     object_instance_id = o.id
	                    and     permission_code = 'WRITE'
	                    and     object_code = 'ODF_OBJECT'
	                    and     object_type = 'RECORD'
	                    and     component_code = 'CMN') )
	    and    @filter@]]></nsql>
