            <nls description="Displays investments that are added to the Release plan's content." languageCode="en" name="Browse Investments for Release Planning"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT	
       @SELECT:p.id:id@,
       @SELECT:p.code:code@,
       @SELECT:p.name:name@,
       @SELECT:p.odf_object_code:object@
FROM inv_investments p, rqp_plans_investments rpi  
 WHERE @WHERE:SECURITY:INV:P.ID@
 AND P.ID = RPI.Investment_ID
 AND P.IS_CUSTOM = 0
 AND RPI.Release_Plan_ID = @WHERE:PARAM:XML:INTEGER:/data/id/@value@
 AND @FILTER@
 @BROWSE-ONLY:
   AND P.IS_ACTIVE = 1
:BROWSE-ONLY@]]></nsql>
