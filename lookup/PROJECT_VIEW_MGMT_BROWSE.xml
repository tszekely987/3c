      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="PROJECT_VIEW_MGMT_BROWSE" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Browse for projects that user has Project - View Management Access Right to." languageCode="en" name="Project View Management Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT @SELECT:P.ID:ID@,
                  @SELECT:P.NAME:NAME@,  
                  @SELECT:P.UNIQUE_NAME:UNIQUE_NAME@,
                  @SELECT:P.UNIQUE_NAME:UNIQUE_CODE@,
                  @SELECT:P.DESCRIPTION:DESCRIPTION@,
                  @SELECT:P.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
   FROM (SELECT INVESTMENTS.ID ID, INVESTMENTS.NAME AS NAME, INVESTMENTS.CODE UNIQUE_NAME,INVESTMENTS.DESCRIPTION DESCRIPTION, INVESTMENTS.LAST_UPDATED_DATE
   FROM     INV_INVESTMENTS INVESTMENTS,
                  INV_PROJECTS PR,
                  CMN_SEC_ASSGND_OBJ_PERM_V APV
   WHERE
                  INVESTMENTS.ID = APV.OBJECT_INSTANCE_ID  AND
                  APV.COMPONENT_CODE = 'PRJ' AND
                  APV.OBJECT_CODE = 'PRJ_PROJECT' AND
                  APV.OBJECT_TYPE = 'RECORD' AND
                  APV.PERMISSION_CODE = 'ProjectViewManagement' AND
                   INVESTMENTS.ID = PR.PRID AND
                  INVESTMENTS.IS_ACTIVE != 0 AND
                  PR.IS_TEMPLATE = 0 AND
                  APV.USER_ID = @WHERE:PARAM:USER_ID@
   UNION
        SELECT INVESTMENTS.ID ID, INVESTMENTS.NAME AS NAME, INVESTMENTS.CODE UNIQUE_NAME,INVESTMENTS.DESCRIPTION DESCRIPTION, INVESTMENTS.LAST_UPDATED_DATE
   FROM     INV_INVESTMENTS INVESTMENTS,
                 INV_PROJECTS PR,
                  CMN_SEC_ASSGND_OBJ_PERM_R_V APV
   WHERE
                  APV.COMPONENT_CODE = 'PRJ' AND
                  APV.OBJECT_CODE = 'PRJ_PROJECT' AND
                  APV.OBJECT_TYPE = 'RECORD' AND
                  APV.PERMISSION_CODE = 'ProjectViewManagement' AND
                  INVESTMENTS.ID = PR.PRID AND
                  INVESTMENTS.IS_ACTIVE != 0 AND
                  PR.IS_TEMPLATE = 0 AND
                 APV.USER_ID = @WHERE:PARAM:USER_ID@) P
   WHERE  @FILTER@]]></nsql>
