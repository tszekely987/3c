          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_BRM_RULE_ACTION_TYPES" displayAttributeName="name" hiddenAttributeName="code" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="The rule action types for an object " languageCode="en" name="Rule Action Types"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT @SELECT:a.code:code@,
   @SELECT:nls.name:name@,
   @SELECT:a.parent_action_type_code:parent_action_type_code@,
   @SELECT:a.has_sub_actions:has_sub_actions@,
   @SELECT:a.category:category@,
   @SELECT:nls.description:description@,
   @SELECT:nls.language_code:language_code@,
   @SELECT:oa.object_level:object_level@,
   @SELECT:oa.object_code:object_code@
FROM brm_rule_action_types a
   JOIN brm_object_rule_actions oa ON a.id = oa.rule_action_type_id
   JOIN cmn_captions_nls nls ON nls.table_name = 'BRM_RULE_ACTION_TYPES'
        AND nls.pk_id = a.id
        AND nls.language_code = @WHERE:PARAM:language@
WHERE @FILTER@ AND a.is_active = 1
   AND ( oa.object_level = ( SELECT CASE WHEN parent_object_code IS NULL THEN 1 ELSE 2 END FROM odf_objects WHERE code = @WHERE:PARAM:USER_DEF:STRING:OBJECT_CODE@ )
             OR oa.object_code = @WHERE:PARAM:USER_DEF:STRING:OBJECT_CODE@ )
   AND a.code NOT IN (
       SELECT CASE WHEN count(capability_code) = 0 THEN 'BRM_REQUIRED_ATTRIBUTE' ELSE 'dummy' END
       FROM  odf_object_capabilities WHERE capability_code in ('MODULE_BLUEPRINT_ENABLED', 'SUB_OBJECT_MODAL') AND object_code = (@WHERE:PARAM:USER_DEF:STRING:OBJECT_CODE@)
   )]]></nsql>
