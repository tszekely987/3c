            <nls description="Object Action System Menu Item Browse" languageCode="en" name="Object Action System Menu Item Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:O.ID:ID@,
            @SELECT:C.NAME:NAME@,
            @SELECT:O.CODE:CODE@,
            @SELECT:O.OBJECT_CODE:OBJECT_CODE@,
            @SELECT:V.NAME:TYPE@,
            @SELECT:C.DESCRIPTION:DESCRIPTION@,
            @SELECT:V.ID:VID@,
            @SELECT:LANG.ID:LANGUAGE_ID@,
            @SELECT:LANG.LANGUAGE_CODE:LANGUAGE_CODE@
            FROM    ODF_ACTIONS O, CMN_CAPTIONS_NLS C, CMN_LOOKUPS_V V, CMN_LANGUAGES LANG
            WHERE   @FILTER@
            AND 	C.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
            AND		O.ID = C.PK_ID
            AND   	O.IS_INSTANCE = 0
            AND		O.IS_ACTIVE = 1
            AND		O.TYPE IN ('internallink', 'externallink', 'report')
            AND		C.TABLE_NAME = 'ODF_ACTIONS'
            AND 	V.IS_ACTIVE = 1
            AND 	V.LOOKUP_TYPE ='STATIC_ACTION_MENU_ITEM_TYPES'
            AND		O.TYPE = V.LOOKUP_CODE
            AND     C.LANGUAGE_CODE = LANG.LANGUAGE_CODE
            AND     V.LANGUAGE_CODE = C.LANGUAGE_CODE
            AND		O.ID NOT IN (
            SELECT	OA.ID
            FROM 	ODF_ACTIONS OA, CMN_ACTIONS A, CMN_MENU_ITEMS I
            WHERE	A.ID = I.ACTION_ID
            AND		OA.ID = A.OBJECT_ACTION_ID
            AND		I.PRINCIPAL_TYPE = 'SYSTEM'
            AND 	I.PRINCIPAL_ID = 0 )
			AND 	o.OBJECT_CODE NOT IN
		(select distinct cfa.code from cmn_features cf, CMN_FEATURE_ARTIFACTS cfa where cfa.type = 'ODF_OBJECT' and o.OBJECT_CODE = cfa.code and cf.ENABLED = 0 and cf.id = cfa.feature_id
		and not exists (select cfa.code from cmn_features cfs, CMN_FEATURE_ARTIFACTS cfas where cfas.type = 'ODF_OBJECT' and cfa.code = cfas.code and cfs.ENABLED = 1 and cfs.id = cfas.feature_id) )]]></nsql>
