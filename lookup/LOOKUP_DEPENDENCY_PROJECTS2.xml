<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_DEPENDENCY_PROJECTS2" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Browse for dependency Project" languageCode="en" name="Project Dependency Lookup"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT @SELECT:P.ID:ID@,
                                @SELECT:P.NAME:NAME@,
                                @SELECT:P.CODE:CODE@,
                                @SELECT:P.CODE:UNIQUE_CODE@,
									              @SELECT:P.TYPE:TYPE@,                            
									              @SELECT:P.TYPE_NAME:TYPE_NAME@              
                FROM
                    (SELECT SR.ID AS ID, SR.NAME AS NAME, SR.CODE AS CODE, CA.ODF_OBJECT_CODE AS TYPE, CLS.NAME AS TYPE_NAME
                     FROM INV_INVESTMENTS SR, INV_PROJECTS PR, ODF_CA_INV CA, ODF_CLASS_V CLS,
                               CMN_SEC_ASSGND_OBJ_PERM_V APV
                     WHERE SR.IS_ACTIVE != 0 AND
                          SR.ID = APV.OBJECT_INSTANCE_ID  AND
                          APV.COMPONENT_CODE = 'PRJ' AND
                          APV.OBJECT_CODE = 'PRJ_PROJECT' AND
                          APV.OBJECT_TYPE = 'RECORD' AND
                          APV.PERMISSION_CODE = 'ProjectViewManagement' AND
                          SR.ID = PR.PRID    AND
                          PR.IS_TEMPLATE = 0  AND
										      CLS.CODE = CA.ODF_OBJECT_CODE AND 						CLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@ AND
													CA.ID = SR.ID AND
                          APV.USER_ID = @WHERE:PARAM:USER_ID@
                     UNION
                     SELECT SR.ID AS ID,SR.NAME AS NAME,SR.CODE AS CODE, CA.ODF_OBJECT_CODE AS TYPE, CLS.NAME AS TYPE_NAME
                     FROM INV_INVESTMENTS SR, INV_PROJECTS PR, ODF_CA_INV CA, ODF_CLASS_V CLS, 
                               CMN_SEC_ASSGND_OBJ_PERM_R_V APV
                     WHERE SR.IS_ACTIVE != 0 AND
                          APV.COMPONENT_CODE = 'PRJ' AND
                          APV.OBJECT_CODE = 'PRJ_PROJECT' AND
                          APV.OBJECT_TYPE = 'RECORD' AND
                          APV.PERMISSION_CODE = 'ProjectViewManagement' AND
                          SR.ID = PR.PRID    AND
                          PR.IS_TEMPLATE = 0    AND
										      CLS.CODE = CA.ODF_OBJECT_CODE AND
				      						CLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@ AND
													CA.ID = SR.ID AND                          
                          APV.USER_ID = @WHERE:PARAM:USER_ID@) P
                WHERE  @FILTER@]]></nsql>
            <displayedSuggestionAttributes>
              <displayedSuggestionAttribute value="name"/>
              <displayedSuggestionAttribute value="code"/>
            </displayedSuggestionAttributes>
            <searchedSuggestionAttributes>
