      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="PRJ_AUTOSCHED_PROJ_BROWSE2" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="User with Management - Schedule In Browser right can use this to browse for Projects that the user has Edit Management Access Right to." languageCode="en" name="Autoschedule Project Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT @SELECT:P.ID:ID@,
              @SELECT:P.NAME:NAME@,
              @SELECT:P.CODE:CODE@,
              @SELECT:P.CODE:UNIQUE_CODE@,
              @SELECT:P.DESCRIPTION:DESCRIPTION@,
              @SELECT:P.TYPE:TYPE@,                            
              @SELECT:P.TYPE_NAME:TYPE_NAME@              
       FROM  (SELECT  SR.ID ID, SR.NAME AS NAME, SR.CODE,SR.DESCRIPTION DESCRIPTION, CA.ODF_OBJECT_CODE AS TYPE, CLS.NAME TYPE_NAME
              FROM    INV_INVESTMENTS SR,
                      INV_PROJECTS PR,
                      ODF_CA_INV CA, ODF_CLASS_V CLS,
                      CMN_SEC_CHK_USER_V0 APV
              WHERE   SR.ID = APV.OBJECT_INSTANCE_ID
              AND     APV.OBJECT_ID=663
              AND     APV.PERMISSION_CODE = 'ProjectViewManagement'
              AND     SR.ID = PR.PRID
              AND     SR.IS_ACTIVE = 1
              AND     PR.IS_TEMPLATE = 0
              AND     CA.ID = SR.ID
				      AND     CLS.CODE = CA.ODF_OBJECT_CODE
				      AND     CLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@                    
              AND     APV.USER_ID = @WHERE:PARAM:USER_ID@
             UNION
              SELECT  SR.ID ID, SR.NAME AS NAME, SR.CODE,SR.DESCRIPTION DESCRIPTION, CA.ODF_OBJECT_CODE AS TYPE, CLS.NAME TYPE_NAME
              FROM    INV_INVESTMENTS SR,
                      INV_PROJECTS PR,
                      ODF_CA_INV CA, ODF_CLASS_V CLS,
                      CMN_SEC_ASSGND_OBJ_PERM_R_V0 APV
              WHERE APV.OBJECT_ID=663
              AND      APV.PERMISSION_CODE = 'ProjectViewManagement'
              AND      SR.ID = PR.PRID
              AND      SR.IS_ACTIVE = 1
              AND      PR.IS_TEMPLATE = 0
              AND      CA.ID = SR.ID
				      AND      CLS.CODE = CA.ODF_OBJECT_CODE
				      AND      CLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@                    
              AND      APV.USER_ID = @WHERE:PARAM:USER_ID@) P
             ,CMN_SEC_ASSGND_OBJ_PERM_R_V0 APG
       WHERE  APG.OBJECT_ID=663
       AND    APG.PERMISSION_CODE = 'permProjectScheduling'
       AND    APG.USER_ID = @WHERE:PARAM:USER_ID@
       AND    @FILTER@]]></nsql>
