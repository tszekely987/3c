<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_FOR_CUST_ATTR_LIST" displayAttributeName="name" hiddenAttributeName="lookup_type" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of active lookups that are available to be added as the lookup for a custom attribute." languageCode="en" name="Active lookups available for custom attributes"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:lookups.name:name@,
      @SELECT:lookups.description:description@,
      @SELECT:lookups.lookup_type:lookup_type@,
      @SELECT:lookups.lookup_source:lookup_source@,
      @SELECT:lookups.lookup_source_code:lookup_source_code@,
      @SELECT:lookups.lookup_return_type_name:lookup_return_type_name@,
      @SELECT:lookups.lookup_code:lookup_return_type@,
      @SELECT:lookups.lookup_id:lookup_id@
      from
(SELECT  a.name as name,
      a.description,
      b.lookup_type,
      e.name lookup_source,
      e.id lookup_source_code,
      f.name lookup_return_type_name,
      f.lookup_code,
      f.id lookup_id
FROM    cmn_captions_nls a,
      cmn_lookup_types b,
      cmn_list_of_values c,
      cmn_lov_objects d,
      cmn_lookups_v e,
      cmn_lookups_v f,
      cmn_attributes type
WHERE   a.table_name =  'CMN_LOOKUP_TYPES'
AND     b.is_active = 1
AND     a.pk_id = b.id                     
AND     a.language_code = @WHERE:PARAM:LANGUAGE@                        
AND     b.lookup_type =        c.lookup_type_code
AND     c.lookup_source_code = e.lookup_code                     
AND     c.lookup_source_code != 'LOOKUP_SOURCE_DYNAMIC'
AND     e.language_code =         @WHERE:PARAM:LANGUAGE@
AND     c.lookup_type_code = d.lookup_type_code
AND     ( c.parent_id = 0 OR c.parent_id is null )
AND     d.ui_type = 'LOV_SELECT'
and     type.id = d.key_attribute_id
and     f.lookup_code = type.data_type
and     f.lookup_type = 'CMN_ATTRIBUTE_DATA_TYPES'
AND     f.language_code =         @WHERE:PARAM:LANGUAGE@
and     b.lookup_type not like 'ODF.OBJECT.%'
and     b.lookup_type not like 'ODF.NSQL.%'
and     b.lookup_type not like 'ODF.SYSTEM.%'
and     b.lookup_type not like 'ODF_CST_%_PAGE_LAYOUT'
and     b.lookup_type not like 'LOOKUP_%_PAGE_LAYOUT'
and     b.lookup_type not like 'OBS_BROWSE_%'
and     b.lookup_type != 'LOOKUP_FOR_CUST_ATTR_LIST'
and     b.lookup_type != 'LOOKUP_LOOKUP_WTYPE_LIST'
and     b.lookup_type not like 'TIME_BROWSE_%'
and     b.lookup_type not like '__RDM__%'
and     b.lookup_type not like '__ODF__%__PL__%'

union all

SELECT  a.name as name,
      a.description,
      b.lookup_type,
      e.name lookup_source,
      e.id lookup_source_code,
      f.name lookup_return_type_name,
      f.lookup_code,
      f.id lookup_id
FROM    cmn_captions_nls a,
      cmn_lookup_types b,
      cmn_list_of_values c,
      cmn_lov_objects d,
      cmn_lookups_v e,
      cmn_lookups_v f,
      cmn_attributes type
WHERE   a.table_name =  'CMN_LOOKUP_TYPES'
AND     b.is_active = 1
AND     a.pk_id = b.id                     
AND     a.language_code = @WHERE:PARAM:LANGUAGE@                        
AND     b.lookup_type =        c.lookup_type_code
AND     c.lookup_source_code = e.lookup_code           
AND     c.lookup_source_code = 'LOOKUP_SOURCE_DYNAMIC'          
AND     e.language_code =         @WHERE:PARAM:LANGUAGE@
AND     c.lookup_type_code = d.lookup_type_code
AND     ( c.parent_id = 0 OR c.parent_id is null )
AND     d.ui_type = 'LOV_SELECT'
and     type.id = d.key_attribute_id
and     f.lookup_code = type.data_type
and     f.lookup_type = 'CMN_ATTRIBUTE_DATA_TYPES'
AND     f.language_code =         @WHERE:PARAM:LANGUAGE@
and     b.lookup_type not like 'ODF.OBJECT.%'
and     b.lookup_type not like 'ODF.NSQL.%'
and     b.lookup_type not like 'ODF.SYSTEM.%'
and     b.lookup_type not like 'ODF_CST_%_PAGE_LAYOUT'
and     b.lookup_type not like 'LOOKUP_%_PAGE_LAYOUT'
and     b.lookup_type not like 'OBS_BROWSE_%'
and     b.lookup_type != 'LOOKUP_FOR_CUST_ATTR_LIST'
and     b.lookup_type != 'LOOKUP_LOOKUP_WTYPE_LIST'
and     b.lookup_type not like 'TIME_BROWSE_%'
and     b.lookup_type not like '__RDM__%'
and     b.lookup_type not like '__ODF__%__PL__%'
and exists(select 1 from cmn_nsql_queries where id = c.sql_text_id and db_id != 'Datawarehouse')
)lookups
WHERE    1=1
AND      @FILTER@]]></nsql>
