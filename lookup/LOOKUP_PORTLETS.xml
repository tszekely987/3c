            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:portlets.id:id@,
            @SELECT:portlets.portlet_code:portlet_code@,
            @SELECT:nls.name:portlet_name@,
            @SELECT:nls.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
            @SELECT:l.ID:LANGUAGE_ID@,
            @SELECT:nls.LANGUAGE_CODE:LANGUAGE_CODE@
            FROM cmn_portlets portlets, cmn_captions_nls nls, cmn_lookups_v lkp,CMN_LANGUAGES l
            WHERE portlets.is_active = 1
            AND nls.pk_id = portlets.id
            AND nls.table_name = 'CMN_PORTLETS'
            AND nls.language_code = @WHERE:PARAM:LANGUAGE@
            AND lkp.language_code = @WHERE:PARAM:LANGUAGE@
            AND lkp.id = portlets.category_id
            AND lkp.lookup_code != 'framework'
            AND portlet_type_code != 'SYSTEM'
            AND lkp.lookup_type = 'CMN_CATEGORIES'
            AND nls.LANGUAGE_CODE = lkp.LANGUAGE_CODE
            AND nls.LANGUAGE_CODE = l.LANGUAGE_CODE
            AND @FILTER@]]></nsql>
