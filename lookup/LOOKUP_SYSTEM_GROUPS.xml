		@SELECT:LANG.ID:LANGUAGE_ID@
FROM    cmn_sec_groups g,
        cmn_captions_nls c,
CMN_LANGUAGES LANG
WHERE   @FILTER@
AND     g.is_active = 1
AND     g.group_role_type = 'GROUP'
AND     g.group_code not like 'CLB%'
and     c.pk_id = g.id
and     c.table_name =  'CMN_SEC_GROUPS'
and     c.language_code = @WHERE:PARAM:LANGUAGE@
AND     LANG.LANGUAGE_CODE=c.LANGUAGE_CODE]]></nsql>
