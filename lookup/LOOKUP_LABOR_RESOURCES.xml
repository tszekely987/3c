      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_LABOR_RESOURCES" displayAttributeName="full_name" hiddenAttributeName="id" objectCode="resource" sortAttributeName="last_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="All Labor Resources browse dynamic lookup" languageCode="en" name="All Labor Resources Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:sr.id:id@,
	                    @SELECT:sr.last_name:last_name@,
	                    @SELECT:sr.first_name:first_name@,
	                    @SELECT:sr.full_name:full_name@,
	                    @SELECT:sr.unique_name:unique_name@,
	                    @SELECT:sr.unique_name:unique_code@,
	                    @SELECT:pr.prprimaryroleid:prprimaryroleid@,
	                    @SELECT:roles.unique_name:role_unique_name@,
	                    @SELECT:roles.full_name:role_name@,
						@SELECT:sr.last_updated_date:last_updated_date@
	                FROM srm_resources sr,
	                     prj_resources pr LEFT OUTER JOIN srm_resources roles ON roles.id = pr.prprimaryroleid
	                WHERE 1=1
	                    AND sr.resource_type = 0
	                    AND sr.person_type in (300,301)
	                    AND @WHERE:SECURITY:RESOURCE:sr.id@
	                    AND @FILTER@
	                    AND sr.ID = pr.PRID
                        AND sr.IS_HIDDEN = 0
	                    @BROWSE-ONLY: AND sr.is_active = 1:BROWSE-ONLY@]]></nsql>
