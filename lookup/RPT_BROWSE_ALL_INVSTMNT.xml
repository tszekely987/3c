          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="RPT_BROWSE_ALL_INVSTMNT" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Investment browse" languageCode="en" name="Investment browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[SELECT @SELECT:INV.ID:ID@
                  ,@SELECT:INV.CODE:CODE@
                  ,@SELECT:INV.NAME:NAME@
                  ,@SELECT:INV.ODF_OBJECT_CODE:OBJECT_CODE@
                  ,@SELECT:INV.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
                FROM INV_INVESTMENTS INV LEFT OUTER JOIN INV_PROJECTS PRJ ON INV.ID = PRJ.PRID
                WHERE
                  INV.IS_CUSTOM=0
                  AND
                  @WHERE:SECURITY:PROJECT:INV.ID@
                   AND ISNULL(PRJ.IS_TEMPLATE,0)=0
                   AND (INV.PURGE_FLAG=0 OR INV.PURGE_FLAG IS NULL)
                   AND
                @FILTER@]]></nsql>
            <nsql databaseId="Niku" dbVendor="oracle"><![CDATA[SELECT @SELECT:INV.ID:ID@
                  ,@SELECT:INV.CODE:CODE@
                  ,@SELECT:INV.NAME:NAME@
                  ,@SELECT:INV.ODF_OBJECT_CODE:OBJECT_CODE@
                  ,@SELECT:INV.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
                FROM INV_INVESTMENTS INV, INV_PROJECTS PRJ
                WHERE
                INV.IS_CUSTOM=0
                AND     				
                @WHERE:SECURITY:PROJECT:INV.ID@
                   AND INV.ID=PRJ.PRID(+)
                   AND NVL(PRJ.IS_TEMPLATE,0)=0
                   AND (INV.PURGE_FLAG=0 OR INV.PURGE_FLAG IS NULL)
                   AND
                @FILTER@]]></nsql>
            <nsql databaseId="Niku" dbVendor="postgres"><![CDATA[SELECT @SELECT:INV.ID:ID@
                  ,@SELECT:INV.CODE:CODE@
                  ,@SELECT:INV.NAME:NAME@
                  ,@SELECT:INV.ODF_OBJECT_CODE:OBJECT_CODE@
                  ,@SELECT:INV.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
                FROM INV_INVESTMENTS INV LEFT OUTER JOIN INV_PROJECTS PRJ ON INV.ID = PRJ.PRID
                WHERE
                  INV.IS_CUSTOM=0
                  AND
                  @WHERE:SECURITY:PROJECT:INV.ID@
                   AND COALESCE(PRJ.IS_TEMPLATE,0)=0
                   AND (INV.PURGE_FLAG=0 OR INV.PURGE_FLAG IS NULL)
                   AND
                @FILTER@]]></nsql>
