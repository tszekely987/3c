      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="BROWSE_IMM_CATEGORY" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="description" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Browse Category" languageCode="en" name="Browse Category"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:p.id:id@,
                       @SELECT:p.code:code@,
                       @SELECT:p.code:UNIQUE_CODE@,
                       @SELECT:p.name:name@,
                       @SELECT:p.description:description@,
                       @SELECT:p.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
        FROM imm_categories p 
        Where @FILTER@
        @BROWSE-ONLY:
        AND exists
          (SELECT 1
            FROM     cmn_sec_assgnd_obj_perm_v a, CMN_SEC_objects so
            WHERE    a.OBJECT_ID = so.id
                    AND so.object_code='INCIDENT_CATEGORY'
                    AND a.permission_code = 'READ'
                    AND a.object_instance_id = p.ID
                    AND a.user_id = @WHERE:PARAM:USER_ID@
           UNION ALL
           SELECT 1
            FROM    cmn_sec_assgnd_obj_perm_r_v a, CMN_SEC_objects so
            WHERE   a.OBJECT_ID = so.id
                    AND so.object_code='INCIDENT_CATEGORY'
                    AND a.permission_code = 'READ'
                    AND a.user_id = @WHERE:PARAM:USER_ID@
                    )
            :BROWSE-ONLY@]]></nsql>
