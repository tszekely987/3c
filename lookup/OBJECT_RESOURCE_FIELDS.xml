<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="OBJECT_RESOURCE_FIELDS" displayAttributeName="name" hiddenAttributeName="internal_name" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Resource Fields for Object" languageCode="en" name="Resource Fields for Object"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT	@SELECT:oca.internal_name:internal_name@,
	@SELECT:ccn.name:name@
from odf_custom_attributes oca 
join cmn_list_of_values lov on oca.lookup_type = lov.lookup_type_code
join cmn_captions_nls ccn on oca.id = ccn.pk_id 
WHERE @FILTER@
and lov.object_code= 'resource' and data_type = 'number' and ccn.table_name = 'ODF_CUSTOM_ATTRIBUTES' and ccn.language_code = @WHERE:PARAM:LANGUAGE@ 
and object_name in (select code from odf_objects where code = @WHERE:PARAM:USER_DEF:string:OBJECT_CODE@
union 
select 
case when template_extension = 'inv_extension' then 'inv' 
when ooe.extension_code = 'inv' then 'inv'
else null 
end 
from odf_objects oo
join odf_object_extensions ooe on oo.code = ooe.object_code where code = @WHERE:PARAM:USER_DEF:string:OBJECT_CODE@)]]></nsql>
