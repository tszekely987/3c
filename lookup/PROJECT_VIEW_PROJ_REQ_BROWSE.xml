      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="false" autoSuggestMaxSuggestions="10" code="PROJECT_VIEW_PROJ_REQ_BROWSE" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Browse for investments that user has Investment - View Requisitions Right to." languageCode="en" name="Project View Requisitions Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT @SELECT:P.ID:ID@,
                @SELECT:P.NAME:NAME@,
                @SELECT:P.UNIQUE_NAME:UNIQUE_NAME@,
                @SELECT:P.UNIQUE_NAME:UNIQUE_CODE@,                
                @SELECT:P.DESCRIPTION:DESCRIPTION@,
                @SELECT:P.TYPE:TYPE@,                
                @SELECT:P.TYPE_NAME:TYPE_NAME@,              
                @SELECT:P.LANGUAGE_ID:LANGUAGE_ID@,                
                @SELECT:P.LANGUAGE_CODE:LANGUAGE_CODE@,              
                @SELECT:P.LAST_UPDATED_DATE:LAST_UPDATED_DATE@                
FROM (SELECT INVESTMENTS.ID ID
             , INVESTMENTS.NAME AS NAME
             , INVESTMENTS.CODE UNIQUE_NAME
             , INVESTMENTS.DESCRIPTION DESCRIPTION
             , CA.ODF_OBJECT_CODE AS TYPE
             , CLS.NAME TYPE_NAME
             , LANG.ID LANGUAGE_ID
             , LANG.LANGUAGE_CODE
             , INVESTMENTS.LAST_UPDATED_DATE
      FROM  INV_INVESTMENTS INVESTMENTS, INV_PROJECTS PR
            , CMN_SEC_ASSGND_OBJ_PERM_V APV, ODF_CA_INV CA, ODF_CLASS_V CLS, CMN_LANGUAGES LANG
      WHERE INVESTMENTS.ID = APV.OBJECT_INSTANCE_ID
      AND   APV.COMPONENT_CODE = 'REQ'
      AND   APV.OBJECT_CODE = 'REQUISITION'
      AND   APV.PERMISSION_CODE = 'READ'
      AND   APV.OBJECT_TYPE = 'RECORD'
      AND   APV.OBJECT_ID=50740
      AND   INVESTMENTS.IS_ACTIVE != 0
      AND   PR.PRID=INVESTMENTS.ID
      AND   PR.IS_TEMPLATE = 0
      AND   PR.IS_PROGRAM = 0
      AND   PR.PRID = CA.ID
      AND   CLS.CODE = CA.ODF_OBJECT_CODE
      AND   CLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@      
      AND   CLS.LANGUAGE_CODE = LANG.LANGUAGE_CODE      
      AND   APV.USER_ID = @WHERE:PARAM:USER_ID@
      UNION
      SELECT INVESTMENTS.ID ID
             , INVESTMENTS.NAME AS NAME
             , INVESTMENTS.CODE UNIQUE_NAME
             , INVESTMENTS.DESCRIPTION DESCRIPTION
             , CA.ODF_OBJECT_CODE AS TYPE
             , CLS.NAME TYPE_NAME
             , LANG.ID LANGUAGE_ID
             , LANG.LANGUAGE_CODE
             , INVESTMENTS.LAST_UPDATED_DATE
      FROM  INV_INVESTMENTS INVESTMENTS, INV_PROJECTS PR, ODF_CA_INV CA, ODF_CLASS_V CLS, CMN_LANGUAGES LANG
      WHERE INVESTMENTS.IS_ACTIVE != 0
      AND PR.PRID=INVESTMENTS.ID
      AND PR.IS_TEMPLATE = 0
      AND PR.IS_PROGRAM = 0
      AND PR.PRID = CA.ID
      AND CLS.CODE = CA.ODF_OBJECT_CODE
      AND CLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
      AND   CLS.LANGUAGE_CODE = LANG.LANGUAGE_CODE) P
   WHERE  @FILTER@]]></nsql>
