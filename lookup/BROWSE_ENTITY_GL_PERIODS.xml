            <nls description="Browse Entity GL Periods. This browse could be used for financially enabled resource to restrict available GL Periods based on resource financial properties." languageCode="en" name="Browse Entity GL Periods"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[SELECT @SELECT:PER.ID:ID@,
        @SELECT:NLS.NAME:PeriodName@, 
        @SELECT:PER.START_DATE:startDate@, 
        @SELECT:PER.END_DATE-1:endDate@, 
        @SELECT:PER.PERIOD_TYPE:periodType@, 
        @SELECT:E.ENTITY:entityCode@,
        @SELECT:LANG.LANGUAGE_CODE:LANGUAGE_CODE@,
        @SELECT:LANG.ID:LANGUAGE_ID@,
        @SELECT:PER.LAST_UPDATED_DATE:LAST_UPDATED_DATE@ 
      FROM BIZ_COM_PERIODS PER,ENTITY E, CMN_CAPTIONS_NLS NLS, CMN_LANGUAGES LANG
      WHERE PER.ENTITY_ID = E.ID AND PER.ID = NLS.PK_ID AND NLS.TABLE_NAME='BIZ_COM_PERIODS' AND NLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@ AND
      NLS.LANGUAGE_CODE = LANG.LANGUAGE_CODE AND
      ( null IS NULL OR PER.ENTITY_ID IN (SELECT ID FROM ENTITY WHERE ID = @where:param:user_def:string:entity_id@))
        AND @FILTER@]]></nsql>
