		 @SELECT:c.last_updated_date:last_updated_date@,
		 @SELECT:lang.id:language_id@,
		 @SELECT:lang.language_code:language_code@
FROM     odf_objects o, 
         cmn_captions_nls c,
         cmn_languages lang
WHERE    c.pk_id = o.ID
AND      c.table_name = 'ODF_OBJECTS'
AND      o.is_api_enabled = 1
AND      o.is_fls_enabled = 1
AND      NOT EXISTS ( select cf.id from cmn_features cf, cmn_feature_artifacts cfa where cfa.feature_id = cf.id and cf.enabled = 0 and cfa.code = o.code and cfa.type = 'FIELD_LEVEL_SECURITY' )
AND      c.language_code = @WHERE:PARAM:LANGUAGE@
AND      lang.language_code = c.language_code
AND      @FILTER@]]></nsql>
