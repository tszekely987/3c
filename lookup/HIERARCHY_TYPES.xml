      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="HIERARCHY_TYPES" displayAttributeName="type_name" hiddenAttributeName="code" objectCode="" sortAttributeName="type_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Different types of hierarchies." languageCode="en" name="Hierarchy Types"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT 
                  @SELECT:OO.CODE:CODE@
                  ,@SELECT:CLS.NAME:TYPE_NAME@
         		  ,@SELECT:CLS.LANGUAGE_CODE:LANGUAGE_CODE@

		        FROM ODF_OBJECT_CAPABILITIES OOC
		        INNER JOIN ODF_OBJECTS OO ON  OOC.OBJECT_CODE = OO.CODE AND OOC.CAPABILITY_CODE ='HIERARCHY_ENABLED'
		        INNER JOIN  ODF_CLASS_V CLS ON OO.CODE = CLS.OBJECT_CODE 
		        INNER JOIN CMN_LANGUAGES LANG ON CLS.LANGUAGE_CODE = LANG.LANGUAGE_CODE
	            WHERE
				( ( OO.IS_CUSTOM = 1 AND  OO.IS_SYSTEM=0) OR  (OO.CODE = 'inv' ) )
           		AND OO.EXTENSIONS IS NULL 
           		AND OO.API_ALIAS  IS NOT NULL
 		           AND CLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
		           AND
                @FILTER@                                         
                @BROWSE-ONLY:AND ooc.enabled=1:BROWSE-ONLY@]]></nsql>
