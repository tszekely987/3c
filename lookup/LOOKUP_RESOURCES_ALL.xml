          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_RESOURCES_ALL" displayAttributeName="full_name" hiddenAttributeName="id" objectCode="resource" sortAttributeName="last_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="All Resources browse dynamic lookup" languageCode="en" name="All Resources Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:SRM_RESOURCES.ID:ID@,
	                    @SELECT:SRM_RESOURCES.LAST_NAME:LAST_NAME@,
	                    @SELECT:SRM_RESOURCES.FIRST_NAME:FIRST_NAME@,
	                    @SELECT:SRM_RESOURCES.FULL_NAME:FULL_NAME@,
	                    @SELECT:SRM_RESOURCES.UNIQUE_NAME:UNIQUE_NAME@,
	                    @SELECT:SRM_RESOURCES.UNIQUE_NAME:UNIQUE_CODE@,
	                    @SELECT:PRJ_RESOURCES.PRPRIMARYROLEID:PRPRIMARYROLEID@,
	                    @SELECT:ROLES.UNIQUE_NAME:ROLE_UNIQUE_NAME@,
	                    @SELECT:ROLES.FULL_NAME:ROLE_NAME@,
						@SELECT:SRM_RESOURCES.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
	                    FROM SRM_RESOURCES, PRJ_RESOURCES
	                       LEFT OUTER JOIN SRM_RESOURCES ROLES ON ROLES.ID = PRJ_RESOURCES.PRPRIMARYROLEID
	                    WHERE
	                    @WHERE:SECURITY:RESOURCE:SRM_RESOURCES.ID@  AND
	                    @FILTER@               AND SRM_RESOURCES.ID=PRJ_RESOURCES.PRID
                          AND SRM_RESOURCES.IS_HIDDEN = 0
	                    @BROWSE-ONLY: AND SRM_RESOURCES.IS_ACTIVE=1:BROWSE-ONLY@]]></nsql>
