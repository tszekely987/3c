            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[SELECT  @SELECT:CA.name + ' (' + CA.clarity_object + ')' :clarity_attribute_name@,
        @SELECT:CA.code:clarity_attribute_code@
        FROM INT_CLARITYATTRIBUTES  CA
        WHERE @FILTER@
        @BROWSE-ONLY:
        AND CA.is_active = 1
        AND CA.extended_type = 'lookup'
        AND CA.lookup_code NOT IN ('RALLY_MILESTONES', 'RALLY_PROJECTS', 'RALLY_INVESTMENT_CATEGORIES', 'RALLY_STATES', 'RALLY_OBJECTIVES_STATES', 'RALLY_OBJECTIVES_PROJECTS')
        AND (CA.clarity_data_type != 'number' or (CA.clarity_data_type = 'number' and 
           CA.lookup_code IN (SELECT CLO.lookup_type_code from CMN_LOV_OBJECTS CLO WHERE CLO.key_attribute_id = 6 )))
        :BROWSE-ONLY@]]></nsql>
