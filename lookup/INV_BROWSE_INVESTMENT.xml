<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="INV_BROWSE_INVESTMENT" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Investment browse" languageCode="en" name="Investment browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:INV.ID:ID@
          ,@SELECT:INV.CODE:CODE@
          ,@SELECT:INV.NAME:NAME@            
          ,@SELECT:CA.ODF_OBJECT_CODE:OBJECT_CODE@
          ,@SELECT:CLS.NAME:TYPE_NAME@
          ,@SELECT:CA.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
          ,@SELECT:CLS.LANGUAGE_CODE:LANGUAGE_CODE@
          ,@SELECT:LANG.ID:LANGUAGE_ID@
          FROM INV_INVESTMENTS INV JOIN ODF_CA_INV CA ON CA.ID = INV.ID
          JOIN ODF_CLASS_V CLS ON CLS.CODE = CA.ODF_OBJECT_CODE
          JOIN CMN_LANGUAGES LANG ON CLS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
              AND CLS.LANGUAGE_CODE = LANG.LANGUAGE_CODE
          LEFT OUTER JOIN INV_PROJECTS PRJ ON INV.ID = PRJ.PRID
          WHERE INV.IS_CUSTOM=0
          @BROWSE-ONLY: AND (@WHERE:SECURITY:INV:INV.ID@ OR EXISTS (
            select  permission_value
                      from    cmn_sec_assgnd_obj_perm_v
                      where   user_id = @where:param:user_id@
                      and     permission_code = 'prProjectViewer'
                      and     object_instance_id = inv.id
                      and     object_code = 'PRJ_PROJECT'
                      and     object_type = 'RECORD'
                      and     component_code = 'PRJ')
            )
           AND @NVL@(INV.PURGE_FLAG, 0)=0
           AND @NVL@(PRJ.IS_TEMPLATE,0)=0 
           AND INV.is_active=1 :BROWSE-ONLY@
           AND @FILTER@]]></nsql>
