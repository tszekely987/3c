<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_PARAM_LOC_PKID" displayAttributeName="shortdesc" hiddenAttributeName="id" objectCode="" sortAttributeName="locationid" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Location Browse" languageCode="en" name="Location Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT
		       @SELECT:RES.ID:ID@,
		       @SELECT:RES.locationid:LOCATIONID@,
		       @SELECT:RES.locationid:unique_code@,
		       @SELECT:RES.shortdesc:SHORTDESC@,
		       @SELECT:RES.entity:ENTITY@
		  FROM
       (SELECT 0 ID,'*' locationid,
       '*' shortdesc,'*' entity
        FROM dual 
        UNION
        SELECT locations.ID ID,locations.LOCATIONID locationid,
        locations.SHORTDESC shortdesc, locations.entity entity
        FROM locations WHERE 1=1 @BROWSE-ONLY: AND @WHERE:SECURITY:LOCATION:LOCATIONS.ID@:BROWSE-ONLY@) RES
		  WHERE @FILTER@]]></nsql>
