    @SELECT:r.last_name:last_name@,
    @SELECT:r.full_name:full_name@,
	  @SELECT:r.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
FROM  srm_resources r,
      cmn_sec_users u,
      cmn_lookups s
WHERE  u.id = r.user_id
AND   @FILTER@ @BROWSE-ONLY: AND @WHERE:SECURITY:RESOURCE:r.ID@:BROWSE-ONLY@
AND   u.user_status_id = s.id
AND   u.IS_HIDDEN = 0
AND   r.IS_HIDDEN = 0
AND   s.lookup_type = 'SEC_USER_STATUS'
AND   s.lookup_code IN ('ACTIVE','LOCK')]]></nsql>
