            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT 	@SELECT:pr.prid:prid@,
	@SELECT:pr.prexternalid:prexternalid@,
	@SELECT:pr.prname:PRNAME@,
	@SELECT:pr.cost_type:cost_type@
	FROM prtask pr, pac_mnt_projects inv
	WHERE @FILTER@
	AND pr.PRPROJECTID=inv.ID
	@BROWSE-ONLY:
	AND @WHERE:PARAM:USER_DEF:STRING:INVESTMENT_CODE@ = inv.project_code
	AND inv.status='O'
    AND pr.pristask = 1
    AND pr.prismilestone = 0
	AND NOT EXISTS (
		   SELECT 1 from prtask a 
		   WHERE a.PRPROJECTID = pr.PRPROJECTID                         
		   AND a.WBS_PARSEQ = pr.PRWBSSEQUENCE )
	:BROWSE-ONLY@]]></nsql>
