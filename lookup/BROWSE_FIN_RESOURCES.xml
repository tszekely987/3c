            <nsql databaseId="Niku" dbVendor="mssql"><![CDATA[SELECT
  @SELECT:P.RESOURCE_CODE:RESOURCE_CODE@,
  @SELECT:(SELECT NAME FROM CMN_LOOKUPS_V WHERE LOOKUP_TYPE = 'RESOURCE_TYPE' AND LANGUAGE_CODE = 'en' AND LOOKUP_CODE = S.RESOURCE_TYPE):RESOURCE_TYPE@,
