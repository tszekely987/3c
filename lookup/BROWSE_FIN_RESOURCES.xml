  @SELECT:S.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
 FROM PAC_MNT_RESOURCES P, SRM_RESOURCES S, PRJ_RESOURCES PR
 WHERE P.ID = S.ID
 AND PR.PRID = S.ID
 AND PR.PRISROLE = 0
 AND S.IS_HIDDEN = 0
 AND @FILTER@]]></nsql>
