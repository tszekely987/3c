          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="FIN_UPDATE_ACTUALS_INVS" displayAttributeName="name" hiddenAttributeName="id" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Investments for Update Financial Actuals Job" languageCode="en" name="Update Actuals Investments"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:inv.ID:ID@
                    ,@SELECT:inv.CODE:CODE@
                    ,@SELECT:inv.NAME:NAME@
                    ,@SELECT:cls.CODE:INVESTMENT_TYPE@
                    ,@SELECT:cls.NAME:INVESTMENT_TYPE_NAME@
                FROM INV_INVESTMENTS inv LEFT OUTER JOIN INV_PROJECTS prj ON (inv.ID = prj.PRID),
                    PAC_MNT_PROJECTS mp, ODF_CLASS_V cls, ODF_OBJECTS obj
                WHERE @WHERE:SECURITY:INV:inv.ID@
                    AND cls.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
                    AND inv.ID = mp.ID
                    AND mp.STATUS = 'O'
                    AND @NVL@(prj.IS_TEMPLATE,0) = 0
                    AND @NVL@(prj.IS_PROGRAM,0) = 0
                    AND cls.CODE = obj.CODE
                    AND mp.ODF_OBJECT_CODE = cls.CODE
                    AND (mp.ODF_OBJECT_CODE IN ('idea', 'project') OR (obj.TEMPLATE_EXTENSION = 'inv_extension'
                            AND obj.IS_CUSTOM = 1))
                    AND @FILTER@]]></nsql>
