      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="INV_GL_PERIODS" displayAttributeName="periodname" hiddenAttributeName="id" objectCode="" sortAttributeName="startdate" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Select GL Period in a investment context" languageCode="en" name="Investment GL periods"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:PER.ID:id@,
       @SELECT:NLS.NAME:periodname@, 
       @SELECT:PER.START_DATE:startdate@, 
       @SELECT:PER.END_DATE:enddate@, 
       @SELECT:LKP.NAME:lkpname@,
       @SELECT:LKP.LOOKUP_CODE:periodtype@,
       @SELECT:LANG.ID:language_id@,
       @SELECT:LANG.LANGUAGE_CODE:language_code@,
       @SELECT:PER.LAST_UPDATED_DATE:last_updated_date@
FROM   BIZ_COM_PERIODS PER
       INNER JOIN ENTITY E ON E.ID = PER.ENTITY_ID
       INNER JOIN (SELECT DISTINCT I.ENTITY_CODE
                   FROM   INV_INVESTMENTS I
                   WHERE  (@WHERE:PARAM:USER_DEF:INTEGER:investment_id@ IS NULL OR I.ID = @WHERE:PARAM:USER_DEF:INTEGER:investment_id@)) INV ON INV.ENTITY_CODE = E.ENTITY
       INNER JOIN CMN_LANGUAGES LANG ON 1 = 1
       INNER JOIN CMN_CAPTIONS_NLS NLS ON  PER.ID = NLS.PK_ID
                                       AND NLS.TABLE_NAME = 'BIZ_COM_PERIODS'
                                       AND NLS.LANGUAGE_CODE = LANG.LANGUAGE_CODE
       INNER JOIN CMN_LOOKUPS_V LKP ON  LKP.LOOKUP_TYPE = 'PERIOD_TYPE'
                                    AND LKP.LOOKUP_CODE = PER.PERIOD_TYPE
                                    AND LKP.LANGUAGE_CODE = LANG.LANGUAGE_CODE
WHERE  LANG.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
AND    @FILTER@]]></nsql>
