      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="IMM_BROWSE_PRJMGR" displayAttributeName="full_name" hiddenAttributeName="id" objectCode="" sortAttributeName="last_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Project Manager browse for Incidents" languageCode="en" name="Project Manager browse for Incidents"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:RESOURCES.USER_ID:ID@,
                @SELECT:RESOURCES.LAST_NAME:LAST_NAME@,
                @SELECT:RESOURCES.FULL_NAME:FULL_NAME@,
                @SELECT:RESOURCES.FIRST_NAME:FIRST_NAME@,
                @SELECT:RESOURCES.UNIQUE_NAME:UNIQUE_NAME@,
                @SELECT:RESOURCES.UNIQUE_NAME:UNIQUE_CODE@,
				@SELECT:RESOURCES.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
                FROM
                @BROWSE-ONLY:
                 (SELECT  a.USER_ID FROM cmn_sec_chk_inst_v0 a, imm_categories c
                  WHERE   a.OBJECT_ID = 50800
                  AND     a.permission_code = 'READ'
                  AND     c.ID = a.object_instance_id
                  AND     a.object_instance_id = @where:param:user_def:integer:category_id@
                  UNION
                  SELECT  a.USER_ID FROM cmn_sec_chk_no_user_inst_v0 a, imm_categories c
                  WHERE   a.OBJECT_ID = 50800
                  AND     a.permission_code = 'READ'
                  AND     c.ID = a.object_instance_id
                  AND     @where:param:user_def:integer:category_id@ IS NULL
                  UNION
                  SELECT  a.USER_ID FROM cmn_sec_assgnd_obj_perm_r_v0 a
                  WHERE   a.OBJECT_ID = 50800
                  AND     a.permission_code = 'READ'
                  ) CAT_USERS,
        :BROWSE-ONLY@
        SRM_RESOURCES RESOURCES
               WHERE @FILTER@
                  @BROWSE-ONLY: AND CAT_USERS.USER_ID = RESOURCES.USER_ID
                  :BROWSE-ONLY@
                    AND RESOURCES.IS_HIDDEN = 0
                    AND RESOURCES.PERSON_TYPE != 0]]></nsql>
