      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_FIN_TRANSCLASSES" displayAttributeName="shortdesc" hiddenAttributeName="id" objectCode="" sortAttributeName="transclass" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Financial Transaction Classes" languageCode="en" name="Financial Transaction Classes"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:tc.id:id@,
              @SELECT:tc.transclass:transclass@,
              @SELECT:tc.shortdesc:shortdesc@,
              @SELECT:tc.TRANSTYPE:trans_type@,              
              @SELECT:nls.NAME:trans_type_caption@,
              @SELECT:lang.id:language_id@,
              @SELECT:lang.language_code:language_code@,
              @SELECT:tc.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
                FROM transclass tc, cmn_lookups_v nls, cmn_languages lang
             WHERE @FILTER@
             AND (tc.built_in = 0 OR tc.built_in IS NULL)
             AND NLS.language_code = @WHERE:PARAM:LANGUAGE@
             AND NLS.lookup_type = 'PAC_TRANSACTION_TYPE'
             AND NLS.lookup_code = tc.TRANSTYPE           AND NLS.language_code = lang.language_code
@BROWSE-ONLY:
             AND (@WHERE:PARAM:USER_DEF:INTEGER:TXN_HEADER_ID_CONSTRAIN@ IS NULL OR tc.transtype in (
                   select (case
                   when VALUE='LABOR' then 'L'
                   when VALUE='EQUIPMENT' then 'Q'
                   when VALUE='MATERIAL' then 'M'
                   when VALUE='EXPENSE' then 'X'
                   END) TRANS_TYPE from ODF_MULTI_VALUED_LOOKUPS where object='transactionheader'
                   and attribute='txn_type' and pk_id = @WHERE:PARAM:USER_DEF:INTEGER:TXN_HEADER_ID_CONSTRAIN@ ))
             AND (@WHERE:PARAM:USER_DEF:INTEGER:RESOURCE_ID_CONSTRAIN@ IS NULL OR tc.TRANSTYPE =
     (SELECT CASE
	 WHEN RES.RESOURCE_TYPE=0 THEN 'L'
	 WHEN RES.RESOURCE_TYPE=1 THEN 'Q'
	 WHEN RES.RESOURCE_TYPE=2 THEN 'M'
 	 WHEN RES.RESOURCE_TYPE=3 THEN 'X'
	 END
	 FROM SRM_RESOURCES RES
	 WHERE RES.id=@WHERE:PARAM:USER_DEF:INTEGER:RESOURCE_ID_CONSTRAIN@))
:BROWSE-ONLY@]]></nsql>
