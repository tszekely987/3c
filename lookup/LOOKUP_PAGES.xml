              </multiSelectPageTitles>
              <filter code="page_code" defaultValue="" order="3" widgetType="text">
                
                
                
                
                <nls description="ID of the Page" languageCode="en" name="Page ID"/>
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
              </filter>
              <filter code="page_name" defaultValue="" order="2" widgetType="text">
                
                
                
                
                <nls description="Name of the Page" languageCode="en" name="Page Name"/>
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
              </filter>
              <listColumn code="id" order="3" widgetType="hidden">
                
                
                
                
                <nls description="PK ID of the Page" languageCode="en" name="Id"/>
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
              </listColumn>
              <listColumn code="page_code" order="2" widgetType="text">
                
                
                
                
                <nls description="ID of the Page" languageCode="en" name="Page ID"/>
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
              </listColumn>
              <listColumn code="page_name" order="1" widgetType="text">
                
                
                
                
                <nls description="Name of the Page" languageCode="en" name="Page Name"/>
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
                
