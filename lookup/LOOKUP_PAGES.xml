<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_PAGES" displayAttributeName="page_name" hiddenAttributeName="id" objectCode="page" sortAttributeName="page_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Page browse" languageCode="en" name="Page browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:pages.id:id@,
              @SELECT:pages.page_code:page_code@,
              @SELECT:nls.name:page_name@,
						  @SELECT:nls.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
              @SELECT:l.ID:LANGUAGE_ID@,
              @SELECT:nls.LANGUAGE_CODE:LANGUAGE_CODE@
              FROM cmn_pages pages, cmn_captions_nls nls, CMN_LANGUAGES l
              WHERE nls.pk_id = pages.id
              AND nls.table_name = 'CMN_PAGES'
              AND pages.page_type_code NOT IN ('tab', 'subtab', 'template')
              AND	pages.principal_type = 'SYSTEM'
              AND nls.language_code = @WHERE:PARAM:LANGUAGE@
					    AND nls.LANGUAGE_CODE = l.LANGUAGE_CODE
              AND @FILTER@]]></nsql>
            <displayedSuggestionAttributes>
              <displayedSuggestionAttribute value="page_name"/>
              <displayedSuggestionAttribute value="page_code"/>
            </displayedSuggestionAttributes>
            <searchedSuggestionAttributes>
              <searchedSuggestionAttribute value="page_name"/>
              <searchedSuggestionAttribute value="page_code"/>
            </searchedSuggestionAttributes>
