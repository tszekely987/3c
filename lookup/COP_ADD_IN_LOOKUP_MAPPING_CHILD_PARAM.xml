<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="COP_ADD_IN_LOOKUP_MAPPING_CHILD_PARAM" displayAttributeName="lookup_name" hiddenAttributeName="lookup_code" objectCode="" sortAttributeName="lookup_sort" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="This is a parameterized lookup to display child lookup values for the mapping type selected in the Add-In Lookup Mapping object." languageCode="en" name="Add-In Lookup Mapping - Child Lookup"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:MAP.LOOKUP_CODE:LOOKUP_CODE@,
@SELECT:MAP.LOOKUP_NAME:LOOKUP_NAME@,
@SELECT:MAP.LOOKUP_SORT:LOOKUP_SORT@
FROM  (SELECT l.lookup_code lookup_code, l.name lookup_name, l.sort_order lookup_sort
       FROM   cmn_lookups_v l
       WHERE  l.language_code = @WHERE:PARAM:LANGUAGE@ 
       AND    l.is_active = 1
       AND   (l.lookup_type = 'APM_SW_SUBCATEGORY' AND 
              (@WHERE:PARAM:USER_DEF:STRING:MAPPING_TYPE_CONSTRAIN@ IS NULL OR
               @WHERE:PARAM:USER_DEF:STRING:MAPPING_TYPE_CONSTRAIN@ = 'apm100'))
) MAP
WHERE  @FILTER@]]></nsql>
            <displayedSuggestionAttributes>
              <displayedSuggestionAttribute value="lookup_name"/>
            </displayedSuggestionAttributes>
            <searchedSuggestionAttributes>
              <searchedSuggestionAttribute value="lookup_name"/>
            </searchedSuggestionAttributes>
