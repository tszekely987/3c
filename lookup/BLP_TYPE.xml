<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="BLP_TYPE" displayAttributeName="name" hiddenAttributeName="type" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="List of workspaces enabled for blueprint configuration in the new user experience" languageCode="en" name="Blueprint Type"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:C.NAME:name@,@SELECT:@UPPER@(O.CODE):type@
                    FROM CMN_CAPTIONS_NLS C, ODF_OBJECTS O
                    WHERE C.TABLE_NAME = 'ODF_OBJECTS'
                    AND C.PK_ID = O.ID
                    AND IS_API_ENABLED = 1
                    AND IS_ABSTRACT = 0
                    AND @WHERE:PARAM:LANGUAGE@ = C.LANGUAGE_CODE
                    AND @UPPER@(O.CODE) IN (SELECT B.TYPE FROM ODF_BLUEPRINTS B
                        WHERE B.IS_PRIVATE = 0
                        AND (@WHERE:PARAM:USER_DEF:INTEGER:BLUEPRINT_LEVEL@ = B.BLUEPRINT_LEVEL
                             or ( 1 =  B.BLUEPRINT_LEVEL and @WHERE:PARAM:USER_DEF:INTEGER:BLUEPRINT_LEVEL@ is null))
                        )
                    AND @FILTER@]]></nsql>
            <displayedSuggestionAttributes>
              <displayedSuggestionAttribute value="name"/>
            </displayedSuggestionAttributes>
