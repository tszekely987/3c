<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_TEAM_QUICK_ADD" displayAttributeName="full_name" hiddenAttributeName="id" objectCode="resource" sortAttributeName="full_name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="A list of active defined Teams for which the user has access." languageCode="en" name="Active Teams Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:RESOURCES.ID:ID@,
				@SELECT:RESOURCES.FULL_NAME:FULL_NAME@,            
                @SELECT:RESOURCES.UNIQUE_NAME:UNIQUE_NAME@
                FROM (
                      SELECT SRM_RESOURCES.ID ID,
					  	SRM_RESOURCES.FULL_NAME,          
                        SRM_RESOURCES.UNIQUE_NAME,
                        SRM_RESOURCES.IS_HIDDEN,
                        SRM_RESOURCES.IS_ACTIVE
                        FROM SRM_RESOURCES JOIN PRJ_RESOURCES ON SRM_RESOURCES.ID = PRJ_RESOURCES.PRID
                        WHERE PRJ_RESOURCES.PRISROLE = 0
						AND SRM_RESOURCES.DEFINED_TEAM_ID IS NOT NULL
                 ) RESOURCES
                WHERE @WHERE:SECURITY:RESOURCE:RESOURCES.ID@               
                AND @FILTER@
                @BROWSE-ONLY:
                 AND     RESOURCES.IS_HIDDEN = 0
                 AND     RESOURCES.IS_ACTIVE = 1
               :BROWSE-ONLY@]]></nsql>
            <displayedSuggestionAttributes>
              <displayedSuggestionAttribute value="full_name"/>
              <displayedSuggestionAttribute value="unique_name"/>
            </displayedSuggestionAttributes>
            <searchedSuggestionAttributes>
              <searchedSuggestionAttribute value="full_name"/>
              <searchedSuggestionAttribute value="unique_name"/>
            </searchedSuggestionAttributes>
