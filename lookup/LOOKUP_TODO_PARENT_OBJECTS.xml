          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_TODO_PARENT_OBJECTS" displayAttributeName="name" hiddenAttributeName="code" objectCode="" sortAttributeName="name" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="The list of objects that have to-dos." languageCode="en" name="To Do Parent Objects"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:oo.code:code@, 
    @SELECT:nls.name:name@,
    @SELECT:oo.api_alias:api_alias@,
    @SELECT:nls.language_code:language_code@
  FROM (
		SELECT o.id, o.code, o.api_alias
		      FROM odf_objects o
		      where exists(
		            select 1 from oba_associations a 
		            where o.api_alias = a.assoc_object_alias
		            AND (@WHERE:PARAM:USER_DEF:STRING:api_alias@ IS NULL OR
		                  @WHERE:PARAM:USER_DEF:STRING:api_alias@ = '' OR 
		                  a.assoc_object_alias = @WHERE:PARAM:USER_DEF:STRING:api_alias@ OR
		                  a.assoc_parent_alias= @WHERE:PARAM:USER_DEF:STRING:api_alias@ )
		      )
  ) oo JOIN cmn_captions_nls nls ON oo.id = nls.pk_id 
    	AND nls.language_code = @WHERE:PARAM:language@
    	AND nls.table_name = 'ODF_OBJECTS'
  WHERE @FILTER@]]></nsql>
