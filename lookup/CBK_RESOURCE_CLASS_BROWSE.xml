<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="CBK_RESOURCE_CLASS_BROWSE" displayAttributeName="description" hiddenAttributeName="id" objectCode="" sortAttributeName="description" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Browse for resource classes" languageCode="en" name="Browse for resource classes"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:w.id:id@,
                 @SELECT:w.resource_class:resource_class@,
                 @SELECT:w.resource_class:unique_code@,
                 @SELECT:w.description:description@,
                 @SELECT:w.active:active@,
        @SELECT:w.RESOURCE_TYPE:resource_type@,                 
        @SELECT:w.last_updated_date:last_updated_date@,                 
        @SELECT:(case
		           when w.RESOURCE_TYPE='L' then 'Labor'
		           when w.RESOURCE_TYPE='Q' then 'Equipment'
		           when w.RESOURCE_TYPE='M' then 'Material'
		           when w.RESOURCE_TYPE='X' then 'Expense'
        END):resource_type_caption@
          FROM pac_fos_resource_class w
          WHERE @FILTER@
                     @BROWSE-ONLY:
                     AND    ACTIVE = 1
                     :BROWSE-ONLY@]]></nsql>
            <displayedSuggestionAttributes>
