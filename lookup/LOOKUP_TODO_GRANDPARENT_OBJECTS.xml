            <nls description="The list of parents of sub-objects that have to-dos." languageCode="en" name="To Do Grandparent Objects"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:oo.code:code@, 
  @SELECT:nls.name:name@,
  @SELECT:oo.api_alias:api_alias@,
  @SELECT:nls.language_code:language_code@
FROM ( 
	SELECT o.id, o.code, o.api_alias FROM oba_associations a JOIN odf_objects o ON o.api_alias = a.assoc_parent_alias
		AND a.assoc_parent_alias is NOT null AND a.assoc_parent_alias != 'investments'
	UNION
	SELECT o.id, o.code, o.api_alias FROM oba_associations a JOIN odf_objects o ON o.api_alias = a.assoc_object_alias
		AND a.assoc_parent_alias IS null AND a.assoc_object_alias = 'checklists'
		
) oo JOIN cmn_captions_nls nls ON oo.id = nls.pk_id AND language_code = @where:param:language@ AND nls.table_name = 'ODF_OBJECTS'
WHERE @FILTER@]]></nsql>
