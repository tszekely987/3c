<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="SCH_BROWSE_DEPT" displayAttributeName="shortdesc" hiddenAttributeName="id" objectCode="department" sortAttributeName="shortdesc" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Department browse" languageCode="en" name="Department browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:D.ID:ID@,
              @SELECT:D.DEPARTCODE:DEPARTCODE@,
              @SELECT:D.DEPARTCODE:UNIQUE_CODE@,
              @SELECT:D.DESCRIPTION:DESCRIPTION@,
              @SELECT:D.SHORTDESC:SHORTDESC@,
              @SELECT:E.ENTITY:ENTITY@,
              @SELECT:D.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
      FROM    DEPARTMENTS D, ENTITY E
      WHERE   @FILTER@ 
          AND (@WHERE:PARAM:USER_DEF:INTEGER:ENTITY_ID_CONSTRAIN@ IS NULL OR
             @WHERE:PARAM:USER_DEF:INTEGER:ENTITY_ID_CONSTRAIN@ = D.ENTITY_ID)
          AND D.ENTITY_ID = E.ID
         @BROWSE-ONLY:
		 AND D.IS_ACTIVE = 1 AND @WHERE:SECURITY:DEPARTMENT:D.ID@
           AND (@WHERE:PARAM:USER_DEF:INTEGER:ENTITY_ID@ IS NULL OR
           @WHERE:PARAM:USER_DEF:INTEGER:ENTITY_ID@ = D.ENTITY_ID)
         :BROWSE-ONLY@]]></nsql>
