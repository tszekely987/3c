              @SELECT:p.transclass:unique_code@,
              @SELECT:p.shortdesc:code@,
              @SELECT:p.description:description@,
              @SELECT:p.TRANSTYPE:trans_type@,
              @SELECT:(case
		           when p.TRANSTYPE='L' then 'Labor'
		           when p.TRANSTYPE='Q' then 'Equipment'
		           when p.TRANSTYPE='M' then 'Material'
		           when p.TRANSTYPE='X' then 'Expenses'
        END):trans_type_caption@,
		      @SELECT:p.last_updated_date:last_updated_date@
              FROM transclass p 
              WHERE @FILTER@
              AND (built_in = 0 OR built_in IS NULL)]]></nsql>
