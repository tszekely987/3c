            <nls description="Report (Fiscal Year)" languageCode="en" name="Report (Fiscal Year)"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:p.id:pid@,
       @SELECT:nls.name:pname@,
       @SELECT:p.period_type:ptype@,
       @SELECT:nls.description:description@,
       @SELECT:l.name:lname@,
       @SELECT:p.start_date:stdate@,
       @SELECT:@DBUSER@.cop_calc_finish_fct(p.end_date):edate@,
       @SELECT:p.last_updated_date:last_updated_date@,
       @SELECT:lang.language_code:language_code@,
       @SELECT:lang.id:language_id@
FROM   biz_com_periods p
       INNER JOIN cmn_lookups_v l ON  l.lookup_code = p.period_type
                                  AND l.lookup_type = 'PERIOD_TYPE'
       INNER JOIN cmn_captions_nls nls ON  p.id = nls.pk_id
                                       AND nls.table_name = 'BIZ_COM_PERIODS' and l.language_code = nls.language_code
       INNER JOIN cmn_languages lang ON nls.language_code =lang.language_code 
WHERE  p.period_type = 'ANNUALLY'
AND    @DBUSER@.cmn_to_char_fct(p.entity_id) = (SELECT entity_key FROM dwh_settings)
AND    nls.language_code = @WHERE:PARAM:LANGUAGE@
AND    l.language_code = @WHERE:PARAM:LANGUAGE@
AND    @FILTER@]]></nsql>
