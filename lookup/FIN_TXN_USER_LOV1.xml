            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT  @SELECT:LOOKUP_CODE:LOOKUP_CODE@, 
	@SELECT:NAME:NAME@ 
FROM (	 SELECT LOOKUP_CODE, NAME, SORT_ORDER
	   FROM   CMN_LOOKUPS_V
	   WHERE  LOOKUP_TYPE = 'PRTIMEENTRY_USER_LOV1'
	   AND    LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
	   AND    IS_ACTIVE = 1
	   AND EXISTS (
	   SELECT 'A' FROM CMN_OPTION_VALUES WHERE OPTION_ID IN (
		 SELECT ID FROM CMN_OPTIONS WHERE OPTION_CODE = 'NKT.GEN.LOOKUP_4_USER_VALUE1')
		 AND USER_ID IS NULL
		 AND VALUE = 'PRTIMEENTRY_USER_LOV1')
	 UNION
	 SELECT LOOKUP_CODE, LOOKUP_CODE AS NAME, 1 SORT_ORDER
	   FROM USER_LOV1_ALL_V
	   WHERE NOT EXISTS (
	   SELECT 'A' FROM CMN_OPTION_VALUES WHERE OPTION_ID IN (
		 SELECT ID FROM CMN_OPTIONS WHERE OPTION_CODE = 'NKT.GEN.LOOKUP_4_USER_VALUE1')
		 AND USER_ID IS NULL
		 AND VALUE = 'PRTIMEENTRY_USER_LOV1')
	   ) LOOKUP_DATA
WHERE @FILTER@]]></nsql>
