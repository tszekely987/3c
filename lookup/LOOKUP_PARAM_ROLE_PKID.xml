      <contentPack update="true">
        <lookups update="true">
          <dynamicLookup autoSuggestEnabled="true" autoSuggestMaxSuggestions="10" code="LOOKUP_PARAM_ROLE_PKID" displayAttributeName="prname" hiddenAttributeName="prid" objectCode="" sortAttributeName="prname" sortDirection="asc" source="niku.com" status="active" update="true">
            
            
            
            
            <nls description="Role Browse" languageCode="en" name="Role Browse"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql databaseId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DATA.PRID:PRID@,
       @SELECT:DATA.UNIQUE_CODE:UNIQUE_CODE@,
       @SELECT:DATA.PRNAME:PRNAME@,
        @SELECT:(case
		           when DATA.RTYPE=0 then 'L'
		           when DATA.RTYPE=1 then 'Q'
		           when DATA.RTYPE=2 then 'M'
		           when DATA.RTYPE=3 then 'X'
		           when DATA.RTYPE=50 then '*'
        END):resource_type@,
        @SELECT:(case
		           when DATA.RTYPE=0 then 'Labor'
		           when DATA.RTYPE=1 then 'Equipment'
		           when DATA.RTYPE=2 then 'Material'
		           when DATA.RTYPE=3 then 'Expense'
		           when DATA.RTYPE=50 then '*'
        END):resource_type_caption@
        FROM
       (SELECT 0 PRID,
       '*' UNIQUE_CODE,
       '*' PRNAME, 
       50 RTYPE,
       1 active FROM PRJ_RESOURCES
       UNION
       SELECT
         PRJ_RES.PRID PRID,
             SRM_RES.UNIQUE_NAME UNIQUE_CODE,
             SRM_RES.FULL_NAME PRNAME,SRM_RES.RESOURCE_TYPE RTYPE,SRM_RES.is_active active
             FROM PRJ_RESOURCES PRJ_RES, SRM_RESOURCES SRM_RES
         WHERE PRJ_RES.PRISROLE != 0
             AND PRJ_RES.PRID = SRM_RES.ID) DATA where @filter@]]></nsql>
