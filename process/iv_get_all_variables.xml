                <Notifications notifyOwner="false">
                  <NotifyWhen stepCompleted="false" stepInError="false" stepStarted="false"/>
                  <Assignees/>
                </Notifications>
                <Operations>
                  <Action code="gs" synchronized="true" type="BPM_SAT_CUSTOM">
                    
                    
                    
                    
                    <nls languageCode="en" name="gs"/>
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    <customScript languageCode="gel">
                      <scriptText>
                        <gel:script xmlns:core="jelly:core" xmlns:gel="jelly:com.niku.union.gel.GELTagLibrary" xmlns:util="jelly:util" xmlns:x="jelly:org.apache.commons.jelly.tags.xml.XMLTagLibrary" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                          <core:set value="${context.getVariables().keySet().toString()}" var="allNames"/>
                          <core:set value="${allNames.substring(1)}" var="allNames"/>
                          <util:replace new="" old="]" var="allNames">${allNames}</util:replace>
                          <core:new className="java.util.TreeMap" var="sortedMap"/>
                          <core:new className="java.util.Hashtable" var="variableClasses"/>
                          <core:forEach items="${allNames}" var="thisVariable">
                            <core:switch on="${thisVariable}">
                              <core:case value="thisVariable"/>
                              <core:case value="context"/>
                              <core:case value="args"/>
                              <core:case value="systemScope">
                                <core:forEach items="${systemScope.propertyNames()}" var="thisProperty">
                                  <core:set value="${systemScope.getProperty(thisProperty)}" var="thisValue"/>
                                  <core:set value="${thisValue.getClass().getName()}" var="thisValueClass"/>
                                  <core:mute>${sortedMap.put(thisProperty, thisValue)}</core:mute>
                                  <core:mute>${variableClasses.put(thisProperty, 'systemScope')}</core:mute>
                                </core:forEach>
                              </core:case>
                              <core:default>
                                <core:set value="${context.getVariable(thisVariable)}" var="thisValue"/>
                                <core:mute>${sortedMap.put(thisVariable, thisValue)}</core:mute>
                                <core:set value="${thisValue.getClass().getName()}" var="thisValueClass"/>
                                <core:mute>${variableClasses.put(thisVariable, thisValueClass)}</core:mute>
                              </core:default>
                            </core:switch>
                          </core:forEach>
                          <core:forEach items="${sortedMap.keySet()}" var="thiskey">
                            <gel:log message="${thiskey} = ${sortedMap.get(thiskey)} (${variableClasses.get(thiskey)})"/>
                          </core:forEach>
                        </gel:script>
                      </scriptText>
                    </customScript>
                    <Notifications notifyOwner="false">
                      <NotifyWhen stepActionInError="false" stepActionPerformed="false" value="0"/>
                      <Assignees/>
                    </Notifications>
                  </Action>
                </Operations>
                <TransitionRestrictions>
