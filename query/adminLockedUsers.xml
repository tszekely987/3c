<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <queries update="true">
          <query code="adminLockedUsers" isUserPortletAvailable="1" source="customer">
            
            
            
            
            <nls description="Query to list all the users locked by the admin lockuser command and users manually locked by admin through UI." languageCode="en" name="Admin Locked Users"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT   @SELECT:DIM:USER_DEF:IMPLIED:RESOURCE:U.USER_NAME:USER_NAME@,
         @SELECT:DIM_PROP:USER_DEF:IMPLIED:RESOURCE:R.FULL_NAME:RESOURCE_NAME@,
         @SELECT:DIM_PROP:USER_DEF:IMPLIED:RESOURCE:U.EMAIL_ADDRESS:EMAIL_ADDRESS@,
         @SELECT:DIM_PROP:USER_DEF:IMPLIED:RESOURCE:U.LAST_LOGGED_IN_DATE:LAST_LOGGED_IN_DATE@,
         @SELECT:DIM_PROP:USER_DEF:IMPLIED:RESOURCE:U.LAST_UPDATED_DATE:LAST_UPDATED_DATE@
FROM     CMN_SEC_USERS U,
         SRM_RESOURCES R
WHERE    U.ID = R.USER_ID
AND      U.ID >= 5000000
AND      U.USER_STATUS_ID = 202
AND      U.LAST_UPDATED_BY = 1
AND      @FILTER@]]></nsql>
            <attribute id="user_name" required="false">
              
              
              
              
              <nls languageCode="en" name="User Name"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="resource_name" required="false">
              
              
              
              
              <nls languageCode="en" name="Resource Name"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="email_address" required="false">
              
              
              
              
              <nls languageCode="en" name="Email Address"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="last_logged_in_date" required="false">
              
              
              
              
              <nls languageCode="en" name="Last Logged in Date"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="last_updated_date" required="false">
              
              
              
              
              <nls languageCode="en" name="Last Updated Date"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
          </query>
        </queries>
      </contentPack>
    </NikuDataBus>