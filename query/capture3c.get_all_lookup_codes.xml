<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <queries update="true">
          <query code="capture3c.get_all_lookup_codes" isUserPortletAvailable="1" source="customer">
            
            
            
            
            <nls languageCode="en" name="capture3c.get_all_lookup_codes"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DIM:USER_DEF:IMPLIED:LOOKUPS:L.LOOKUP_TYPE:CODE@,
@SELECT:DIM_PROP:USER_DEF:IMPLIED:LOOKUPS:L.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
@SELECT:DIM_PROP:USER_DEF:IMPLIED:LOOKUPS:CSU.USER_NAME:LAST_UPDATED_BY@
FROM cmn_lookup_types l
LEFT JOIN cmn_sec_users csu ON csu.id = l.last_updated_by
WHERE l.lookup_type NOT LIKE 'ODF.%'
	AND l.is_admin_visible != 0
	AND l.lookup_type NOT IN ('REQUIREMENT_PRIORITY')
	AND @FILTER@]]></nsql>
            <attribute id="code" required="false"/>
            <attribute id="last_updated_date" required="false"/>
            <attribute id="last_updated_by" required="false"/>
          </query>
        </queries>
      </contentPack>
    </NikuDataBus>