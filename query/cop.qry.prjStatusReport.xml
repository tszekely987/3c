        <queries update="true">
          <query category="PRODUCTIVITY" code="cop.qry.prjStatusReport" isUserPortletAvailable="1" source="csk.niku.com">
            
            
            
            
            <nls description="Status Report Indicators" languageCode="en" name="Status Report Indicators"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT 
       @SELECT:DIM:USER_DEF:IMPLIED:STATUSREPORT:sr.odf_pk:dim_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:sr.odf_pk:sr_int_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:sr.odf_parent_id:project_int_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:odf_proj.name:project_name@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:sr.name:report_name@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:sr.cop_report_update:report_update_desc@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:sr.cop_report_date:report_date@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:sr.cop_key_accomplish:key_accomplish@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:sr.cop_upcoming_act:upcoming_act@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:sr.cop_project_manager:project_manager@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:r.user_id:mgr_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:STATUSREPORT:r.full_name:manager_name@, 
       @SELECT:METRIC:USER_DEF:IMPLIED:sr.COP_OVERALL_STATUS:overall_status@,
       @SELECT:METRIC:USER_DEF:IMPLIED:sr.COP_SCHEDULE_STATUS:schedule_status@,
       @SELECT:METRIC:USER_DEF:IMPLIED:sr.COP_SCOPE_STATUS:scope_status@,
       @SELECT:METRIC:USER_DEF:IMPLIED:sr.COP_COST_EFT_STATUS:cost_effort_status@
FROM   odf_cop_prj_statusrpt_v2 sr
INNER  JOIN  cop_prj_statusrpt_latest_v lsr 
       ON    lsr.investment_id = sr.odf_parent_id
       AND   lsr.report_id = sr.odf_pk 
       AND   lsr.report_order = 1 
INNER  JOIN  odf_project_v2 odf_proj 
       ON    odf_proj.odf_pk = sr.odf_parent_id
       AND   odf_proj.odf_pk = @NVL@(@WHERE:PARAM:XML:INTEGER:/data/id/@value@,sr.odf_parent_id)
LEFT   OUTER JOIN srm_resources r
             ON   sr.cop_project_manager = r.user_id 
LEFT   OUTER JOIN prj_obs_associations assoc 
             ON   sr.odf_parent_id = assoc.record_id 
             AND  @UPPER@(assoc.table_name) = 'SRM_PROJECTS'
LEFT   OUTER JOIN prj_obs_units_flat flat 
             ON   assoc.unit_id = flat.unit_id  
             AND  flat.branch_unit_id = @WHERE:PARAM:USER_DEF:INTEGER:obs@
WHERE  (@WHERE:PARAM:USER_DEF:INTEGER:p_managerid@ IS NULL OR @WHERE:PARAM:USER_DEF:INTEGER:p_managerid@ = sr.cop_project_manager)
AND    (@WHERE:PARAM:USER_DEF:STRING:p_projtype@ IS NULL   OR @WHERE:PARAM:USER_DEF:STRING:p_projtype@ = odf_proj.obj_request_type)
AND    (@WHERE:PARAM:USER_DEF:INTEGER:p_status@ IS NULL    OR @WHERE:PARAM:USER_DEF:INTEGER:p_status@ = CASE WHEN odf_proj.status = 1 THEN 1 WHEN odf_proj.status = 5 THEN 1 ELSE 0 END) 
AND    (@WHERE:PARAM:USER_DEF:INTEGER:obs@ IS NULL         OR @WHERE:PARAM:USER_DEF:INTEGER:obs@ = flat.branch_unit_id)
AND    @WHERE:PARAM:USER_DEF:INTEGER:odf_proj.is_active:active_filter@
AND    @WHERE:PARAM:USER_DEF:INTEGER:odf_proj.is_template:template_filter@
AND    @WHERE:PARAM:USER_DEF:INTEGER:odf_proj.is_program:program_filter@
AND    @WHERE:SECURITY:PROJECT:odf_proj.odf_pk@
AND    @FILTER@]]></nsql>
