<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <queries update="true">
          <query category="RESOURCE" code="rqrmnt.requirementsByRisk" isUserPortletAvailable="1" source="customer">
            
            
            
            
            <nls description="Requirements by Risk Graph Query" languageCode="en" name="Requirements by Risk"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT   @SELECT:DIM:USER_DEF:IMPLIED:RISK:RQP_REQUIREMENTS.RISK:RISK@,
         @SELECT:METRIC:USER_DEF:IMPLIED:COUNT(RISK):COUNTS:COUNTS@
FROM RQP_REQUIREMENTS
WHERE RQP_REQUIREMENTS.RELEASE_ID = @where:param:xml:integer:/data/id/@value@ AND @FILTER@
            GROUP BY RISK HAVING COUNT(RISK) > 0]]></nsql>
            <attribute id="risk" required="false"/>
            <attribute id="counts" required="false"/>
          </query>
        </queries>
      </contentPack>
