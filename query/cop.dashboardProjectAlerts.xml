        <queries update="true">
          <query category="PRODUCTIVITY" code="cop.dashboardProjectAlerts" isUserPortletAvailable="0" source="csk.niku.com">
            
            
            
            
            <nls description="Project Indicators (Instance = Project)" languageCode="en" name="Project Indicators"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DIM:USER_DEF:IMPLIED:PROJECT:odf_proj.odf_pk:project_int_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:PROJECT:odf_proj.name:project_name@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:PROJECT:odf_proj.unique_code:project_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:PROJECT:odf_proj.stage_code:stage_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:PROJECT:stage.name:stage_name@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:PROJECT:odf_proj.obj_alignment:business_alignment@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:PROJECT:odf_proj.risk_score:risk_sl@,
       @SELECT:METRIC:USER_DEF:IMPLIED:inv_d.days_late:schedule@,
       @SELECT:METRIC:USER_DEF:IMPLIED:inv_d.days_late:schedule_sl@,
       @SELECT:METRIC:USER_DEF:IMPLIED:inv_d.days_late_pct:schedule_pct@,
       @SELECT:METRIC:USER_DEF:IMPLIED:inv_d.days_late_pct:schedule_pct_sl@,
       @SELECT:METRIC:USER_DEF:IMPLIED:odf_proj.obj_cost_pct_var:cv_percent_sl@,
       @SELECT:METRIC:USER_DEF:IMPLIED:odf_proj.obj_effort_var:effort_sl@,
       @SELECT:METRIC:USER_DEF:IMPLIED:odf_proj.stage_number:stage_number@,
       @SELECT:METRIC:USER_DEF:IMPLIED:odf_proj.stage_count:stage_count@,
       @SELECT:METRIC:USER_DEF:IMPLIED:issue.issue_sl:issue_sl@,
       @SELECT:METRIC:USER_DEF:IMPLIED:change.change_exists:change_sl@
FROM   ODF_PROJECT_V2 odf_proj
INNER  JOIN  COP_INV_DAYS_LATE_V inv_d
             ON  odf_proj.odf_pk =  inv_d.investment_id
-- Issues --
LEFT   OUTER JOIN cop_inv_rim_stoplights_v issue
             ON   odf_proj.odf_pk = issue.investment_id
-- Change Requests --
LEFT   OUTER JOIN (SELECT c.pk_id project_id,
                          COUNT(c.odf_pk) change_exists
                   FROM   odf_change_v2 c
                   WHERE  c.pk_id = @WHERE:PARAM:XML:INTEGER:/data/id/@value@
                   AND    c.status_code NOT IN ('CLOSED','RESOLVED') 
                   GROUP  BY c.pk_id) change 
             ON   odf_proj.odf_pk = change.project_id 
LEFT   OUTER JOIN  CMN_LOOKUPS_V stage
             ON    stage.lookup_code = odf_proj.stage_code
             AND   stage.lookup_type = 'INV_STAGE_TYPE' 
             AND   stage.language_code =  @WHERE:PARAM:LANGUAGE@
WHERE  odf_proj.odf_pk = @WHERE:PARAM:XML:INTEGER:/data/id/@value@  
AND    @WHERE:SECURITY:PROJECT:odf_proj.odf_pk@
AND    @FILTER@]]></nsql>
