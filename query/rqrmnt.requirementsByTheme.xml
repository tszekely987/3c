<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <queries update="true">
          <query category="RESOURCE" code="rqrmnt.requirementsByTheme" isUserPortletAvailable="1" source="niku.com">
            
            
            
            
            <nls description="Requirements by Theme Graph Query" languageCode="en" name="Requirements by Theme"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT   @SELECT:DIM:USER_DEF:IMPLIED:THEME:CMN_LOOKUPS_v.NAME:THEME@,
         @SELECT:METRIC:USER_DEF:IMPLIED:COUNT(THEME):COUNTS:COUNTS@
FROM RQP_REQUIREMENTS, CMN_LOOKUPS_v 
WHERE RQP_REQUIREMENTS.RELEASE_ID = @where:param:xml:integer:/data/id/@value@
AND RQP_REQUIREMENTS.THEME = CMN_LOOKUPS_v.LOOKUP_CODE
AND CMN_LOOKUPS_v.LANGUAGE_CODE=@WHERE:PARAM:LANGUAGE@
AND @FILTER@
            GROUP BY NAME HAVING COUNT(NAME) > 0]]></nsql>
            <attribute id="theme" required="false"/>
            <attribute id="counts" required="false"/>
          </query>
        </queries>
      </contentPack>
    </NikuDataBus>