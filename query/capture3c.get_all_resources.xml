<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <queries update="true">
          <query code="capture3c.get_all_resources" isUserPortletAvailable="1" source="customer">
            
            
            
            
            <nls languageCode="en" name="capture3c.get_all_resources"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DIM:USER_DEF:IMPLIED:RESOURCES:SR.UNIQUE_NAME:CODE@,
@SELECT:DIM_PROP:USER_DEF:IMPLIED:RESOURCES:SR.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
@SELECT:DIM_PROP:USER_DEF:IMPLIED:RESOURCES:CSU.USER_NAME:LAST_UPDATED_BY@
FROM srm_resources sr
JOIN prj_resources pr ON pr.prid = sr.id
LEFT JOIN cmn_sec_users csu ON csu.id = sr.last_updated_by
WHERE pr.prisrole = 0
	AND sr.unique_name IN ('place_codes_here')
	AND @FILTER@]]></nsql>
            <attribute id="code" required="false"/>
            <attribute id="last_updated_date" required="false"/>
            <attribute id="last_updated_by" required="false"/>
          </query>
        </queries>
      </contentPack>
    </NikuDataBus>