            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DIM:USER_DEF:IMPLIED:project:project.odf_pk:project_int_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:project:project.name:project_name@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:project:@UPPER@(project.investment_type):investment_code@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:project:r.full_name:proj_manager@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:project:project.risk_score:risk_sl@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:project:project.is_approved:approved@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:project:project.obj_alignment:alignment_sl@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:project:project.schedule_start:start_date@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:project:project.baseline_start:base_start_date@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:project:@DBUSER@.cop_calc_finish_time_fct(project.schedule_finish):finish_date@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:project:@DBUSER@.cop_calc_finish_time_fct(project.baseline_finish):base_finish_date@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.actuals,0):act_qty@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.etc,0):etc_qty@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.labor_effort,0):eac_qty@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.baseline_usage,0):base_qty@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.ev_bac,0):baseline_cost@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.ev_etc,0):etc_cost@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.ev_acwp,0) + @NVL@(project.ev_etc,0):eac_cost@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.ev_acwp,0):actual_cost@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.ev_bcws,0):planned_value@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.ev_bcwp,0):earned_value@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.planned_cst_total,0):planned_cost@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.planned_ben_total,0):planned_benefit@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.planned_cst_capital_total,0):planned_cst_capital_total@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.planned_cst_capital_pct,0):planned_cst_capital_pct@,	   
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.planned_cst_operating_total,0):planned_cst_operating_total@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.planned_cst_operating_pct,0):planned_cst_operating_pct@,	      	      
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.obj_effort_amt_var,0):effort_amt@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.obj_effort_var,0):effort_sl@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.obj_cost_amt_var,0):cv_percent@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(project.obj_cost_pct_var,0):cv_percent_sl@,	   
       @SELECT:METRIC:USER_DEF:IMPLIED:(SELECT COUNT(i.odf_pk) high_priority
                                        FROM   odf_issue_v2 i
                                        WHERE  @UPPER@(i.priority_code) = 'HIGH'
                                        AND    i.status_code NOT IN ('CLOSED','RESOLVED')
                                        AND    program.project_id = i.pk_id 
                                        GROUP  BY i.pk_id):issues_high_priority@,         
       @SELECT:METRIC:USER_DEF:IMPLIED:(SELECT COUNT(r.odf_pk) high_priority
                                        FROM   odf_risk_v2 r
				                        WHERE  @UPPER@(r.priority_code) = 'HIGH'
                                        AND    r.status_code NOT IN ('CLOSED','RESOLVED')
                                        AND    program.project_id = r.pk_id 				   
                                        GROUP  BY r.pk_id):risks_high_priority@,
       @SELECT:METRIC:USER_DEF:IMPLIED:(SELECT COUNT(c.odf_pk) high_priority
                                        FROM   odf_change_v2 c
                                        WHERE  @UPPER@(c.priority_code) = 'HIGH'
                                        AND    c.status_code NOT IN ('CLOSED','RESOLVED')
                                        AND    program.project_id = c.pk_id 				   
                                        GROUP  BY c.pk_id):change_high_priority@,
       @SELECT:METRIC:USER_DEF:IMPLIED:inv_d.days_late:days_late@,
       @SELECT:METRIC:USER_DEF:IMPLIED:inv_d.days_late:days_late_sl@,
       @SELECT:METRIC:USER_DEF:IMPLIED:inv_d.days_late_pct:days_late_pct@,
       @SELECT:METRIC:USER_DEF:IMPLIED:inv_d.days_late_pct:days_late_pct_sl@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(fin.budget_cst_total,0):program_budget_cost@
FROM   cop_program_v program
INNER  JOIN  odf_project_v2 project
       ON    program.project_id = project.odf_pk
INNER  JOIN  COP_INV_DAYS_LATE_V inv_d
       ON    project.odf_pk =  inv_d.investment_id
INNER  JOIN  odf_object_instance_mapping map
       ON    program.program_id = map.primary_object_instance_id
       AND   @LOWER@(map.secondary_object_instance_code) = 'financials'
INNER  JOIN  fin_financials fin
             ON   map.secondary_object_instance_id = fin.id
             AND  @LOWER@(fin.ODF_OBJECT_CODE) = 'project'
LEFT   OUTER JOIN srm_resources r
             ON   project.manager = r.user_id    
WHERE  program.program_id = @WHERE:PARAM:XML:INTEGER:/data/id/@value@
AND    program.is_program <> 1  
AND    @WHERE:SECURITY:PROJECT:project.odf_pk@
AND    @FILTER@]]></nsql>
