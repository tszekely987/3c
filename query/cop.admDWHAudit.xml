<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <queries update="true">
          <query category="PRODUCTIVITY" code="cop.admDWHAudit" isUserPortletAvailable="1" source="csk.niku.com">
            
            
            
            
            <nls description="Data Warehouse Audit" languageCode="en" name="Data Warehouse Audit"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Datawarehouse" dbVendor="all"><![CDATA[SELECT @SELECT:DIM:USER_DEF:IMPLIED:DWH:ROW_NUMBER() OVER (ORDER BY a.table_name):row_num@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:DWH:a.table_name:table_name@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:DWH:a.dw_updated_date:dw_updated_date@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:DWH:a.dw_load_start_date:dw_load_start_date@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:DWH:a.dw_load_end_date:dw_load_end_date@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:DWH:(a.dw_load_end_date - a.dw_load_start_date)*24*60*60:load_duration@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:DWH:r.num_rows:table_count@
FROM   dwh_cfg_audit  a
       LEFT OUTER JOIN user_all_tables r ON a.table_name = r.table_name
WHERE  @FILTER@]]></nsql>
            <attribute id="row_num" required="false">
              
              
              
              
              <nls description="Row ID (Internal)" languageCode="en" name="Row ID (Internal)"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="table_name" required="false">
              
              
              
              
              <nls description="Table Name" languageCode="en" name="Table Name"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="dw_updated_date" required="false">
              
              
              
              
              <nls description="Updated Date" languageCode="en" name="Updated Date"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="dw_load_start_date" required="false">
              
              
              
              
              <nls description="Load Start Date" languageCode="en" name="Load Start Date"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="dw_load_end_date" required="false">
              
              
              
              
              
              
              
              
              <nls description="Load End Date" languageCode="en" name="Load End Date"/>
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="load_duration" required="false">
              
              
              
              
              <nls description="Duration (seconds)" languageCode="en" name="Duration (seconds)"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="table_count" required="false">
              
              
              
              
              <nls description="Row Count" languageCode="en" name="Row Count"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
          </query>
        </queries>
      </contentPack>
    </NikuDataBus>