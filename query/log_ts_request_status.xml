<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <queries update="true">
          <query code="log_ts_request_status" isUserPortletAvailable="1" source="niku.com">
            
            
            
            
            <nls languageCode="en" name="Time Slice Request Statuses"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DIM:USER_DEF:IMPLIED:REQUEST:R.ID:ID@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:S.JOB_RUN_ID:JOB_RUN_ID@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:R.REQUEST_NAME:REQUEST@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:STATUS.NAME:STATUS@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:OPERATION.NAME:OPERATION@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:S.DESCRIPTION:DESCRIPTION@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:S.BATCH_NUM:BATCH_NUM@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:R.TABLE_NAME:TABLE_NAME@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:S.CREATED_DATE:CREATED_DATE@
					FROM PRJ_BLB_SLICEREQUESTS R, TEMP_TS_REQUEST_STATUS S
						 LEFT OUTER JOIN CMN_LOOKUPS_V STATUS ON STATUS.LOOKUP_CODE = S.STATUS AND STATUS.LOOKUP_TYPE = 'LOG_TS_REQUEST_STATUS' AND STATUS.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
						 LEFT OUTER JOIN CMN_LOOKUPS_V OPERATION ON OPERATION.LOOKUP_CODE = S.OPERATION AND OPERATION.LOOKUP_TYPE = 'LOG_TS_REQUEST_OPERATION' AND OPERATION.LANGUAGE_CODE = @WHERE:PARAM:LANGUAGE@
					WHERE R.ID = S.SLICE_REQUEST_ID
					AND @FILTER@]]></nsql>
            <attribute id="id" required="false">
              
              
              
              
              <nls languageCode="en" name="Request ID"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="job_run_id" required="false">
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              <nls languageCode="en" name="Job Run Id"/>
              
            </attribute>
            <attribute id="request" lookup="LOG_TS_REQUEST_NAME" required="false">
              
              
              
              
              
              <nls languageCode="en" name="Request Name"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="status" lookup="LOG_TS_REQUEST_STATUS" required="false">
              
              
              
              
              <nls languageCode="en" name="Status"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="operation" lookup="LOG_TS_REQUEST_OPERATION" required="false">
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              <nls languageCode="en" name="Operation"/>
              
            </attribute>
            <attribute id="description" required="false">
              
              
              
              
              <nls languageCode="en" name="Description"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="batch_num" required="false">
              
              
              
              
              <nls languageCode="en" name="Batch Num"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="table_name" lookup="LOG_TS_TABLE_NAME" required="false">
              
              
              
              
              
              
              
              
              
              
              
              <nls languageCode="en" name="Table Name"/>
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="created_date" required="false">
              
              
              
              
              <nls languageCode="en" name="Created Date"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
          </query>
        </queries>
      </contentPack>
    </NikuDataBus>