        <queries update="true">
          <query category="PRODUCTIVITY" code="cop.prjCurrentIssuesLinkable" isUserPortletAvailable="0" source="csk.niku.com">
            
            
            
            
            <nls description="Issues Listing (Instance = Project)" languageCode="en" name="Issues Listing"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT 	DISTINCT 
        @SELECT:DIM:USER_DEF:IMPLIED:ISSUE:i.odf_pk:issue_int_id@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.pk_id:proj_int_id@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.pk_unique_name:proj_id@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.pk_name:proj_name@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:@UPPER@(inv.odf_object_code):investment_type_upper@,		
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.rim_risk_issue_code:issue_id@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.name:issue_name@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.description:description@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.resolution:resolution@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:category.name:category@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:CASE WHEN i.priority_code = 'LOW' THEN 1 
                                                     WHEN i.priority_code = 'MEDIUM' THEN 2 
                                                     WHEN i.priority_code = 'HIGH' THEN 3 ELSE NULL END:priority_sl@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:priority.name:priority@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:status.name:status@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:r.full_name:assigned_to@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:r.email:assigned_email@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.created_by:created_by@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.created_date:created_date@,                     
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.target_resolve_date:target_resolve_date@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.resolved_date:resolved_date@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.updated_by:last_updated_by@,
        @SELECT:DIM_PROP:USER_DEF:IMPLIED:ISSUE:i.last_updated_date:last_updated_date@,
        @SELECT:METRIC:USER_DEF:IMPLIED:@DBUSER@.CMN_DATEDIFF_FCT(@DBUSER@.COP_DATE_TRUNC_FCT(@SYSDATE@), @DBUSER@.COP_DATE_TRUNC_FCT(i.target_resolve_date)):schedule_sl@
FROM    ODF_ISSUE_V2 i
INNER   JOIN  inv_investments inv 
              ON inv.id = i.pk_id                                                     
LEFT    OUTER JOIN SRM_RESOURCES r 
              ON   i.assigned_to = r.user_id
LEFT    OUTER JOIN  CMN_LOOKUPS_V status
              ON    status.lookup_code = i.status_code
              AND   status.lookup_type = 'RIM_STATUS' 
              AND   status.language_code =  @WHERE:PARAM:LANGUAGE@ 
LEFT    OUTER JOIN  CMN_LOOKUPS_V category
              ON    category.lookup_code = i.category_type
              AND   category.lookup_type = 'RIM_CATEGORY_TYPE' 
              AND   category.language_code =  @WHERE:PARAM:LANGUAGE@ 
LEFT    OUTER JOIN  CMN_LOOKUPS_V priority
              ON    priority.lookup_code = i.priority_code
              AND   priority.lookup_type = 'RIM_PRIORITY' 
              AND   priority.language_code =  @WHERE:PARAM:LANGUAGE@
WHERE   i.status_code NOT IN ('CLOSED','RESOLVED') 
AND     i.pk_id = @WHERE:PARAM:XML:INTEGER:/data/id/@value@
AND     i.priority_code = @NVL@(@WHERE:PARAM:XML:STRING:/data/Priority/@value@,i.priority_code)
AND     ((i.category_type IS NULL) OR (@WHERE:PARAM:USER_DEF:STRING:i.category_type:CATEGORY@))
AND     ((i.status_code IS NULL)   OR (@WHERE:PARAM:USER_DEF:STRING:i.status_code:STATUS@))
AND     ((i.assigned_to IS NULL)   OR (@WHERE:PARAM:USER_DEF:INTEGER:i.assigned_to:ASSIGNED_TO@))
AND     @WHERE:SECURITY:PROJECT:i.pk_id@                    
AND     @FILTER@]]></nsql>
