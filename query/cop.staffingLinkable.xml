            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT @select:dim:user_def:implied:assignment:a.prid:assign_internal_id@,
       @select:dim_prop:user_def:implied:assignment:t.prid:task_internal_id@,
       @select:dim_prop:user_def:implied:assignment:t.prprojectid:project_internal_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:assignment:inv.code:project_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:assignment:inv.name:project_name@,
       @select:dim_prop:user_def:implied:assignment:@UPPER@(inv.odf_object_code):investment_type_upper@,
       @select:dim_prop:user_def:implied:assignment:t.prwbssequence:wbs_sequence@,
       @select:dim_prop:user_def:implied:assignment:t.prname:task_name@,
       @select:dim_prop:user_def:implied:assignment:a.prresourceid:role_id@,
       @select:dim_prop:user_def:implied:assignment:r.full_name:role_name@,
       @select:dim_prop:user_def:implied:assignment:a.prstart:assign_start_date@,
       @select:dim_prop:user_def:implied:assignment:@DBUSER@.cop_calc_finish_time_fct(a.prfinish):assign_finish_date@,
       @select:metric:user_def:implied:@NVL@(a.prestsum,0)/3600:etc_hrs@
FROM   prassignment a
INNER  JOIN prtask t
       ON   a.prtaskid = t.prid
       AND  t.pristask <> 0
       AND  t.prismilestone <> 1
       AND  t.prstatus <> 2
INNER  JOIN srm_resources r
       ON   a.prresourceid = r.id
       AND  r.person_type = 0
       AND  r.resource_type = 0     /* Labor Roles Only */
INNER  JOIN inv_investments inv
       ON   inv.id = t.prprojectid
WHERE  t.prprojectid = @WHERE:PARAM:XML:INTEGER:/data/id/@value@
AND    t.prstart 
         BETWEEN (@NVL@(@WHERE:PARAM:USER_DEF:DATE:as_of_date@,@SYSDATE@) - @NVL@(@WHERE:PARAM:USER_DEF:INTEGER:days@,45)) 
             AND (@NVL@(@WHERE:PARAM:USER_DEF:DATE:as_of_date@,@SYSDATE@) + @NVL@(@WHERE:PARAM:USER_DEF:INTEGER:days@,45))
AND    @WHERE:SECURITY:PROJECT:t.prprojectid@
AND    @FILTER@
HAVING @HAVING_FILTER@]]></nsql>
