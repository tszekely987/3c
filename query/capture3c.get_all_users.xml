LEFT JOIN cmn_sec_users lub ON lub.id = csu.last_updated_by
WHERE csu.user_name IN ('place_codes_here')
	AND @FILTER@]]></nsql>
            <attribute id="code" required="false"/>
            <attribute id="last_updated_date" required="false"/>
            <attribute id="last_updated_by" required="false"/>
          </query>
        </queries>
