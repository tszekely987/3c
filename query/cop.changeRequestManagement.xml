        <queries update="true">
          <query category="PRODUCTIVITY" code="cop.changeRequestManagement" isUserPortletAvailable="1" source="csk.niku.com">
            
            
            
            
            <nls description="Change Request Management" languageCode="en" name="Change Request Management"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT 
       @SELECT:DIM:USER_DEF:IMPLIED:CHANGE:c.odf_pk:change_int_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.pk_id:proj_int_id@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.pk_unique_name:proj_id@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.pk_name:proj_name@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:@UPPER@(inv.odf_object_code):investment_code_upper@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:inv.manager_id:proj_mngr_id@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.rim_risk_issue_code:change_id@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.name:change_name@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.description:description@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:category.name:category@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:CASE WHEN c.priority_code = 'LOW' THEN 1 
                                                     WHEN c.priority_code = 'MEDIUM' THEN 2 
                                                     WHEN c.priority_code = 'HIGH' THEN 3 ELSE NULL END:priority_sl@,	   
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:priority.name:priority@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:status.name:status@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:owner.full_name:assigned_to@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:owner.email:assigned_email@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:assessor.full_name:assessor@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:approved.full_name:approved_by@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.exp_closure_date:exp_close_date@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.assessment_date:assessment_date@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.review_date:review_date@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.closure_date:close_date@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.approved_date:approved_date@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.effect_on_schedule:effect_on_schedule@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.effect_on_resources:effect_on_resources@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.reasons:reasons@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.impact_baseline:impact_baseline@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.impact_description:impact_other_proj@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.benefits:benefits@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.created_by:created_by@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.created_date:created_date@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.updated_by:updated_by@, 
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:CHANGE:c.last_updated_date:last_updated_date@
FROM   ODF_CHANGE_V2 c
       INNER  JOIN  INV_INVESTMENTS inv ON  c.pk_id = inv.id 
                                        AND inv.is_active <> 0 
       INNER  JOIN  INV_PROJECTS prj ON  c.pk_id = prj.prid 
       LEFT   OUTER JOIN SRM_RESOURCES owner ON  c.assigned_to = owner.user_id 
       LEFT   OUTER JOIN SRM_RESOURCES assessor ON  c.assessor = assessor.user_id 
       LEFT   OUTER JOIN SRM_RESOURCES approved ON  c.approved_by = approved.user_id 
       LEFT   OUTER JOIN PRJ_OBS_ASSOCIATIONS assoc ON  inv.id = assoc.record_id 
                                                    AND @UPPER@(assoc.table_name) = 'SRM_PROJECTS'
       LEFT   OUTER JOIN PRJ_OBS_UNITS_FLAT flat ON  assoc.unit_id = flat.unit_id
       LEFT   OUTER JOIN CMN_LOOKUPS_V status ON  status.lookup_code = c.status_code
                                              AND status.lookup_type = 'RIM_STATUS' 
                                              AND status.language_code =  @WHERE:PARAM:LANGUAGE@ 
       LEFT   OUTER JOIN CMN_LOOKUPS_V category ON  category.lookup_code = c.category_type
                                                AND category.lookup_type = 'RIM_CATEGORY_TYPE' 
                                                AND category.language_code =  @WHERE:PARAM:LANGUAGE@ 
       LEFT   OUTER JOIN CMN_LOOKUPS_V priority ON  priority.lookup_code = c.priority_code
                                                AND priority.lookup_type = 'RIM_PRIORITY' 
                                                AND priority.language_code =  @WHERE:PARAM:LANGUAGE@  
WHERE  (@WHERE:PARAM:USER_DEF:INTEGER:OBS@ IS NULL OR @WHERE:PARAM:USER_DEF:INTEGER:OBS@ = flat.branch_unit_id)
AND    ((c.category_type IS NULL) OR (@WHERE:PARAM:USER_DEF:STRING:c.category_type:CATEGORY@)) 
AND    ((c.status_code IS NULL)   OR (@WHERE:PARAM:USER_DEF:STRING:c.status_code:STATUS@))
AND    ((c.assigned_to IS NULL)   OR (@WHERE:PARAM:USER_DEF:INTEGER:c.assigned_to:ASSIGNED_TO@))
AND    (prj.is_program = 0 OR @NVL@(@WHERE:PARAM:USER_DEF:INTEGER:show_programs@,1) = 1)
AND    prj.is_template <> 1
AND    @WHERE:SECURITY:PROJECT:inv.id@ 
AND    @FILTER@]]></nsql>
