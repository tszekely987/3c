            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT                                                        
       @select:dim:user_def:implied:project:Project.odf_pk:project_int_id@,
       @select:dim_prop:user_def:implied:project:Project.name:project_name@,
       @select:dim_prop:user_def:implied:project:@UPPER@(Project.odf_object_code):investment_type_upper@,
       @select:dim_prop:user_def:implied:project:r.user_id:proj_manager_int_id@, 
       @select:dim_prop:user_def:implied:project:r.full_name:project_manager@,
       @select:metric:user_def:implied:@NVL@(Project.planned_roi,0)*100:roi@,
       @select:dim_prop:user_def:implied:project:Project.risk_score:risk@,
       @select:dim_prop:user_def:implied:project:stage.name:stage_name@,
       @select:dim_prop:user_def:implied:project:Project.schedule_start:start_date@,
       @select:dim_prop:user_def:implied:project:@DBUSER@.cop_calc_finish_time_fct(Project.schedule_finish):finish_date@,
       @select:metric:user_def:implied:Project.ev_bac:baseline_cost@,
       @select:metric:user_def:implied:@NVL@(Project.ev_acwp,0) + @NVL@(Project.ev_etc,0):eac_cost@,
       @select:metric:user_def:implied:Project.obj_cost_amt_var:cv_percent@, /* Projected Cost Variance Amount */
       @select:metric:user_def:implied:Project.obj_cost_pct_var:cv_percent_sl@, /* Projected Cost Variance Percent */
       @select:metric:user_def:implied:@NVL@(Program.budget_cst_total,0):program_budget_cost@,
       /* NEW ATTRIBUTES V13 */
       @select:metric:user_def:implied:@NVL@(Project.Planned_Irr,0)*100:Planned_Irr@,
       @select:metric:user_def:implied:@NVL@(Project.Planned_mIrr,0)*100:Planned_mIrr@,
       @select:metric:user_def:implied:@NVL@(Project.Planned_Payback_period,0):Planned_Payback_period@,
       @select:metric:user_def:implied:@NVL@(Project.Planned_Cst_Total,0):Planned_Cst_Total@,
       @select:metric:user_def:implied:@NVL@(Project.Planned_Ben_Total,0):Planned_Ben_Total@,
       @select:metric:user_def:implied:@NVL@(Project.planned_cst_capital_total,0):planned_cst_capital_total@,
       @select:metric:user_def:implied:@NVL@(Project.planned_cst_capital_pct,0):planned_cst_capital_pct@,	   
	   @select:metric:user_def:implied:@NVL@(Project.planned_cst_operating_total,0):planned_cst_operating_total@,
       @select:metric:user_def:implied:@NVL@(Project.planned_cst_operating_pct,0):planned_cst_operating_pct@,		   
       @select:metric:user_def:implied:@NVL@(Project.Planned_NPV,0):Planned_NPV@,
       @select:metric:user_def:implied:@DBUSER@.cop_calc_finish_time_fct(Project.Planned_Breakeven):Planned_Breakeven@,
       @select:metric:user_def:implied:@NVL@(Project.etc,0):ETC_QTY@,
       @select:metric:user_def:implied:@NVL@(Project.ev_etc,0):ETC_COST@,
       @select:metric:user_def:implied:Project.baseline_usage:Base_Qty@,
       @select:metric:user_def:implied:@NVL@(Project.actuals,0):Act_Qty@,
       @select:metric:user_def:implied:@NVL@(Project.ev_acwp,0):Act_Cost_ACWP@,
       @select:metric:user_def:implied:@NVL@(Project.labor_effort,0):EAC_QTY@,
       @select:metric:user_def:implied:Project.obj_effort_amt_var:projected_effort_var@, /* Projected Effort Variance Amount */
       @select:metric:user_def:implied:Project.obj_effort_var:projected_effort_var_pct_sl@, /* Projected Effort Variance Percent */
       @select:metric:user_def:implied:@NVL@(Project.ev_BCWS,0):Planned_Value_BCWS@,
       @select:metric:user_def:implied:@NVL@(Project.ev_BCWP,0):Earned_Value_BCWP@,
       @select:metric:user_def:implied:@NVL@(Project.ev_sv,0):Sched_var_SV@,
       @select:metric:user_def:implied:@NVL@(Project.obj_ev_sv_pct,0):Sched_var_pct@,
       @select:metric:user_def:implied:Project.ev_spi:Sched_Perf_SPI@,
       @select:metric:user_def:implied:Project.ev_cv:Cost_var_CV@,
       @select:metric:user_def:implied:Project.obj_ev_cv_pct:Cost_var_pct@,
       @select:metric:user_def:implied:Project.ev_cpi:Cost_perf_CPI@
FROM   cop_program_v v
LEFT   OUTER JOIN odf_project_v2 Program
             ON   Program.odf_pk = v.program_id
LEFT   OUTER JOIN odf_project_v2 Project
             ON   Project.odf_pk = v.project_id
LEFT   OUTER JOIN CMN_LOOKUPS_V stage
             ON   stage.lookup_code = project.stage_code 
             AND  stage.lookup_type = 'INV_STAGE_TYPE' 
             AND  stage.language_code = @WHERE:PARAM:LANGUAGE@
LEFT   OUTER JOIN SRM_RESOURCES r
             ON   r.user_id = Project.Manager_id 
WHERE  1=1
AND    v.is_program <> 1
AND    Program.is_active = 1
AND    Project.is_active = 1
AND    v.Program_id = @WHERE:PARAM:USER_DEF:INTEGER:PROGRAM@
AND    @WHERE:SECURITY:PROJECT:v.Project_id@
AND    @FILTER@]]></nsql>
