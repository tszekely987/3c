            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT  @select:dim:user_def:implied:WIP:wip.wip_transno:wip_transno@,
        @select:dim_prop:user_def:implied:WIP:wip.wip_transdate:wip_transdate@,
        @select:dim_prop:user_def:implied:WIP:inv.id:investment_int_id@,
        @select:dim_prop:user_def:implied:WIP:inv.code:investment_id@,
        @select:dim_prop:user_def:implied:WIP:inv.odf_object_code:inv_object_code@,
        @select:dim_prop:user_def:implied:WIP:@UPPER@(inv.odf_object_code):inv_object_code_upper@,
        @select:dim_prop:user_def:implied:WIP:inv.name:investment_name@,
        @select:dim_prop:user_def:implied:WIP:t.prid:task_int_id@,
        @select:dim_prop:user_def:implied:WIP:t.prexternalid:task_id@,
        @select:dim_prop:user_def:implied:WIP:CASE WHEN t.prexternalid = '~rmw' THEN tname.name ELSE t.prname END:task_name@,
        @select:dim_prop:user_def:implied:WIP:CASE WHEN t.prexternalid = '~rmw' THEN 0 ELSE 1 END:task_disable@,
        @select:dim_prop:user_def:implied:WIP:t.prwbssequence:task_wbs_sequence@,
        @select:dim_prop:user_def:implied:WIP:wip.chargecode_int_id:chargecode_int_id@,
        @select:dim_prop:user_def:implied:WIP:wip.chargecode_name:chargecode_name@,
        @select:dim_prop:user_def:implied:WIP:r.id:resource_int_id@,
        @select:dim_prop:user_def:implied:WIP:r.unique_name:resource_id@,
        @select:dim_prop:user_def:implied:WIP:r.full_name:resource_name@,
        @select:dim_prop:user_def:implied:WIP:wip.transclass_int_id:transclass_int_id@,
        @select:dim_prop:user_def:implied:WIP:wip.transclass_name:transclass_name@,
        @select:dim_prop:user_def:implied:WIP:wip.inputtype_int_id:inputtype_int_id@,
        @select:dim_prop:user_def:implied:WIP:wip.inputtype_name:inputtype_name@,
        @select:dim_prop:user_def:implied:WIP:wip.user_value1_id:user_value1_id@,
        @select:dim_prop:user_def:implied:WIP:userlov1.name:user_value1_name@,
        @select:dim_prop:user_def:implied:WIP:wip.user_value2_id:user_value2_id@,
        @select:dim_prop:user_def:implied:WIP:userlov2.name:user_value2_name@,
        @select:dim_prop:user_def:implied:WIP:wip.notes:notes@,
        @select:dim_prop:user_def:implied:WIP:wip.quantity:quantity@,
        @select:dim_prop:user_def:implied:WIP:wip.prj_entity_id:prj_entity_id@,
        @select:dim_prop:user_def:implied:WIP:wip.prj_entity_name:prj_entity_name@,
        @select:dim_prop:user_def:implied:WIP:wip.res_location_id:res_location_id@,
        @select:dim_prop:user_def:implied:WIP:wip.res_location_name:res_location_name@,
        @select:dim_prop:user_def:implied:WIP:wip.res_dept_id:res_dept_id@,
        @select:dim_prop:user_def:implied:WIP:wip.res_dept_name:res_dept_name@,
        @select:dim_prop:user_def:implied:WIP:wip.prj_location_id:prj_location_id@,
        @select:dim_prop:user_def:implied:WIP:wip.prj_location_name:prj_location_name@,
        @select:dim_prop:user_def:implied:WIP:wip.prj_dept_id:prj_dept_id@,
        @select:dim_prop:user_def:implied:WIP:wip.prj_dept_name:prj_dept_name@,
        @select:dim_prop:user_def:implied:WIP:wip.wipclass_id:wipclass_id@,
        @select:dim_prop:user_def:implied:WIP:wip.wipclass_name:wipclass_name@,
        @select:dim_prop:user_def:implied:WIP:wip.resource_class_id:resource_class_id@,
        @select:dim_prop:user_def:implied:WIP:wip.resource_class_name:resource_class_name@,
        @select:dim_prop:user_def:implied:WIP:wip.prjclass_id:prjclass_id@,
        @select:dim_prop:user_def:implied:WIP:wip.prjclass_name:prjclass_name@,
        @select:dim_prop:user_def:implied:WIP:wip.transtype_id:transtype_id@,
        @select:dim_prop:user_def:implied:WIP:transtype.name:transtype_name@,
        @select:dim_prop:user_def:implied:WIP:wip.costtype_id:costtype_id@,
        @select:dim_prop:user_def:implied:WIP:costtype.name:costtype_name@,
        @select:dim_prop:user_def:implied:WIP:wip.chargeable:chargeable@,
        @select:metric:user_def:implied:wip.home_totalcost:home_totalcost:AGG@,
        @select:metric:user_def:implied:wip.home_amt:home_amt:AGG@,
        @select:metric:user_def:implied:wip.home_curr:home_curr:AGG@,
        @select:metric:user_def:implied:wip.home_rate:home_rate:AGG@,
        @select:metric:user_def:implied:wip.natural_totalcost:natural_totalcost:AGG@,
        @select:metric:user_def:implied:wip.natural_amt:natural_amt:AGG@,
        @select:metric:user_def:implied:wip.natural_curr:natural_curr:AGG@,
        @select:metric:user_def:implied:wip.natural_rate:natural_rate:AGG@,
        @select:metric:user_def:implied:wip.rpt_totalcost:rpt_totalcost:AGG@,
        @select:metric:user_def:implied:wip.rpt_amt:rpt_amt:AGG@,
        @select:metric:user_def:implied:wip.rpt_curr:rpt_curr:AGG@,
        @select:metric:user_def:implied:wip.rpt_rate:rpt_rate:AGG@,
        @select:metric:user_def:implied:wip.bill_totalcost:bill_totalcost:AGG@,
        @select:metric:user_def:implied:wip.bill_amt:bill_amt:AGG@,
        @select:metric:user_def:implied:wip.bill_curr:bill_curr:AGG@,
        @select:metric:user_def:implied:wip.bill_rate:bill_rate:AGG@
FROM   (SELECT w.transno AS wip_transno, 
               w.transdate AS wip_transdate,
               w.investment_id AS investment_id,
               w.task_id AS task_int_id,
               cc.prid AS chargecode_int_id,
               cc.prname AS chargecode_name,
               w.resource_code AS resource_code,
               tc.id AS transclass_int_id,
               tc.description AS transclass_name,
               tyc.prid AS inputtype_int_id,
               w.input_type AS inputtype_name,
               w.user_lov1 AS user_value1_id,
               w.user_lov2 AS user_value2_id,
               w.notes AS notes,
               w.quantity AS quantity,
               prj_ent.entity AS prj_entity_id,
               prj_ent.shortdesc AS prj_entity_name,
               res_loc.locationid AS res_location_id,
               res_loc.shortdesc AS res_location_name,
               res_dept.departcode AS res_dept_id,
               res_dept.shortdesc AS res_dept_name,
               prj_loc.locationid AS prj_location_id,
               prj_loc.shortdesc AS prj_location_name,
               prj_dept.departcode AS prj_dept_id,
               prj_dept.shortdesc AS prj_dept_name,
               wipclass.wipclass AS wipclass_id,
               wipclass.shortdesc AS wipclass_name,
               resclass.resource_class AS resource_class_id,
               resclass.description AS resource_class_name,
               prjclass.PROJCLASS AS prjclass_id,
               prjclass.shortdesc AS prjclass_name,
               w.transtype AS transtype_id,
               w.cost_type AS costtype_id,
               w.chargeable AS chargeable,
               SUM(CASE WHEN wv.currency_type = 'HOME' THEN wv.totalcost END) AS home_totalcost,
               SUM(CASE WHEN wv.currency_type = 'HOME' THEN wv.totalamount END) AS home_amt,
               MAX(CASE WHEN wv.currency_type = 'HOME'THEN wv.currency_code END) AS home_curr,
               SUM(CASE WHEN wv.currency_type = 'HOME' THEN wv.rate_exchange_rate END) AS home_rate,
               SUM(CASE WHEN wv.currency_type = 'NATURAL' THEN wv.totalcost END) AS natural_totalcost,
               SUM(CASE WHEN wv.currency_type = 'NATURAL' THEN wv.totalamount END) AS natural_amt,
               MAX(CASE WHEN wv.currency_type = 'NATURAL'THEN wv.currency_code END) AS natural_curr,
               SUM(CASE WHEN wv.currency_type = 'NATURAL' THEN wv.rate_exchange_rate END) AS natural_rate,
               SUM(CASE WHEN wv.currency_type = 'REPORTING' THEN wv.totalcost END) AS rpt_totalcost,
               SUM(CASE WHEN wv.currency_type = 'REPORTING' THEN wv.totalamount END) AS rpt_amt,
               MAX(CASE WHEN wv.currency_type = 'REPORTING'THEN wv.currency_code END) AS rpt_curr,
               SUM(CASE WHEN wv.currency_type = 'REPORTING' THEN wv.rate_exchange_rate END) AS rpt_rate,
               SUM(CASE WHEN wv.currency_type = 'BILLING' THEN wv.totalcost END) AS bill_totalcost,
               SUM(CASE WHEN wv.currency_type = 'BILLING' THEN wv.totalamount END) AS bill_amt,
               MAX(CASE WHEN wv.currency_type = 'BILLING'THEN wv.currency_code END) AS bill_curr,
               SUM(CASE WHEN wv.currency_type = 'BILLING' THEN wv.rate_exchange_rate END) AS bill_rate
        FROM   ppa_wip w
               LEFT OUTER JOIN ppa_wip_values wv ON w.transno = wv.transno and wv.currency_type <> 'EURO'
               LEFT OUTER JOIN prchargecode cc ON w.charge_code = cc.prexternalid
               LEFT OUTER JOIN transclass tc ON w.transclass = tc.transclass
               LEFT OUTER JOIN prtypecode tyc ON w.input_type = tyc.prexternalid
               LEFT OUTER JOIN entity prj_ent ON w.entity = prj_ent.entity
               LEFT OUTER JOIN locations res_loc ON w.emplyhomelocation = res_loc.locationid
               LEFT OUTER JOIN departments res_dept ON w.emplyhomedepart  = res_dept.departcode
               LEFT OUTER JOIN locations prj_loc ON w.project_location = prj_loc.locationid
               LEFT OUTER JOIN departments prj_dept ON w.project_department = prj_dept.departcode
               LEFT OUTER JOIN wipclass wipclass ON w.wipclass = wipclass.wipclass
               LEFT OUTER JOIN pac_fos_resource_class resclass ON w.resource_class = resclass.resource_class
               LEFT OUTER JOIN projclass prjclass ON w.projectclass = prjclass.projclass
               LEFT OUTER JOIN ppa_wipapinfo info ON info.id = w.ppa_wipapinfo_id AND @WHERE:PARAM:USER_DEF:STRING:info.voucherno:entryno_filter@
        WHERE  w.status = 0
        AND    (@WHERE:PARAM:XML:INTEGER:/data/id/@value@ IS NULL OR w.investment_id = @WHERE:PARAM:XML:INTEGER:/data/id/@value@)
        AND    @WHERE:SECURITY:INV:w.investment_id@
        GROUP  BY  w.transno, w.transdate, w.investment_id, w.task_id, cc.prid, cc.prname, w.resource_code, tc.id, tc.description, tyc.prid, w.input_type,
               w.user_lov1,w.user_lov2, w.notes,w.quantity, prj_ent.entity, prj_ent.shortdesc, res_loc.locationid, res_loc.shortdesc,res_dept.departcode,
               res_dept.shortdesc, prj_loc.locationid, prj_loc.shortdesc, prj_dept.departcode, prj_dept.shortdesc, wipclass.wipclass, wipclass.shortdesc,
               resclass.resource_class, resclass.description, prjclass.projclass, prjclass.shortdesc, w.transtype, w.cost_type, w.chargeable) wip
        LEFT   OUTER JOIN inv_investments inv ON inv.id = wip.investment_id
        LEFT   OUTER JOIN prtask t ON wip.task_int_id = t.prid
        LEFT   OUTER JOIN srm_resources r ON wip.resource_code = r.unique_name
        LEFT   OUTER JOIN cmn_lookups_v tname ON  tname.lookup_type = 'COP_PORTLET_QUERY_TRANSLATIONS'
                                              AND tname.language_code = @WHERE:PARAM:LANGUAGE@
                                              AND tname.lookup_code = 'Effort'
        LEFT   OUTER JOIN cmn_lookups_v userlov1 ON wip.user_value1_id = userlov1.lookup_code 
                                                 AND userlov1.lookup_type = 'PRTIMEENTRY_USER_LOV1'
                                                 AND userlov1.language_code = @WHERE:PARAM:LANGUAGE@
        LEFT   OUTER JOIN cmn_lookups_v userlov2 ON wip.user_value2_id = userlov2.lookup_code
                                                 AND userlov2.lookup_type = 'PRTIMEENTRY_USER_LOV2'
                                                 AND userlov2.language_code =  @WHERE:PARAM:LANGUAGE@
        LEFT   OUTER JOIN cmn_lookups_v transtype ON wip.transtype_id = transtype.lookup_code
                                                  AND transtype.lookup_type = 'RPT_TRANSACTION_TYPE'
                                                  AND transtype.language_code = @WHERE:PARAM:LANGUAGE@
        LEFT   OUTER JOIN cmn_lookups_v costtype ON wip.costtype_id = costtype.lookup_code
                                                 AND costtype.lookup_type = 'LOOKUP_FIN_COSTTYPECODE'
                                                 AND costtype.language_code = @WHERE:PARAM:LANGUAGE@
WHERE   @FILTER@
AND     @HAVING_FILTER@]]></nsql>
