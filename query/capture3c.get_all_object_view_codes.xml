<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <queries update="true">
          <query code="capture3c.get_all_object_view_codes" isUserPortletAvailable="1" source="customer">
            
            
            
            
            <nls languageCode="en" name="List all object view types"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DIM:USER_DEF:IMPLIED:VIEWS:O.CODE:CODE@,
@SELECT:DIM_PROP:USER_DEF:IMPLIED:VIEWS:O.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
@SELECT:DIM_PROP:USER_DEF:IMPLIED:VIEWS:CSU.USER_NAME:LAST_UPDATED_BY@
FROM odf_objects o
LEFT JOIN cmn_sec_users csu ON csu.id = o.last_updated_by
WHERE EXISTS (SELECT 1 FROM odf_views ov WHERE ov.object_code = o.code)
	AND @FILTER@]]></nsql>
            <attribute id="code" required="false"/>
            <attribute id="last_updated_date" required="false"/>
            <attribute id="last_updated_by" required="false"/>
          </query>
        </queries>
      </contentPack>
    </NikuDataBus>