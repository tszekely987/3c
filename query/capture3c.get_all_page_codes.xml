<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.1.1485"/>
      <contentPack update="true">
        <queries update="true">
          <query code="capture3c.get_all_page_codes" isUserPortletAvailable="1" source="customer">
            
            
            
            
            <nls languageCode="en" name="capture3c.get_all_page_codes"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DIM:USER_DEF:IMPLIED:PAGES:P.PAGE_CODE:CODE@,
@SELECT:DIM_PROP:USER_DEF:IMPLIED:PAGES:P.LAST_UPDATED_DATE:LAST_UPDATED_DATE@,
@SELECT:DIM_PROP:USER_DEF:IMPLIED:PAGES:CSU.USER_NAME:LAST_UPDATED_BY@
FROM cmn_pages p
LEFT JOIN cmn_sec_users csu ON csu.id = p.last_updated_by
WHERE p.page_code NOT LIKE 'SYSTEM5%'
AND p.principal_type != 'USER'
AND (p.parent_page_id is null or p.parent_page_id = -1)
AND @FILTER@]]></nsql>
            <attribute id="code" required="false"/>
            <attribute id="last_updated_date" required="false"/>
            <attribute id="last_updated_by" required="false"/>
          </query>
        </queries>
      </contentPack>
    </NikuDataBus>