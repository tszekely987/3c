<?xml version="1.0" encoding="UTF-8" standalone="no"?><NikuDataBus xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../xsd/nikuxog_contentPack.xsd">
      <Header action="write" externalSource="NIKU" objectType="contentPack" version="16.1.2.1567"/>
      <contentPack update="true">
        <queries update="true">
          <query code="current_process_ts_requests" isUserPortletAvailable="1" source="niku.com">
            
            
            
            
            <nls languageCode="en" name="Current Process Time Slice Requests"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DIM:USER_DEF:IMPLIED:REQUEST:R.ID:ID@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:R.REQUEST_NAME:REQUEST@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:R.FROM_DATE:FROM_DATE@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:R.NUM_PERIODS:NUM_PERIODS@,
					@SELECT:DIM_PROP:USER_DEF:IMPLIED:REQUEST:(case when r.request_status = '1' then 'UPDATE' when r.request_status = '3' then 'ROLLOVER' when r.request_status = '4' then 'NEW' else 'UNKNOWN' end):REQUEST_STATUS@
					FROM PRJ_BLB_SLICEREQUESTS R WHERE R.REQUEST_STATUS IS NOT NULL
					AND @FILTER@]]></nsql>
            <attribute id="id" required="false">
              
              
              
              
              <nls languageCode="en" name="Request ID"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="request" required="false">
              
              
              
              
              <nls languageCode="en" name="Request Name"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="from_date" required="false">
              
              
              
              
              <nls languageCode="en" name="From Date"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="num_periods" required="false">
              
              
              
              
              <nls languageCode="en" name="Number Periods"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
            <attribute id="request_status" required="false">
              
              
              
              
              <nls languageCode="en" name="Status"/>
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
              
            </attribute>
          </query>
        </queries>
      </contentPack>
    </NikuDataBus>