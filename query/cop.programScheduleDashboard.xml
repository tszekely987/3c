        <queries update="true">
          <query category="PRODUCTIVITY" code="cop.programScheduleDashboard" isUserPortletAvailable="1" source="csk.niku.com">
            
            
            
            
            <nls description="Program Manager Schedule Dashboard" languageCode="en" name="Program Manager Schedule Dashboard"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT DISTINCT
       @select:dim:user_def:implied:project:inv2.id:project_int_id@,
       @select:dim_prop:user_def:implied:project:inv2.name:project_name@,
       @select:dim_prop:user_def:implied:project:@UPPER@(inv2.odf_object_code):investment_type_upper@,
       @select:dim_prop:user_def:implied:project:prj2.is_program:is_program@,
       @select:dim_prop:user_def:implied:project:r.user_id:proj_manager_int_id@,
       @select:dim_prop:user_def:implied:project:r.full_name:project_manager@,
       @select:dim_prop:user_def:implied:project:inv2.status_indicator:status@,
       @select:metric:user_def:implied:sr.cop_overall_status:status_rpt_overall_status@,
       @select:dim_prop:user_def:implied:project:inv2.schedule_start:start_date@,
       @select:dim_prop:user_def:implied:project:det.start_date:base_start_date@,
       @select:dim_prop:user_def:implied:project:@DBUSER@.cop_calc_finish_time_fct(inv2.schedule_finish):finish_date@,
       @select:dim_prop:user_def:implied:project:@DBUSER@.cop_calc_finish_time_fct(det.FINISH_DATE):base_finish_date@,
       @select:metric:user_def:implied:idl.days_late:days_late@,
       @select:metric:user_def:implied:idl.days_late_pct:days_late_pct@,
       @select:metric:user_def:implied:idl.days_late:schedule_sl@,
       @select:metric:user_def:implied:idl.days_late_pct:schedule_pct_sl@
FROM   cop_program_v v
       INNER JOIN  inv_investments inv ON  v.program_id = inv.id
                                       AND inv.is_active <> 0
       INNER JOIN  inv_projects prj ON  inv.id = prj.prid
                                    AND prj.is_program = 1
       INNER JOIN  inv_investments inv2 ON  v.project_id = inv2.id
       INNER JOIN  inv_projects prj2 ON  inv2.id = prj2.prID
                                     AND prj.is_program = 1
       LEFT  OUTER JOIN cop_prj_statusrpt_latest_v lsr ON  lsr.investment_id = inv2.id
                                                       AND lsr.report_order = 1 
       LEFT  OUTER JOIN odf_cop_prj_statusrpt_v2 sr ON  sr.odf_pk = lsr.report_id 
	                                                AND sr.odf_parent_id = lsr.investment_id
       LEFT  OUTER JOIN PRJ_BASELINE_DETAILS det ON  prj2.prid = det.object_id 
                                                 AND det.object_type = 'PROJECT' 
                                                 AND det.is_current = 1
       LEFT   OUTER JOIN SRM_RESOURCES r ON inv2.manager_id = r.user_id
       LEFT   OUTER JOIN cop_inv_days_late_v idl ON idl.investment_id = inv2.ID
WHERE  (prj2.is_template <> 1) 
AND    (inv2.is_active <> 0) 
AND    (inv.id = @WHERE:PARAM:USER_DEF:INTEGER:PROGRAM@) 
AND    (@WHERE:SECURITY:PROJECT:inv2.id@) 
AND    @FILTER@]]></nsql>
