        <queries update="true">
          <query category="PRODUCTIVITY" code="agl.prjFeatureStatus" isUserPortletAvailable="0" source="agl.ca.com">
            
            
            
            
            <nls description="Feature Status" languageCode="en" name="Feature Status"/>
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            
            <nsql dbId="Niku" dbVendor="all"><![CDATA[SELECT @SELECT:DIM:USER_DEF:IMPLIED:FEATURE:t.prid:dim_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:t.prid:task_int_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:t.prprojectid:project_int_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:t.prexternalid:task_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:t.prname:task_name@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:t.prwbssequence:wbs_sequence@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:odf_t.agl_tsk_pln_start:pln_start@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:odf_t.agl_tsk_pln_finish:pln_finish@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:odf_t.agl_tsk_act_start:act_start@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:odf_t.agl_tsk_act_finish:act_finish@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:odf_t.agl_tsk_id:feature_id@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:odf_t.agl_tsk_state:agl_state@,	   
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:odf_t.agl_tsk_prelim_est:prelim_est@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:odf_t.agl_tsk_release:release@,
       @SELECT:DIM_PROP:USER_DEF:IMPLIED:FEATURE:(SELECT LISTAGG(m.milestone_name, '; ' ) WITHIN GROUP (ORDER BY m.milestone_name)
                                                  FROM   odf_multi_valued_lookups mvl
                                                         INNER JOIN int_agl_milestones m ON m.milestone_id = mvl.value 
                                                  WHERE  mvl.attribute = 'agl_tsk_milestones'  
                                                  AND    mvl.pk_id = t.prid):milestones@,	   
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(odf_t.agl_tsk_est_stypts,0):est_stypts@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(odf_t.agl_tsk_est_usrsty,0):est_usrsty@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(odf_t.agl_tsk_acp_stypts,0):acp_stypts@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(odf_t.agl_tsk_acp_usrsty,0):acp_usrsty@,	   
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(odf_t.agl_tsk_pctdon_stc,0) * 100:pctdon_stc@,
       @SELECT:METRIC:USER_DEF:IMPLIED:@NVL@(odf_t.agl_tsk_pctdon_plest,0) * 100:pctdon_plest@,	   
       @SELECT:METRIC:USER_DEF:IMPLIED:odf_t.agl_tsk_risk_score:risk_score@,
       @SELECT:METRIC:USER_DEF:IMPLIED:odf_t.agl_tsk_value_score:value_score@,
       @SELECT:METRIC:USER_DEF:IMPLIED:odf_t.agl_tsk_wsjf_score:wsjf_score@,
       @SELECT:METRIC:USER_DEF:IMPLIED:10:no_of_stages@,
       @SELECT:METRIC:USER_DEF:IMPLIED:ROUND((@NVL@(odf_t.agl_tsk_pctdon_plest,0) * 100 / 10),0):current_stage@,
       @SELECT:METRIC:USER_DEF:IMPLIED:CASE WHEN @NVL@(odf_t.agl_tsk_pln_start,t.prstart) >  @SYSDATE@ THEN 100
                                            WHEN @NVL@(odf_t.agl_tsk_pln_start,t.prstart) <= @SYSDATE@ THEN ROUND((@NVL@(odf_t.agl_tsk_pctdon_plest,0) * 100),0)
                                       ELSE 0 END:current_stage_sl@
FROM  prtask t  
      INNER JOIN odf_ca_task odf_t ON odf_t.id = t.prid
WHERE odf_t.agl_tsk_id IS NOT NULL
AND   odf_t.agl_tsk_story_id IS NULL
AND   (odf_t.agl_tsk_ordinal IS NULL OR odf_t.agl_tsk_ordinal = 0)
AND   t.prprojectid = @WHERE:PARAM:XML:INTEGER:/data/id/@value@
AND  (@WHERE:PARAM:USER_DEF:STRING:release@ IS NULL OR @WHERE:PARAM:USER_DEF:STRING:release@ = odf_t.agl_tsk_release)
AND  (@WHERE:PARAM:USER_DEF:INTEGER:milestone@ IS NULL 
      OR @WHERE:PARAM:USER_DEF:INTEGER:milestone@ IN (SELECT mvl.value FROM odf_multi_valued_lookups mvl
                                                      WHERE  mvl.pk_id = odf_t.id
                                                      AND    mvl.attribute = 'agl_tsk_milestones'))
AND   @FILTER@]]></nsql>
